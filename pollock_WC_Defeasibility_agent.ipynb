    "# ==========================================================================\n",
    "# OSCAR-Style Summary Output\n",
    "# ==========================================================================\n",
    "# Human-readable output mimicking Pollock's OSCAR system explanations.\n",
    "# ==========================================================================\n",
    "\n",
    "ARGUMENT_STATUS_SYMBOLS = {\n",
    "    ArgumentStatus.UNDEFEATED: \"✓\",\n",
    "    ArgumentStatus.DEFEATED: \"✗\",\n",
    "    ArgumentStatus.UNDECIDED: \"?\",\n",
    "}\n",
    "\n",
    "REASON_KIND_LABELS = {\n",
    "    ReasonKind.PRIMA_FACIE: \"prima facie\",\n",
    "    ReasonKind.CONCLUSIVE: \"CONCLUSIVE\",\n",
    "}\n",
    "\n",
    "\n",
    "def format_reason_for_display(reason: Reason, indent: int = 2) -> str:\n",
    "    \"\"\"Format a single reason for display.\"\"\"\n",
    "    prefix = \" \" * indent\n",
    "    kind_label = REASON_KIND_LABELS.get(reason.kind, str(reason.kind))\n",
    "    \n",
    "    lines = [\n",
    "        f\"{prefix}Reason [{reason.id}]: {reason.name}\",\n",
    "        f\"{prefix}  Kind: {kind_label}\",\n",
    "        f\"{prefix}  Strength: {reason.final_strength():.2f} (base: {reason.base_strength:.2f})\",\n",
    "    ]\n",
    "    \n",
    "    if reason.premises:\n",
    "        lines.append(f\"{prefix}  Premises: {', '.join(reason.premises[:3])}\")\n",
    "    if reason.conclusions:\n",
    "        lines.append(f\"{prefix}  Concludes: {reason.conclusions[0][:60]}\")\n",
    "    if reason.defeated_by:\n",
    "        lines.append(f\"{prefix}  Defeated by: {reason.defeated_by}\")\n",
    "    if reason.undercutters:\n",
    "        lines.append(f\"{prefix}  Undercut by: {reason.undercutters}\")\n",
    "    \n",
    "    return \"\\n\".join(lines)\n",
    "\n",
    "\n",
    "def format_argument_for_display(arg: Argument, indent: int = 0) -> str:\n",
    "    \"\"\"Format a single argument for display.\"\"\"\n",
    "    prefix = \" \" * indent\n",
    "    status_symbol = ARGUMENT_STATUS_SYMBOLS.get(arg.status, \"?\")\n",
    "    \n",
    "    lines = [\n",
    "        f\"{prefix}{status_symbol} Argument [{arg.id}]: {arg.conclusion[:70]}\",\n",
    "        f\"{prefix}  Status: {arg.status.value.upper()} (strength: {arg.computed_strength:.2f})\",\n",
    "    ]\n",
    "    \n",
    "    if arg.reasons:\n",
    "        lines.append(f\"{prefix}  Based on {len(arg.reasons)} reason(s):\")\n",
    "        for reason in arg.reasons:\n",
    "            lines.append(format_reason_for_display(reason, indent + 4))\n",
    "    \n",
    "    if arg.attacks:\n",
    "        lines.append(f\"{prefix}  Attacks: {arg.attacks}\")\n",
    "    if arg.attacked_by:\n",
    "        lines.append(f\"{prefix}  Attacked by: {arg.attacked_by}\")\n",
    "    \n",
    "    return \"\\n\".join(lines)\n",
    "\n",
    "\n",
    "def print_argument_summary(\n",
    "    arguments: Dict[str, Argument],\n",
    "    reasons: Dict[str, Reason] = None,\n",
    "    title: str = \"OSCAR-Style Argument Analysis\"\n",
    ") -> str:\n",
    "    \"\"\"\n",
    "    Print a human-readable summary of the argument graph in OSCAR style.\n",
    "    \n",
    "    Groups arguments by status and shows attack relationships.\n",
    "    \"\"\"\n",
    "    reasons = reasons or REASONS\n",
    "    \n",
    "    output_lines = [\n",
    "        \"=\" * 70,\n",
    "        f\"  {title}\",\n",
    "        \"=\" * 70,\n",
    "        \"\",\n",
    "    ]\n",
    "    \n",
    "    # Group by status\n",
    "    undefeated = [a for a in arguments.values() if a.status == ArgumentStatus.UNDEFEATED]\n",
    "    defeated = [a for a in arguments.values() if a.status == ArgumentStatus.DEFEATED]\n",
    "    undecided = [a for a in arguments.values() if a.status == ArgumentStatus.UNDECIDED]\n",
    "    \n",
    "    # Summary stats\n",
    "    output_lines.extend([\n",
    "        \"SUMMARY:\",\n",
    "        f\"  Total arguments: {len(arguments)}\",\n",
    "        f\"  ✓ Undefeated: {len(undefeated)}\",\n",
    "        f\"  ✗ Defeated: {len(defeated)}\",\n",
    "        f\"  ? Undecided: {len(undecided)}\",\n",
    "        f\"  Total reasons: {len(reasons)}\",\n",
    "        \"\",\n",
    "    ])\n",
    "    \n",
    "    # Undefeated arguments (accepted conclusions)\n",
    "    if undefeated:\n",
    "        output_lines.extend([\n",
    "            \"-\" * 70,\n",
    "            \"ACCEPTED (Undefeated) Arguments:\",\n",
    "            \"-\" * 70,\n",
    "        ])\n",
    "        for arg in sorted(undefeated, key=lambda a: -a.computed_strength):\n",
    "            output_lines.append(format_argument_for_display(arg))\n",
    "            output_lines.append(\"\")\n",
    "    \n",
    "    # Defeated arguments (rejected conclusions)\n",
    "    if defeated:\n",
    "        output_lines.extend([\n",
    "            \"-\" * 70,\n",
    "            \"REJECTED (Defeated) Arguments:\",\n",
    "            \"-\" * 70,\n",
    "        ])\n",
    "        for arg in sorted(defeated, key=lambda a: -a.computed_strength):\n",
    "            output_lines.append(format_argument_for_display(arg))\n",
    "            output_lines.append(\"\")\n",
    "    \n",
    "    # Undecided arguments (dialectical standoff)\n",
    "    if undecided:\n",
    "        output_lines.extend([\n",
    "            \"-\" * 70,\n",
    "            \"UNDECIDED (Dialectical Standoff):\",\n",
    "            \"-\" * 70,\n",
    "        ])\n",
    "        for arg in undecided:\n",
    "            output_lines.append(format_argument_for_display(arg))\n",
    "            output_lines.append(\"\")\n",
    "    \n",
    "    # Attack graph\n",
    "    output_lines.extend([\n",
    "        \"-\" * 70,\n",
    "        \"ATTACK RELATIONSHIPS:\",\n",
    "        \"-\" * 70,\n",
    "    ])\n",
    "    for arg in arguments.values():\n",
    "        if arg.attacks:\n",
    "            for target_id in arg.attacks:\n",
    "                target = arguments.get(target_id)\n",
    "                if target:\n",
    "                    attacker_symbol = ARGUMENT_STATUS_SYMBOLS.get(arg.status, \"?\")\n",
    "                    target_symbol = ARGUMENT_STATUS_SYMBOLS.get(target.status, \"?\")\n",
    "                    output_lines.append(\n",
    "                        f\"  {attacker_symbol} {arg.id} --defeats--> {target_symbol} {target_id}\"\n",
    "                    )\n",
    "    \n",
    "    output_lines.append(\"=\" * 70)\n",
    "    \n",
    "    result = \"\\n\".join(output_lines)\n",
    "    print(result)\n",
    "    return result\n",
    "\n",
    "\n",
    "def print_defeat_explanation(\n",
    "    arg: Argument,\n",
    "    arguments: Dict[str, Argument]\n",
    ") -> str:\n",
    "    \"\"\"\n",
    "    Print a detailed explanation of why an argument was defeated/undefeated.\n",
    "    \"\"\"\n",
    "    lines = [\n",
    "        f\"Explanation for {arg.id}:\",\n",
    "        f\"  Conclusion: {arg.conclusion}\",\n",
    "        f\"  Status: {arg.status.value}\",\n",
    "        f\"  Strength: {arg.computed_strength:.2f}\",\n",
    "        \"\",\n",
    "    ]\n",
    "    \n",
    "    if not arg.attacked_by:\n",
    "        lines.append(\"  → No attackers, therefore UNDEFEATED\")\n",
    "    else:\n",
    "        lines.append(f\"  Attacked by: {len(arg.attacked_by)} argument(s)\")\n",
    "        for attacker_id in arg.attacked_by:\n",
    "            attacker = arguments.get(attacker_id)\n",
    "            if attacker:\n",
    "                status = attacker.status.value\n",
    "                strength = attacker.computed_strength\n",
    "                lines.append(f\"    - {attacker_id}: {status} (strength {strength:.2f})\")\n",
    "        \n",
    "        if arg.status == ArgumentStatus.DEFEATED:\n",
    "            lines.append(\"  → Has undefeated stronger attacker, therefore DEFEATED\")\n",
    "        elif arg.status == ArgumentStatus.UNDECIDED:\n",
    "            lines.append(\"  → In dialectical standoff with attacker(s), therefore UNDECIDED\")\n",
    "        else:\n",
    "            lines.append(\"  → All attackers are defeated, therefore UNDEFEATED\")\n",
    "    \n",
    "    result = \"\\n\".join(lines)\n",
    "    print(result)\n",
    "    return result\n",
    "\n",
    "\n",
    "print(\"✓ OSCAR-style summary output functions defined\")\n",
    "print(\"  - format_reason_for_display(reason)\")\n",
    "print(\"  - format_argument_for_display(argument)\")\n",
    "print(\"  - print_argument_summary(arguments, reasons)\")\n",
    "print(\"  - print_defeat_explanation(arg, arguments)\")"
   ]

    "# ---------- Workers' Compensation Plan Initialization ----------\n",
    "\n",
    "def init_workers_comp_claim_plan(\n",
    "    claim_description: str,\n",
    "    jurisdiction: str,\n",
    "    severity_level: str,\n",
    "    tool_context: ToolContext,\n",
    ") -> Dict[str, Any]:\n",
    "    \"\"\"Initialize a structured workers' compensation claim evaluation plan.\n",
    "    \n",
    "    Creates a canonical plan for evaluating and resolving a workers' comp claim,\n",
    "    with steps covering intake, eligibility, medical review, compensability,\n",
    "    compliance, fraud screening, benefit calculation, and resolution.\n",
    "    \n",
    "    Args:\n",
    "        claim_description: Brief description of the injury and circumstances.\n",
    "        jurisdiction: State abbreviation (e.g., \"IL\", \"CA\").\n",
    "        severity_level: One of \"minor\", \"moderate\", \"severe\", \"catastrophic\".\n",
    "        tool_context: ADK tool context for state access.\n",
    "    \n",
    "    Returns:\n",
    "        Summary of the initialized plan.\n",
    "    \"\"\"\n",
    "    state = tool_context.state\n",
    "    beliefs: List[Dict[str, Any]] = state.get(\"beliefs\", [])\n",
    "    \n",
    "    # Create goal\n",
    "    goal = Goal(\n",
    "        id=\"wc-goal-1\",\n",
    "        description=f\"Resolve workers' compensation claim fairly, timely, and in compliance with {jurisdiction} regulations\",\n",
    "        priority=1,\n",
    "    )\n",
    "    \n",
    "    # Define the workers' comp claim plan steps\n",
    "    steps: List[WorkersCompPlanStep] = []\n",
    "    \n",
    "    # Step 1: Initial Claim Intake\n",
    "    steps.append(WorkersCompPlanStep(\n",
    "        id=\"wc-step-1\",\n",
    "        description=\"Initial claim intake and incident verification. Confirm employer, employee identity, date/time of injury, location, and on-duty status.\",\n",
    "        preconditions=[],\n",
    "        effects=[\n",
    "            \"claim:intake_completed=true\",\n",
    "            \"claim:incident_verified=true\",\n",
    "        ],\n",
    "        risk_factors=[\"incomplete_information\", \"delayed_reporting\"],\n",
    "        required_documents=[\"First Report of Injury\", \"Employer Incident Report\"],\n",
    "        regulatory_dependencies=[],\n",
    "    ))\n",
    "    \n",
    "    # Step 2: Coverage and Employment Eligibility\n",
    "    steps.append(WorkersCompPlanStep(\n",
    "        id=\"wc-step-2\",\n",
    "        description=\"Verify employment eligibility and policy coverage. Confirm employee status, policy applicability, and employer coverage under workers' compensation.\",\n",
    "        preconditions=[\"claim:intake_completed=true\"],\n",
    "        effects=[\n",
    "            \"employment:eligibility_verified=true\",\n",
    "            \"policy:coverage_confirmed=true\",\n",
    "        ],\n",
    "        risk_factors=[\"contractor_misclassification\", \"policy_lapse\", \"excluded_employment_category\"],\n",
    "        required_documents=[\"Employment Records\", \"Policy Declaration Page\"],\n",
    "        regulatory_dependencies=[\"employment_status_eligible_for_coverage\"],\n",
    "    ))\n",
    "    \n",
    "    # Step 3: Medical Evidence Collection\n",
    "    steps.append(WorkersCompPlanStep(\n",
    "        id=\"wc-step-3\",\n",
    "        description=\"Collect and validate medical evidence. Obtain initial medical report, treating physician information, diagnosis, and preliminary work-relatedness assessment.\",\n",
    "        preconditions=[\"claim:intake_completed=true\"],\n",
    "        effects=[\n",
    "            \"medical:initial_report_received=true\",\n",
    "            \"medical:diagnosis_documented=true\",\n",
    "        ],\n",
    "        risk_factors=[\"delayed_treatment\", \"pre_existing_condition\", \"multiple_treating_physicians\"],\n",
    "        required_documents=[\"Initial Medical Report\", \"Treating Physician Statement\", \"Medical Authorization Form\"],\n",
    "        regulatory_dependencies=[],\n",
    "    ))\n",
    "    \n",
    "    # Step 4: Compensability Analysis\n",
    "    steps.append(WorkersCompPlanStep(\n",
    "        id=\"wc-step-4\",\n",
    "        description=\"Evaluate compensability. Determine if the injury arose out of and in the course of employment per jurisdictional standards.\",\n",
    "        preconditions=[\n",
    "            \"claim:incident_verified=true\",\n",
    "            \"employment:eligibility_verified=true\",\n",
    "            \"medical:initial_report_received=true\",\n",
    "        ],\n",
    "        effects=[\n",
    "            \"analysis:compensability_prelim=evaluated\",\n",
    "        ],\n",
    "        risk_factors=[\"causation_dispute\", \"off_premises_injury\", \"horseplay\", \"intoxication\"],\n",
    "        required_documents=[\"Witness Statements\", \"Supervisor Report\"],\n",
    "        regulatory_dependencies=[\"jurisdiction_compensability_standard_met\"],\n",
    "    ))\n",
    "    \n",
    "    # Step 5: Regulatory Compliance Check\n",
    "    steps.append(WorkersCompPlanStep(\n",
    "        id=\"wc-step-5\",\n",
    "        description=\"Verify regulatory compliance and timelines. Check waiting period satisfaction, notice requirements, and mandatory form filings.\",\n",
    "        preconditions=[\"claim:intake_completed=true\"],\n",
    "        effects=[\n",
    "            \"compliance:timelines_verified=true\",\n",
    "            \"compliance:forms_filed=true\",\n",
    "        ],\n",
    "        risk_factors=[\"late_filing\", \"missed_deadline\", \"incomplete_forms\"],\n",
    "        required_documents=[\"State-Mandated Forms\", \"Filing Receipts\"],\n",
    "        regulatory_dependencies=[\n",
    "            \"state_waiting_period_satisfied\",\n",
    "            \"notice_deadline_met\",\n",
    "            \"mandatory_forms_submitted\",\n",
    "        ],\n",
    "    ))\n",
    "    \n",
    "    # Step 6: Fraud and Red Flag Screening\n",
    "    steps.append(WorkersCompPlanStep(\n",
    "        id=\"wc-step-6\",\n",
    "        description=\"Conduct fraud and red flag screening. Review fraud indicators, prior claim history, and statement consistency.\",\n",
    "        preconditions=[\"claim:intake_completed=true\", \"medical:initial_report_received=true\"],\n",
    "        effects=[\n",
    "            \"risk:fraud_screening_completed=true\",\n",
    "        ],\n",
    "        risk_factors=[\"high_fraud_score\", \"monday_morning_claim\", \"prior_similar_claims\", \"conflicting_statements\"],\n",
    "        required_documents=[\"Prior Claims History\", \"Statement Comparison Analysis\"],\n",
    "        regulatory_dependencies=[],\n",
    "    ))\n",
    "    \n",
    "    # Step 7: Benefit Calculation\n",
    "    steps.append(WorkersCompPlanStep(\n",
    "        id=\"wc-step-7\",\n",
    "        description=\"Calculate provisional benefits. Estimate temporary total disability (TTD), medical coverage amounts, and payment schedule based on jurisdictional rates.\",\n",
    "        preconditions=[\n",
    "            \"analysis:compensability_prelim=evaluated\",\n",
    "            \"compliance:timelines_verified=true\",\n",
    "            \"risk:fraud_screening_completed=true\",\n",
    "        ],\n",
    "        effects=[\n",
    "            \"benefits:ttd_calculated=true\",\n",
    "            \"benefits:medical_reserve_set=true\",\n",
    "        ],\n",
    "        risk_factors=[\"wage_documentation_incomplete\", \"maximum_benefit_exceeded\"],\n",
    "        required_documents=[\"Wage Records\", \"Benefit Calculation Worksheet\"],\n",
    "        regulatory_dependencies=[\"jurisdiction_benefit_rates_applied\"],\n",
    "    ))\n",
    "    \n",
    "    # Step 8: Resolution Decision\n",
    "    steps.append(WorkersCompPlanStep(\n",
    "        id=\"wc-step-8\",\n",
    "        description=\"Determine claim resolution path. Based on all evidence and analysis, recommend approval, denial with stated grounds, or escalation to legal/medical review.\",\n",
    "        preconditions=[\n",
    "            \"analysis:compensability_prelim=evaluated\",\n",
    "            \"benefits:ttd_calculated=true\",\n",
    "            \"risk:fraud_screening_completed=true\",\n",
    "        ],\n",
    "        effects=[\n",
    "            \"resolution:decision_rendered=true\",\n",
    "            \"resolution:justification_documented=true\",\n",
    "        ],\n",
    "        risk_factors=[\"litigation_exposure\", \"appeal_likely\", \"regulatory_scrutiny\"],\n",
    "        required_documents=[\"Decision Summary\", \"Justification Memorandum\"],\n",
    "        regulatory_dependencies=[\"all_mandatory_reviews_completed\"],\n",
    "    ))\n",
    "    \n",
    "    # Store in state\n",
    "    steps_dict = [s.model_dump() for s in steps]\n",
    "    state[\"active_plan\"] = steps_dict\n",
    "    state[\"goals\"] = [goal.model_dump()]\n",
    "    state[\"claim_context\"] = {\n",
    "        \"description\": claim_description,\n",
    "        \"jurisdiction\": jurisdiction,\n",
    "        \"severity_level\": severity_level,\n",
    "    }\n",
    "    \n",
    "    # Add initial beliefs about the claim\n",
    "    initial_beliefs = [\n",
    "        Belief(\n",
    "            id=\"wc-b-1\",\n",
    "            proposition=f\"jurisdiction:state={jurisdiction}\",\n",
    "            support=\"Claim filing location\",\n",
    "            strength=1.0,\n",
    "        ),\n",
    "        Belief(\n",
    "            id=\"wc-b-2\",\n",
    "            proposition=f\"claim:severity_level={severity_level}\",\n",
    "            support=\"Initial severity assessment\",\n",
    "            strength=0.8,\n",
    "        ),\n",
    "    ]\n",
    "    \n",
    "    for b in initial_beliefs:\n",
    "        beliefs.append(b.model_dump())\n",
    "    state[\"beliefs\"] = beliefs\n",
    "    \n",
    "    return {\n",
    "        \"status\": \"initialized\",\n",
    "        \"domain\": \"workers_compensation\",\n",
    "        \"jurisdiction\": jurisdiction,\n",
    "        \"severity_level\": severity_level,\n",
    "        \"goal\": goal.model_dump(),\n",
    "        \"num_steps\": len(steps_dict),\n",
    "        \"step_ids\": [s.id for s in steps],\n",
    "        \"step_summaries\": [{\"id\": s.id, \"description\": s.description[:80] + \"...\"} for s in steps],\n",
    "    }\n",
    "\n",
    "\n",
    "print(\"Workers' compensation plan initialization function defined.\")"
   ]

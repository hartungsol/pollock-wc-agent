    "# ==========================================================================\n",
    "# Helper: Print Workers' Comp Summary\n",
    "# ==========================================================================\n",
    "\n",
    "def print_workers_comp_summary(result: dict):\n",
    "    \"\"\"\n",
    "    Pretty-print a summary of the iterative analysis result.\n",
    "    \n",
    "    Args:\n",
    "        result: Dict returned by run_iterative_wc_analysis\n",
    "    \"\"\"\n",
    "    print(\"\\n\" + \"=\" * 70)\n",
    "    print(\"WORKERS' COMPENSATION CLAIM ANALYSIS SUMMARY\")\n",
    "    print(\"=\" * 70)\n",
    "    \n",
    "    # Iteration summary\n",
    "    print(f\"\\nAnalysis Iterations: {result.get('iterations_completed', 0)}/{result.get('max_iterations', 3)}\")\n",
    "    \n",
    "    # Show iteration history\n",
    "    history = result.get(\"iteration_history\", [])\n",
    "    if history:\n",
    "        print(\"\\nIteration History:\")\n",
    "        for h in history:\n",
    "            beliefs_added = h.get(\"beliefs_added\", 0)\n",
    "            added_str = f\" (+{beliefs_added} beliefs)\" if beliefs_added else \"\"\n",
    "            print(f\"  Iteration {h['iteration']}: \"\n",
    "                  f\"QUESTIONED={h.get('questioned_count', 0)}, \"\n",
    "                  f\"DEFEATED={h.get('defeated_count', 0)}\"\n",
    "                  f\"{added_str}\")\n",
    "    \n",
    "    # Final counts\n",
    "    print(f\"\\nFinal Status:\")\n",
    "    print(f\"  Total beliefs: {result.get('final_beliefs_count', 0)}\")\n",
    "    print(f\"  QUESTIONED steps: {result.get('final_questioned_count', 0)}\")\n",
    "    print(f\"  DEFEATED steps: {result.get('final_defeated_count', 0)}\")\n",
    "    \n",
    "    # Show plan steps\n",
    "    state = result.get(\"state\", {})\n",
    "    active_plan = state.get(\"active_plan\", [])\n",
    "    \n",
    "    if active_plan:\n",
    "        print(\"\\nPlan Steps:\")\n",
    "        for step in active_plan:\n",
    "            status = step.get(\"status\", \"UNKNOWN\")\n",
    "            symbol = STATUS_SYMBOLS.get(status, \"?\")\n",
    "            defeater_count = len(step.get(\"defeaters\", []))\n",
    "            \n",
    "            if defeater_count > 0:\n",
    "                print(f\"  {symbol} {step.get('id')}: {status} ({defeater_count} defeaters)\")\n",
    "            else:\n",
    "                print(f\"  {symbol} {step.get('id')}: {status}\")\n",
    "    \n",
    "    # Recommendation\n",
    "    print(\"\\n\" + \"-\" * 70)\n",
    "    questioned = result.get(\"final_questioned_count\", 0)\n",
    "    defeated = result.get(\"final_defeated_count\", 0)\n",
    "    \n",
    "    if defeated > 0:\n",
    "        print(\"RECOMMENDATION: ⚠️  ESCALATE FOR REVIEW\")\n",
    "        print(f\"  Reason: {defeated} step(s) have fatal defeaters (severity >= 0.8)\")\n",
    "    elif questioned > 0:\n",
    "        print(\"RECOMMENDATION: ⚠️  INVESTIGATE FURTHER\")\n",
    "        print(f\"  Reason: {questioned} step(s) still have unresolved questions\")\n",
    "    else:\n",
    "        print(\"RECOMMENDATION: ✓  PROCEED TO APPROVAL REVIEW\")\n",
    "        print(\"  Reason: All steps supported, no blocking defeaters\")\n",
    "    \n",
    "    print(\"-\" * 70)\n",
    "\n",
    "\n",
    "print(\"Summary helper loaded:\")\n",
    "print(\"  - print_workers_comp_summary(result)\")"
   ]

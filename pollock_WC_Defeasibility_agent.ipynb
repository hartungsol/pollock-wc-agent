    "# ==========================================================================\n",
    "# Forward & Backward Reasoning Integration Helpers\n",
    "# ==========================================================================\n",
    "# These helpers wire the forward and backward reasoners into the main\n",
    "# workers' compensation analysis pipeline.\n",
    "\n",
    "def run_forward_reasoning_step(state: dict) -> int:\n",
    "    \"\"\"\n",
    "    Apply all registered forward reasons to the current beliefs in state.\n",
    "    \n",
    "    This function should be called BEFORE rule evaluation to ensure\n",
    "    derived beliefs (e.g., compliance:timely_notice=true) are available.\n",
    "    \n",
    "    Args:\n",
    "        state: The state dict containing 'beliefs' list\n",
    "        \n",
    "    Returns:\n",
    "        Number of new beliefs added\n",
    "    \"\"\"\n",
    "    result = apply_forwards_reasons(state)\n",
    "    return result.get(\"num_new_beliefs\", 0)\n",
    "\n",
    "\n",
    "def run_backward_analysis_for_key_goals(state: dict) -> list:\n",
    "    \"\"\"\n",
    "    Run backward reasoning for key high-level goals and store results in state.\n",
    "    \n",
    "    This provides diagnostic information about what is needed to reach\n",
    "    key decisions (approve, deny, escalate, confirm compensability).\n",
    "    \n",
    "    Args:\n",
    "        state: The state dict containing 'beliefs' list\n",
    "        \n",
    "    Returns:\n",
    "        List of analysis results for each goal\n",
    "    \"\"\"\n",
    "    # Key high-level goals to analyze\n",
    "    key_goals = [\n",
    "        \"decision:approve_claim\",\n",
    "        \"decision:deny_claim\", \n",
    "        \"decision:escalate_claim\",\n",
    "        \"analysis:compensability_confirmed=true\",\n",
    "    ]\n",
    "    \n",
    "    results = []\n",
    "    \n",
    "    for goal in key_goals:\n",
    "        # Use the existing check_subgoals_satisfied function\n",
    "        analysis = check_subgoals_satisfied(goal, state)\n",
    "        results.append({\n",
    "            \"goal\": goal,\n",
    "            \"analysis\": analysis,\n",
    "        })\n",
    "    \n",
    "    # Store in state for later inspection\n",
    "    state[\"backward_analysis\"] = results\n",
    "    return results\n",
    "\n",
    "\n",
    "def display_backward_analysis(state: dict) -> None:\n",
    "    \"\"\"\n",
    "    Display a formatted summary of backward analysis results.\n",
    "    \n",
    "    Shows which subgoals are satisfied vs missing for key decisions.\n",
    "    \n",
    "    Args:\n",
    "        state: The state dict containing 'backward_analysis'\n",
    "    \"\"\"\n",
    "    analysis_list = state.get(\"backward_analysis\", [])\n",
    "    \n",
    "    if not analysis_list:\n",
    "        print(\"\\n[BACKWARD ANALYSIS] No analysis available.\")\n",
    "        return\n",
    "    \n",
    "    print(\"\\n\" + \"=\" * 70)\n",
    "    print(\"BACKWARD ANALYSIS: Goal Decomposition Summary\")\n",
    "    print(\"=\" * 70)\n",
    "    \n",
    "    for entry in analysis_list:\n",
    "        goal = entry.get(\"goal\", \"Unknown\")\n",
    "        analysis = entry.get(\"analysis\", {})\n",
    "        status = analysis.get(\"status\", \"unknown\")\n",
    "        \n",
    "        # Status symbol\n",
    "        if status == \"supported\":\n",
    "            symbol = \"✓\"\n",
    "        elif status == \"pending\":\n",
    "            symbol = \"○\"\n",
    "        else:\n",
    "            symbol = \"?\"\n",
    "        \n",
    "        print(f\"\\n{symbol} Goal: {goal}\")\n",
    "        print(f\"  Status: {status.upper()}\")\n",
    "        \n",
    "        # Show per-reason breakdown\n",
    "        for reason_result in analysis.get(\"analyses\", []):\n",
    "            reason_name = reason_result.get(\"reason\", \"unknown\")\n",
    "            completion = reason_result.get(\"completion_pct\", 0)\n",
    "            satisfied = reason_result.get(\"satisfied\", [])\n",
    "            missing = reason_result.get(\"missing\", [])\n",
    "            \n",
    "            print(f\"\\n  Reason: {reason_name} ({completion:.0f}% complete)\")\n",
    "            \n",
    "            if satisfied:\n",
    "                print(f\"    ✓ Satisfied ({len(satisfied)}):\")\n",
    "                for sg in satisfied[:3]:  # Show first 3\n",
    "                    print(f\"      - {sg}\")\n",
    "                if len(satisfied) > 3:\n",
    "                    print(f\"      ... and {len(satisfied) - 3} more\")\n",
    "            \n",
    "            if missing:\n",
    "                print(f\"    ✗ Missing ({len(missing)}):\")\n",
    "                for sg in missing[:5]:  # Show first 5\n",
    "                    print(f\"      - {sg}\")\n",
    "                if len(missing) > 5:\n",
    "                    print(f\"      ... and {len(missing) - 5} more\")\n",
    "        \n",
    "        # Show recommendation from analysis\n",
    "        rec = analysis.get(\"recommendation\", \"\")\n",
    "        if rec:\n",
    "            print(f\"\\n  Recommendation: {rec[:80]}...\")\n",
    "    \n",
    "    print(\"\\n\" + \"=\" * 70)\n",
    "\n",
    "\n",
    "def get_backward_analysis_summary(state: dict) -> dict:\n",
    "    \"\"\"\n",
    "    Get a compact summary of backward analysis for use in investigation prompts.\n",
    "    \n",
    "    Args:\n",
    "        state: The state dict\n",
    "        \n",
    "    Returns:\n",
    "        Summary dict with goals and their missing subgoals\n",
    "    \"\"\"\n",
    "    analysis_list = state.get(\"backward_analysis\", [])\n",
    "    \n",
    "    summary = {\n",
    "        \"goals_analyzed\": len(analysis_list),\n",
    "        \"goals_supported\": 0,\n",
    "        \"goals_pending\": 0,\n",
    "        \"missing_subgoals_by_goal\": {},\n",
    "    }\n",
    "    \n",
    "    for entry in analysis_list:\n",
    "        goal = entry.get(\"goal\", \"Unknown\")\n",
    "        analysis = entry.get(\"analysis\", {})\n",
    "        status = analysis.get(\"status\", \"unknown\")\n",
    "        \n",
    "        if status == \"supported\":\n",
    "            summary[\"goals_supported\"] += 1\n",
    "        else:\n",
    "            summary[\"goals_pending\"] += 1\n",
    "        \n",
    "        # Collect missing subgoals\n",
    "        all_missing = []\n",
    "        for reason_result in analysis.get(\"analyses\", []):\n",
    "            all_missing.extend(reason_result.get(\"missing\", []))\n",
    "        \n",
    "        if all_missing:\n",
    "            summary[\"missing_subgoals_by_goal\"][goal] = all_missing\n",
    "    \n",
    "    return summary\n",
    "\n",
    "\n",
    "print(\"Forward & Backward Reasoning Integration loaded:\")\n",
    "print(\"  - run_forward_reasoning_step(state) -> int: Apply forward reasoning\")\n",
    "print(\"  - run_backward_analysis_for_key_goals(state) -> list: Analyze key goals\")\n",
    "print(\"  - display_backward_analysis(state): Print formatted summary\")\n",
    "print(\"  - get_backward_analysis_summary(state) -> dict: Compact summary\")"
   ]
  },

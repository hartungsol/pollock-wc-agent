   "source": [
    "# ==========================================================================\n",
    "# Generic Reasoning Tools for Pollock-style Planning\n",
    "# ==========================================================================\n",
    "# These tools provide the core belief and plan management functionality\n",
    "# used by the workers' compensation planning system.\n",
    "\n",
    "from __future__ import annotations\n",
    "from typing import Any, List, Dict, Optional\n",
    "from google.adk.tools import ToolContext\n",
    "\n",
    "\n",
    "# ---------- 1. Read Project State ----------\n",
    "\n",
    "def get_project_state(tool_context: ToolContext) -> Dict[str, Any]:\n",
    "    \"\"\"Return current beliefs, goals, and active plan.\n",
    "\n",
    "    Use this whenever you need to reason about current evidence and plan status.\n",
    "    \"\"\"\n",
    "    state = tool_context.state\n",
    "    return {\n",
    "        \"beliefs\": state.get(\"beliefs\", []),\n",
    "        \"goals\": state.get(\"goals\", []),\n",
    "        \"active_plan\": state.get(\"active_plan\", []),\n",
    "    }\n",
    "\n",
    "\n",
    "# ---------- 2. Update Active Plan ----------\n",
    "\n",
    "def update_active_plan(\n",
    "    plan_steps: List[Dict[str, Any]],\n",
    "    tool_context: ToolContext,\n",
    ") -> Dict[str, Any]:\n",
    "    \"\"\"Replace the current active plan with the provided list of steps.\n",
    "\n",
    "    The agent can use this to revise or extend the current plan.\n",
    "    Each step must have at least a 'description' field.\n",
    "    \"\"\"\n",
    "    state = tool_context.state\n",
    "    normalized: List[Dict[str, Any]] = []\n",
    "    errors: List[str] = []\n",
    "\n",
    "    for i, step in enumerate(plan_steps):\n",
    "        if not isinstance(step, dict):\n",
    "            errors.append(f\"Step {i+1}: Expected dict, got {type(step).__name__}\")\n",
    "            continue\n",
    "        \n",
    "        description = step.get(\"description\")\n",
    "        if not description:\n",
    "            description = step.get(\"desc\") or step.get(\"name\") or step.get(\"action\")\n",
    "        \n",
    "        if not description:\n",
    "            errors.append(f\"Step {i+1}: Missing 'description' field. Got keys: {list(step.keys())}\")\n",
    "            continue\n",
    "        \n",
    "        try:\n",
    "            s = PlanStep(\n",
    "                id=step.get(\"id\") or f\"step-{i+1}\",\n",
    "                description=str(description),\n",
    "                preconditions=step.get(\"preconditions\", []),\n",
    "                effects=step.get(\"effects\", []),\n",
    "                risk_factors=step.get(\"risk_factors\", []),\n",
    "                defeaters=step.get(\"defeaters\", []),\n",
    "            )\n",
    "            normalized.append(s.model_dump())\n",
    "        except Exception as e:\n",
    "            errors.append(f\"Step {i+1}: Validation error - {e}\")\n",
    "\n",
    "    if errors:\n",
    "        return {\n",
    "            \"status\": \"error\",\n",
    "            \"message\": \"Some steps could not be parsed\",\n",
    "            \"errors\": errors,\n",
    "            \"valid_steps_count\": len(normalized),\n",
    "        }\n",
    "\n",
    "    if not normalized:\n",
    "        return {\n",
    "            \"status\": \"error\",\n",
    "            \"message\": \"No valid steps provided. Each step needs at least a 'description' field.\",\n",
    "        }\n",
    "\n",
    "    state[\"active_plan\"] = normalized\n",
    "    return {\n",
    "        \"status\": \"ok\",\n",
    "        \"num_steps\": len(normalized),\n",
    "        \"step_ids\": [s[\"id\"] for s in normalized],\n",
    "    }\n",
    "\n",
    "\n",
    "# ---------- 3. Record Defeater for a Step (Pollock-style) ----------\n",
    "\n",
    "def record_step_defeater(\n",
    "    step_id: str,\n",
    "    reason: str,\n",
    "    tool_context: ToolContext,\n",
    ") -> Dict[str, Any]:\n",
    "    \"\"\"Record a defeater for a particular plan step.\n",
    "\n",
    "    A defeater is a reason why a step's conclusion may not hold.\n",
    "    Examples:\n",
    "      step_id=\"wc-step-4\", reason=\"Contractor status excludes coverage.\"\n",
    "    \"\"\"\n",
    "    state = tool_context.state\n",
    "    plan = state.get(\"active_plan\", [])\n",
    "\n",
    "    for step in plan:\n",
    "        if step.get(\"id\") == step_id:\n",
    "            defeaters = step.get(\"defeaters\", [])\n",
    "            defeaters.append(reason)\n",
    "            step[\"defeaters\"] = defeaters\n",
    "            state[\"active_plan\"] = plan\n",
    "            return {\n",
    "                \"status\": \"defeated\",\n",
    "                \"step_id\": step_id,\n",
    "                \"defeaters\": defeaters,\n",
    "            }\n",
    "\n",
    "    return {\"status\": \"not_found\", \"step_id\": step_id}\n",
    "\n",
    "\n",
    "# ---------- 4. Add a New Belief ----------\n",
    "\n",
    "def add_belief(\n",
    "    proposition: str,\n",
    "    support: str,\n",
    "    strength: float,\n",
    "    tool_context: ToolContext,\n",
    ") -> Dict[str, Any]:\n",
    "    \"\"\"Add a new belief to the agent's belief state.\n",
    "    \n",
    "    Args:\n",
    "        proposition: The belief content, e.g. \"employment:status=full_time\"\n",
    "        support: Justification for the belief (source of evidence)\n",
    "        strength: Confidence in the belief (0.0 to 1.0)\n",
    "    \n",
    "    Returns:\n",
    "        The created belief.\n",
    "    \"\"\"\n",
    "    state = tool_context.state\n",
    "    beliefs: List[Dict[str, Any]] = state.get(\"beliefs\", [])\n",
    "    \n",
    "    b = Belief(\n",
    "        id=f\"b-{len(beliefs) + 1}\",\n",
    "        proposition=proposition,\n",
    "        support=support,\n",
    "        strength=min(max(strength, 0.0), 1.0),\n",
    "    )\n",
    "    beliefs.append(b.model_dump())\n",
    "    state[\"beliefs\"] = beliefs\n",
    "    \n",
    "    return {\"status\": \"added\", \"belief\": b.model_dump()}\n",
    "\n",
    "\n",
    "# ---------- 5. Remove a Belief (Retraction) ----------\n",
    "\n",
    "def remove_belief(\n",
    "    belief_id: str,\n",
    "    tool_context: ToolContext,\n",
    ") -> Dict[str, Any]:\n",
    "    \"\"\"Remove a belief from the agent's belief state.\n",
    "    \n",
    "    This implements belief retraction - when evidence is withdrawn\n",
    "    or contradicted.\n",
    "    \n",
    "    Args:\n",
    "        belief_id: The ID of the belief to remove.\n",
    "    \n",
    "    Returns:\n",
    "        Status of the removal.\n",
    "    \"\"\"\n",
    "    state = tool_context.state\n",
    "    beliefs: List[Dict[str, Any]] = state.get(\"beliefs\", [])\n",
    "    \n",
    "    for i, b in enumerate(beliefs):\n",
    "        if b.get(\"id\") == belief_id:\n",
    "            removed = beliefs.pop(i)\n",
    "            state[\"beliefs\"] = beliefs\n",
    "            return {\"status\": \"removed\", \"belief\": removed}\n",
    "    \n",
    "    return {\"status\": \"not_found\", \"belief_id\": belief_id}\n",
    "\n",
    "\n",
    "# ---------- 6. Check Step Readiness ----------\n",
    "\n",
    "def check_step_readiness(\n",
    "    step_id: str,\n",
    "    tool_context: ToolContext,\n",
    ") -> Dict[str, Any]:\n",
    "    \"\"\"Check if a plan step is ready to execute.\n",
    "    \n",
    "    A step is ready if:\n",
    "    - All preconditions are satisfied (present in beliefs)\n",
    "    - There are no unresolved defeaters\n",
    "    \n",
    "    Args:\n",
    "        step_id: The ID of the step to check.\n",
    "    \n",
    "    Returns:\n",
    "        Readiness status with details.\n",
    "    \"\"\"\n",
    "    state = tool_context.state\n",
    "    beliefs: List[Dict[str, Any]] = state.get(\"beliefs\", [])\n",
    "    plan = state.get(\"active_plan\", [])\n",
    "    \n",
    "    step = next((s for s in plan if s.get(\"id\") == step_id), None)\n",
    "    if not step:\n",
    "        return {\"status\": \"not_found\", \"step_id\": step_id}\n",
    "    \n",
    "    belief_props = {b.get(\"proposition\", \"\") for b in beliefs}\n",
    "    \n",
    "    unmet_preconditions = [\n",
    "        p for p in step.get(\"preconditions\", [])\n",
    "        if p not in belief_props\n",
    "    ]\n",
    "    \n",
    "    active_defeaters = step.get(\"defeaters\", [])\n",
    "    \n",
    "    is_ready = len(unmet_preconditions) == 0 and len(active_defeaters) == 0\n",
    "    \n",
    "    return {\n",
    "        \"status\": \"ready\" if is_ready else \"blocked\",\n",
    "        \"step_id\": step_id,\n",
    "        \"unmet_preconditions\": unmet_preconditions,\n",
    "        \"active_defeaters\": active_defeaters,\n",
    "    }\n",
    "\n",
    "\n",
    "# ---------- 7. Generic Plan Evaluation (Precondition Check) ----------\n",
    "\n",
    "def evaluate_plan_preconditions(\n",
    "    tool_context: ToolContext,\n",
    ") -> Dict[str, Any]:\n",
    "    \"\"\"Check all plan steps for unmet preconditions.\n",
    "    \n",
    "    This is a generic evaluation that adds defeaters for any step\n",
    "    whose preconditions are not satisfied by current beliefs.\n",
    "    \n",
    "    Returns:\n",
    "        Summary of precondition violations found.\n",
    "    \"\"\"\n",
    "    state = tool_context.state\n",
    "    plan = state.get(\"active_plan\", [])\n",
    "    beliefs = state.get(\"beliefs\", [])\n",
    "    \n",
    "    violations: List[Dict[str, Any]] = []\n",
    "    belief_props = {b.get(\"proposition\", \"\") for b in beliefs}\n",
    "    \n",
    "    for step in plan:\n",
    "        for precond in step.get(\"preconditions\", []):\n",
    "            if precond not in belief_props:\n",
    "                defeater = f\"Unmet precondition: {precond}\"\n",
    "                if defeater not in step.get(\"defeaters\", []):\n",
    "                    step.setdefault(\"defeaters\", []).append(defeater)\n",
    "                    violations.append({\n",
    "                        \"step_id\": step.get(\"id\"),\n",
    "                        \"rule\": \"precondition_check\",\n",
    "                        \"defeater\": defeater,\n",
    "                    })\n",
    "    \n",
    "    state[\"active_plan\"] = plan\n",
    "    \n",
    "    return {\n",
    "        \"status\": \"evaluated\",\n",
    "        \"violations_found\": len(violations),\n",
    "        \"violations\": violations,\n",
    "    }\n",
    "\n",
    "\n",
    "print(\"Generic reasoning tools loaded: get_project_state, update_active_plan, record_step_defeater,\")\n",
    "print(\"  add_belief, remove_belief, check_step_readiness, evaluate_plan_preconditions\")"
   ]
  },

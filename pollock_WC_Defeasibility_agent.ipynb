    "    \n",
    "    # Interactive Q&A loop\n",
    "    for round_num in range(max_rounds):\n",
    "        try:\n",
    "            user_input = input(\"You: \").strip()\n",
    "        except EOFError:\n",
    "            # Handle non-interactive environments\n",
    "            print(\"\\n[Warning] Non-interactive environment detected. Requesting summary...\")\n",

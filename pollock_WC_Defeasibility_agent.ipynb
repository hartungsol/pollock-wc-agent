    "    print(\"\\n\" + \"=\" * 60)\n",
    "    print(f\"CURRENT STATE - ITERATION {iteration}\")\n",
    "    print(\"=\" * 60)\n",
    "    \n",
    "    # Count step statuses\n",
    "    status_counts = {\"SUPPORTED\": 0, \"QUESTIONED\": 0, \"DEFEATED\": 0}\n",
    "    for step in active_plan:\n",

    "# ==========================================================================\n",
    "# Step 2: Helper to Extract Unresolved Issues from State\n",
    "# ==========================================================================\n",
    "\n",
    "def get_unresolved_plan_issues(state: dict) -> dict:\n",
    "    \"\"\"\n",
    "    Inspect the current state (beliefs + active_plan) and return a structured\n",
    "    summary of unresolved issues.\n",
    "\n",
    "    Unresolved issues are primarily:\n",
    "      - Plan steps with status == 'QUESTIONED'\n",
    "      - Optionally, steps with DEFEATED status where the reason indicates missing info\n",
    "\n",
    "    Args:\n",
    "        state: The current state dictionary with 'beliefs' and 'active_plan'\n",
    "        \n",
    "    Returns:\n",
    "        dict with 'questioned_steps' list and 'defeated_steps' list\n",
    "    \"\"\"\n",
    "    active_plan = state.get(\"active_plan\", [])\n",
    "    questioned_steps = []\n",
    "    defeated_steps = []\n",
    "\n",
    "    for step in active_plan:\n",
    "        status = step.get(\"status\", \"SUPPORTED\")\n",
    "        step_info = {\n",
    "            \"id\": step.get(\"id\"),\n",
    "            \"description\": step.get(\"description\", \"\")[:100],\n",
    "            \"defeaters\": step.get(\"defeaters\", []),\n",
    "            \"status\": status,\n",
    "        }\n",
    "        \n",
    "        if status == \"QUESTIONED\":\n",
    "            questioned_steps.append(step_info)\n",
    "        elif status == \"DEFEATED\":\n",
    "            # Include defeated steps too - may need investigation\n",
    "            defeated_steps.append(step_info)\n",
    "\n",
    "    return {\n",
    "        \"questioned_steps\": questioned_steps,\n",
    "        \"defeated_steps\": defeated_steps,\n",
    "        \"total_unresolved\": len(questioned_steps) + len(defeated_steps),\n",
    "    }\n",
    "\n",
    "\n",
    "def format_unresolved_for_investigation(state: dict) -> str:\n",
    "    \"\"\"\n",
    "    Format unresolved issues as a string for the Investigation Agent.\n",
    "    \n",
    "    Args:\n",
    "        state: The current state dictionary\n",
    "        \n",
    "    Returns:\n",
    "        Formatted string describing unresolved issues and current beliefs\n",
    "    \"\"\"\n",
    "    issues = get_unresolved_plan_issues(state)\n",
    "    beliefs = state.get(\"beliefs\", [])\n",
    "    \n",
    "    lines = []\n",
    "    lines.append(\"=== CURRENT CLAIM BELIEFS ===\")\n",
    "    for b in beliefs[:20]:  # Limit to avoid token overflow\n",
    "        prop = b.get(\"proposition\", str(b)) if isinstance(b, dict) else str(b)\n",
    "        lines.append(f\"  - {prop}\")\n",
    "    if len(beliefs) > 20:\n",
    "        lines.append(f\"  ... and {len(beliefs) - 20} more beliefs\")\n",
    "    \n",
    "    lines.append(\"\\n=== QUESTIONED STEPS (need clarification) ===\")\n",
    "    if issues[\"questioned_steps\"]:\n",
    "        for step in issues[\"questioned_steps\"]:\n",
    "            lines.append(f\"\\n  Step: {step['id']}\")\n",
    "            lines.append(f\"  Description: {step['description']}\")\n",
    "            lines.append(f\"  Defeaters:\")\n",
    "            for d in step[\"defeaters\"][:3]:\n",
    "                if isinstance(d, dict):\n",
    "                    msg = d.get(\"message\", str(d))[:80]\n",
    "                    kind = d.get(\"kind\", \"unknown\")\n",
    "                    severity = d.get(\"severity\", \"?\")\n",
    "                    lines.append(f\"    - [{kind}] (severity: {severity}) {msg}...\")\n",
    "                else:\n",
    "                    lines.append(f\"    - {str(d)[:80]}...\")\n",
    "    else:\n",
    "        lines.append(\"  (No QUESTIONED steps)\")\n",
    "    \n",
    "    lines.append(\"\\n=== DEFEATED STEPS (may need investigation) ===\")\n",
    "    if issues[\"defeated_steps\"]:\n",
    "        for step in issues[\"defeated_steps\"]:\n",
    "            lines.append(f\"\\n  Step: {step['id']}\")\n",
    "            lines.append(f\"  Description: {step['description']}\")\n",
    "            defeater_count = len(step[\"defeaters\"])\n",
    "            lines.append(f\"  Defeaters: {defeater_count} total\")\n",
    "    else:\n",
    "        lines.append(\"  (No DEFEATED steps)\")\n",
    "    \n",
    "    return \"\\n\".join(lines)\n",
    "\n",
    "\n",
    "print(\"Unresolved issue detection helpers loaded:\")\n",
    "print(\"  - get_unresolved_plan_issues(state) -> dict\")\n",
    "print(\"  - format_unresolved_for_investigation(state) -> str\")"
   ]

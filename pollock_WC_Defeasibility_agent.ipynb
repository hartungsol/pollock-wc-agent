   "source": [
    "# ==========================================================================\n",
    "# SimpleContext Adapter\n",
    "# ==========================================================================\n",
    "# Decouples core reasoning from ADK's ToolContext for testability and demos.\n",
    "# ==========================================================================\n",
    "\n",
    "from dataclasses import dataclass, field\n",
    "from typing import Dict, List, Any, Optional\n",
    "from copy import deepcopy\n",
    "\n",
    "\n",
    "@dataclass\n",
    "class SimpleContext:\n",
    "    \"\"\"\n",
    "    A lightweight context object that mirrors the state structure used in ADK ToolContext.\n",
    "    \n",
    "    This allows the defeasible reasoning core to operate independently of ADK,\n",
    "    making it testable and usable in demo/standalone mode.\n",
    "    \n",
    "    Attributes:\n",
    "        beliefs: List of belief dicts (id, content, supported, evidence, etc.)\n",
    "        goals: List of goal dicts (id, description, status, priority)\n",
    "        plan: List of plan step dicts with defeaters and status\n",
    "        claim_context: Dict with claim-specific data (claimant, injury, dates, etc.)\n",
    "        iteration: Current reasoning iteration count\n",
    "        metadata: Optional dict for additional state (e.g., regulatory notes)\n",
    "    \"\"\"\n",
    "    beliefs: List[Dict[str, Any]] = field(default_factory=list)\n",
    "    goals: List[Dict[str, Any]] = field(default_factory=list)\n",
    "    plan: List[Dict[str, Any]] = field(default_factory=list)\n",
    "    claim_context: Dict[str, Any] = field(default_factory=dict)\n",
    "    iteration: int = 0\n",
    "    metadata: Dict[str, Any] = field(default_factory=dict)\n",
    "    \n",
    "    def get_belief_by_id(self, belief_id: str) -> Optional[Dict[str, Any]]:\n",
    "        \"\"\"Look up a belief by its ID.\"\"\"\n",
    "        for b in self.beliefs:\n",
    "            if b.get(\"id\") == belief_id:\n",
    "                return b\n",
    "        return None\n",
    "    \n",
    "    def get_step_by_id(self, step_id: str) -> Optional[Dict[str, Any]]:\n",
    "        \"\"\"Look up a plan step by its ID.\"\"\"\n",
    "        for s in self.plan:\n",
    "            if s.get(\"id\") == step_id:\n",
    "                return s\n",
    "        return None\n",
    "    \n",
    "    def add_belief(self, belief: Dict[str, Any]) -> None:\n",
    "        \"\"\"Add a belief to the context.\"\"\"\n",
    "        self.beliefs.append(belief)\n",
    "    \n",
    "    def add_step(self, step: Dict[str, Any]) -> None:\n",
    "        \"\"\"Add a plan step to the context.\"\"\"\n",
    "        self.plan.append(step)\n",
    "    \n",
    "    def to_dict(self) -> Dict[str, Any]:\n",
    "        \"\"\"Convert to a plain dict (for serialization or state storage).\"\"\"\n",
    "        return {\n",
    "            \"beliefs\": deepcopy(self.beliefs),\n",
    "            \"goals\": deepcopy(self.goals),\n",
    "            \"plan\": deepcopy(self.plan),\n",
    "            \"claim_context\": deepcopy(self.claim_context),\n",
    "            \"iteration\": self.iteration,\n",
    "            \"metadata\": deepcopy(self.metadata),\n",
    "        }\n",
    "    \n",
    "    @classmethod\n",
    "    def from_dict(cls, data: Dict[str, Any]) -> \"SimpleContext\":\n",
    "        \"\"\"Create a SimpleContext from a dict.\"\"\"\n",
    "        return cls(\n",
    "            beliefs=data.get(\"beliefs\", []),\n",
    "            goals=data.get(\"goals\", []),\n",
    "            plan=data.get(\"plan\", []),\n",
    "            claim_context=data.get(\"claim_context\", {}),\n",
    "            iteration=data.get(\"iteration\", 0),\n",
    "            metadata=data.get(\"metadata\", {}),\n",
    "        )\n",
    "\n",
    "\n",
    "def simple_context_from_tool_context(tool_context: Any) -> SimpleContext:\n",
    "    \"\"\"\n",
    "    Extract a SimpleContext from an ADK ToolContext.\n",
    "    \n",
    "    This reads the relevant state keys from tool_context.state and\n",
    "    packages them into a SimpleContext for use by reasoning functions.\n",
    "    \n",
    "    Args:\n",
    "        tool_context: An ADK ToolContext (or any object with a .state dict).\n",
    "        \n",
    "    Returns:\n",
    "        A SimpleContext populated with the current state.\n",
    "    \"\"\"\n",
    "    state = getattr(tool_context, \"state\", {})\n",
    "    return SimpleContext(\n",
    "        beliefs=state.get(\"beliefs\", []),\n",
    "        goals=state.get(\"goals\", []),\n",
    "        plan=state.get(\"plan\", []),\n",
    "        claim_context=state.get(\"claim_context\", {}),\n",
    "        iteration=state.get(\"iteration\", 0),\n",
    "        metadata=state.get(\"metadata\", {}),\n",
    "    )\n",
    "\n",
    "\n",
    "def update_tool_context_from_simple(tool_context: Any, simple_ctx: SimpleContext) -> None:\n",
    "    \"\"\"\n",
    "    Write a SimpleContext back to an ADK ToolContext.\n",
    "    \n",
    "    This updates tool_context.state with the values from simple_ctx,\n",
    "    allowing changes made by reasoning functions to be persisted.\n",
    "    \n",
    "    Args:\n",
    "        tool_context: An ADK ToolContext to update.\n",
    "        simple_ctx: The SimpleContext with updated state.\n",
    "    \"\"\"\n",
    "    state = tool_context.state\n",
    "    state[\"beliefs\"] = simple_ctx.beliefs\n",
    "    state[\"goals\"] = simple_ctx.goals\n",
    "    state[\"plan\"] = simple_ctx.plan\n",
    "    state[\"claim_context\"] = simple_ctx.claim_context\n",
    "    state[\"iteration\"] = simple_ctx.iteration\n",
    "    state[\"metadata\"] = simple_ctx.metadata\n",
    "\n",
    "\n",
    "def create_demo_context(\n",
    "    claim_context: Optional[Dict[str, Any]] = None,\n",
    "    initial_beliefs: Optional[List[Dict[str, Any]]] = None,\n",

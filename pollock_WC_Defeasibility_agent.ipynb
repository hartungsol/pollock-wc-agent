    "# ==========================================================================\n",
    "# Backward Reasoning Layer\n",
    "# ==========================================================================\n",
    "# Implements Pollock-style backward reasons for decomposing goals\n",
    "# into subgoals that must be established.\n",
    "\n",
    "from dataclasses import dataclass, field\n",
    "from typing import Dict, Any, List, Callable, Optional\n",
    "\n",
    "\n",
    "# ---------- Backward Reasoning Data Structures ----------\n",
    "\n",
    "@dataclass\n",
    "class BackwardReason:\n",
    "    \"\"\"A backward reason that decomposes goals into subgoals.\n",
    "    \n",
    "    Attributes:\n",
    "        name: Unique identifier for this reason\n",
    "        goal_match_fn: Function that matches target propositions/goals\n",
    "        subgoals_fn: Function that returns subgoals from bindings\n",
    "        description: Human-readable description\n",
    "        strength: Confidence in this decomposition\n",
    "        variables: Variables used in the reason\n",
    "    \"\"\"\n",
    "    name: str\n",
    "    goal_match_fn: Callable[[str], Optional[Binding]]\n",
    "    subgoals_fn: Callable[[Binding], List[str]]\n",
    "    description: Optional[str] = None\n",
    "    strength: float = 1.0\n",
    "    variables: List[str] = field(default_factory=list)\n",
    "\n",
    "\n",
    "# Registry of all backward reasons\n",
    "BACKWARDS_REASONS: Dict[str, BackwardReason] = {}\n",
    "\n",
    "\n",
    "# ---------- Decorator DSL for Backward Reasons ----------\n",
    "\n",
    "def def_backwards_reason(\n",
    "    name: str,\n",
    "    *,\n",
    "    goal_match_fn: Callable[[str], Optional[Binding]],\n",
    "    description: str | None = None,\n",
    "    strength: float = 1.0,\n",
    "    variables: List[str] = None,\n",
    "):\n",
    "    \"\"\"Decorator to define a backward reason.\n",
    "    \n",
    "    Usage:\n",
    "        @def_backwards_reason(\n",
    "            name=\"approve_claim_requirements\",\n",
    "            goal_match_fn=lambda p: {\"matched\": True} if p == \"decision:approve_claim\" else None,\n",
    "            description=\"Requirements for claim approval\"\n",
    "        )\n",
    "        def approval_subgoals(binding: Binding) -> List[str]:\n",
    "            return [\n",
    "                \"analysis:compensability_confirmed=true\",\n",
    "                \"compliance:all_requirements_met=true\",\n",
    "                \"risk:fraud_cleared=true\",\n",
    "            ]\n",
    "    \"\"\"\n",
    "    variables = variables or []\n",
    "\n",
    "    def decorator(subgoals_fn: Callable[[Binding], List[str]]):\n",
    "        reason = BackwardReason(\n",
    "            name=name,\n",
    "            goal_match_fn=goal_match_fn,\n",
    "            subgoals_fn=subgoals_fn,\n",
    "            description=description,\n",
    "            strength=strength,\n",
    "            variables=variables,\n",
    "        )\n",
    "        BACKWARDS_REASONS[name] = reason\n",
    "        return subgoals_fn\n",
    "\n",
    "    return decorator\n",
    "\n",
    "\n",
    "# ---------- Backward Reasoning Functions ----------\n",
    "\n",
    "def suggest_backward_subgoals(target_prop: str) -> List[Dict[str, Any]]:\n",
    "    \"\"\"Given a target proposition, return applicable backward reasons and subgoals.\n",
    "    \n",
    "    Args:\n",
    "        target_prop: The goal proposition (e.g., 'decision:approve_claim')\n",
    "        \n",
    "    Returns:\n",
    "        List of dicts with reason name, subgoals, and description\n",
    "    \"\"\"\n",
    "    results = []\n",
    "    for reason in BACKWARDS_REASONS.values():\n",
    "        binding = reason.goal_match_fn(target_prop)\n",
    "        if binding is None:\n",
    "            continue\n",
    "        subgoals = reason.subgoals_fn(binding)\n",
    "        results.append({\n",
    "            \"reason\": reason.name,\n",
    "            \"subgoals\": subgoals,\n",
    "            \"description\": reason.description,\n",
    "            \"strength\": reason.strength,\n",
    "        })\n",
    "    return results\n",
    "\n",
    "\n",
    "def check_subgoals_satisfied(\n",
    "    target_prop: str, \n",
    "    context_or_state\n",
    ") -> Dict[str, Any]:\n",
    "    \"\"\"Check which subgoals for a target are satisfied by current beliefs.\n",
    "    \n",
    "    Args:\n",
    "        target_prop: The goal proposition\n",
    "        context_or_state: ToolContext or state dict\n",
    "        \n",
    "    Returns:\n",
    "        Analysis of which subgoals are satisfied, missing, and recommendations\n",
    "    \"\"\"\n",
    "    if hasattr(context_or_state, \"state\"):\n",
    "        state = context_or_state.state\n",
    "    else:\n",
    "        state = context_or_state\n",
    "    \n",
    "    beliefs = state.get(\"beliefs\", [])\n",
    "    props = set(_get_prop(b) for b in beliefs)\n",
    "    \n",
    "    applicable_reasons = suggest_backward_subgoals(target_prop)\n",
    "    \n",
    "    if not applicable_reasons:\n",
    "        return {\n",
    "            \"target\": target_prop,\n",
    "            \"status\": \"no_applicable_reasons\",\n",
    "            \"message\": f\"No backward reasons found for '{target_prop}'\",\n",
    "        }\n",
    "    \n",
    "    results = []\n",
    "    for reason_info in applicable_reasons:\n",
    "        subgoals = reason_info[\"subgoals\"]\n",
    "        satisfied = []\n",
    "        missing = []\n",
    "        \n",
    "        for sg in subgoals:\n",
    "            # Check if subgoal is in beliefs (exact or partial match)\n",
    "            if sg in props:\n",
    "                satisfied.append(sg)\n",
    "            else:\n",
    "                # Try partial matching for key=value patterns\n",
    "                sg_prefix = sg.split(\"=\")[0] if \"=\" in sg else sg\n",
    "                partial_match = any(p.startswith(sg_prefix) for p in props)\n",
    "                if partial_match:\n",
    "                    satisfied.append(sg)\n",
    "                else:\n",
    "                    missing.append(sg)\n",
    "        \n",
    "        all_satisfied = len(missing) == 0\n",
    "        results.append({\n",
    "            \"reason\": reason_info[\"reason\"],\n",
    "            \"description\": reason_info[\"description\"],\n",
    "            \"total_subgoals\": len(subgoals),\n",
    "            \"satisfied\": satisfied,\n",
    "            \"missing\": missing,\n",
    "            \"all_satisfied\": all_satisfied,\n",
    "            \"completion_pct\": len(satisfied) / len(subgoals) * 100 if subgoals else 0,\n",
    "        })\n",
    "    \n",
    "    # Determine overall status\n",
    "    any_complete = any(r[\"all_satisfied\"] for r in results)\n",
    "    \n",
    "    return {\n",
    "        \"target\": target_prop,\n",
    "        \"status\": \"supported\" if any_complete else \"pending\",\n",
    "        \"analyses\": results,\n",
    "        \"recommendation\": \"Target can be concluded\" if any_complete \n",
    "                         else f\"Missing subgoals: {results[0]['missing']}\" if results else \"Unknown\",\n",
    "    }\n",
    "\n",
    "\n",
    "# ---------- Workers' Comp Backward Reasons ----------\n",
    "\n",
    "# Backward reason 1: Approval requirements\n",
    "@def_backwards_reason(\n",
    "    name=\"approve_claim_requirements\",\n",
    "    goal_match_fn=lambda p: {\"matched\": True} if \"decision:approve\" in p.lower() else None,\n",
    "    description=\"Subgoals required to approve a workers' comp claim\"\n",
    ")\n",
    "def approval_subgoals(binding: Binding) -> List[str]:\n",
    "    return [\n",
    "        \"claim:prima_facie_compensable=true\",\n",
    "        \"analysis:compensability_confirmed=true\",\n",
    "        \"compliance:timely_notice=true\",\n",
    "        \"risk:fraud_level=low\",\n",
    "        \"employment:eligibility_verified=true\",\n",
    "    ]\n",
    "\n",
    "\n",
    "# Backward reason 2: Denial justification\n",
    "@def_backwards_reason(\n",
    "    name=\"deny_claim_justification\",\n",
    "    goal_match_fn=lambda p: {\"matched\": True} if \"decision:deny\" in p.lower() else None,\n",
    "    description=\"Justifications that support claim denial\"\n",
    ")\n",
    "def denial_subgoals(binding: Binding) -> List[str]:\n",
    "    return [\n",
    "        \"policy:coverage_exclusion=contractor_status\",\n",
    "        \"claim:prima_facie_compensable=false\",\n",
    "        \"compliance:notice_deadline_exceeded=true\",\n",
    "        \"risk:fraud_level=high\",\n",
    "    ]\n",
    "\n",
    "\n",
    "# Backward reason 3: Escalation triggers\n",
    "@def_backwards_reason(\n",
    "    name=\"escalate_claim_triggers\",\n",
    "    goal_match_fn=lambda p: {\"matched\": True} if \"decision:escalate\" in p.lower() else None,\n",
    "    description=\"Conditions that require claim escalation\"\n",
    ")\n",
    "def escalation_subgoals(binding: Binding) -> List[str]:\n",
    "    return [\n",
    "        \"risk:fraud_level=high\",\n",
    "        \"claim:severity_level=catastrophic\",\n",
    "        \"analysis:causation_disputed=true\",\n",
    "        \"compliance:mandatory_review_required=true\",\n",
    "    ]\n",
    "\n",
    "\n",
    "# Backward reason 4: Compensability establishment\n",
    "@def_backwards_reason(\n",
    "    name=\"establish_compensability\",\n",
    "    goal_match_fn=lambda p: {\"matched\": True} if \"analysis:compensability_confirmed\" in p else None,\n",
    "    description=\"Requirements to establish compensability\"\n",
    ")\n",
    "def compensability_subgoals(binding: Binding) -> List[str]:\n",
    "    return [\n",
    "        \"employment:status=full_time\",\n",
    "        \"employment:on_duty_at_time=true\",\n",
    "        \"medical:work_related=true\",\n",
    "        \"claim:location=workplace\",\n",
    "    ]\n",
    "\n",
    "\n",
    "print(f\"Backward reasoning layer loaded with {len(BACKWARDS_REASONS)} registered reasons:\")\n",
    "for name, reason in BACKWARDS_REASONS.items():\n",
    "    print(f\"  - {name}: {reason.description or 'No description'}\")"
   ]

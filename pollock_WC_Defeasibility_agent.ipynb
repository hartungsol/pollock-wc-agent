    "# ==========================================================================\n",
    "# Forward Reasoning Layer\n",
    "# ==========================================================================\n",
    "# Implements Pollock-style forward reasons for deriving new beliefs\n",
    "# from existing ones.\n",
    "\n",
    "from dataclasses import dataclass, field\n",
    "from typing import Dict, Any, List, Callable, Optional, Tuple\n",
    "\n",
    "# Type aliases\n",
    "Binding = Dict[str, Any]\n",
    "Conclusion = Tuple[str, bool]  # (proposition, is_defeasible)\n",
    "\n",
    "\n",
    "# ---------- Forward Reasoning Data Structures ----------\n",
    "\n",
    "@dataclass\n",
    "class ForwardsPremise:\n",
    "    \"\"\"A premise for a forward reason.\n",
    "    \n",
    "    Attributes:\n",
    "        name: Identifier for this premise\n",
    "        match_fn: Function that takes a proposition string and returns\n",
    "                  a binding dict if matched, None otherwise\n",
    "        condition_fn: Optional additional condition on the match\n",
    "    \"\"\"\n",
    "    name: str\n",
    "    match_fn: Callable[[str], Optional[Binding]]\n",
    "    condition_fn: Optional[Callable[[str, Binding], bool]] = None\n",
    "\n",
    "\n",
    "@dataclass\n",
    "class ForwardsReason:\n",
    "    \"\"\"A forward reason that derives conclusions from premises.\n",
    "    \n",
    "    Attributes:\n",
    "        name: Unique identifier for this reason\n",
    "        premises: List of premises that must be satisfied\n",
    "        conclusion_fn: Function that produces conclusions from bindings\n",
    "        variables: Variables used in the reason\n",
    "        strength: Confidence strength of derived conclusions\n",
    "        defeasible: Whether conclusions can be defeated\n",
    "        description: Human-readable description\n",
    "        discount_factor: Factor to reduce strength through chaining\n",
    "        temporal: Whether this reason has temporal aspects\n",
    "    \"\"\"\n",
    "    name: str\n",
    "    premises: List[ForwardsPremise]\n",
    "    conclusion_fn: Callable[[Binding], List[Conclusion]]\n",
    "    variables: List[str] = field(default_factory=list)\n",
    "    strength: float = 1.0\n",
    "    defeasible: bool = True\n",
    "    description: Optional[str] = None\n",
    "    discount_factor: float = 1.0\n",
    "    temporal: Optional[bool] = None\n",
    "\n",
    "\n",
    "# Registry of all forward reasons\n",
    "FORWARDS_REASONS: Dict[str, ForwardsReason] = {}\n",
    "\n",
    "\n",
    "# ---------- Decorator DSL for Forward Reasons ----------\n",
    "\n",
    "def def_forwards_reason(\n",
    "    name: str,\n",
    "    *,\n",
    "    premises: List[ForwardsPremise],\n",
    "    variables: List[str] = None,\n",
    "    strength: float = 1.0,\n",
    "    defeasible: bool = True,\n",
    "    description: str | None = None,\n",
    "    discount_factor: float = 1.0,\n",
    "    temporal: bool | None = None,\n",
    "):\n",
    "    \"\"\"Decorator to define a forward reason.\n",
    "    \n",
    "    Usage:\n",
    "        @def_forwards_reason(\n",
    "            name=\"prima_facie_compensable\",\n",
    "            premises=[...],\n",
    "            description=\"Infer prima facie compensability\"\n",
    "        )\n",
    "        def infer_compensable(binding: Binding) -> List[Conclusion]:\n",
    "            return [(\"claim:prima_facie_compensable=true\", True)]\n",
    "    \"\"\"\n",
    "    variables = variables or []\n",
    "\n",
    "    def decorator(conclusion_fn: Callable[[Binding], List[Conclusion]]):\n",
    "        reason = ForwardsReason(\n",
    "            name=name,\n",
    "            premises=premises,\n",
    "            conclusion_fn=conclusion_fn,\n",
    "            variables=variables,\n",
    "            strength=strength,\n",
    "            defeasible=defeasible,\n",
    "            description=description,\n",
    "            discount_factor=discount_factor,\n",
    "            temporal=temporal,\n",
    "        )\n",
    "        FORWARDS_REASONS[name] = reason\n",
    "        return conclusion_fn\n",
    "\n",
    "    return decorator\n",
    "\n",
    "\n",
    "# ---------- Pattern Matchers for Premises ----------\n",
    "\n",
    "def make_simple_pattern_matcher(\n",
    "    prefix: str, \n",
    "    var_name: str, \n",
    "    cast: Callable[[str], Any] = str\n",
    ") -> Callable[[str], Optional[Binding]]:\n",
    "    \"\"\"Create a matcher for propositions like 'prefix:value' or 'prefix:key=value'.\n",
    "    \n",
    "    Args:\n",
    "        prefix: The prefix to match (e.g., \"employment\", \"risk\")\n",
    "        var_name: Name of the variable to bind the value to\n",
    "        cast: Function to cast the extracted value\n",
    "        \n",
    "    Returns:\n",
    "        A match function suitable for ForwardsPremise\n",
    "    \"\"\"\n",
    "    def match_fn(prop: str) -> Optional[Binding]:\n",
    "        if not prop.startswith(prefix + \":\"):\n",
    "            return None\n",
    "        try:\n",
    "            value_str = prop.split(\":\", 1)[1]\n",
    "            # Handle key=value format\n",
    "            if \"=\" in value_str:\n",
    "                key, val = value_str.split(\"=\", 1)\n",
    "                return {var_name: cast(val), f\"{var_name}_key\": key}\n",
    "            return {var_name: cast(value_str)}\n",
    "        except Exception:\n",
    "            return None\n",
    "    return match_fn\n",
    "\n",
    "\n",
    "def make_exact_matcher(target_prop: str) -> Callable[[str], Optional[Binding]]:\n",
    "    \"\"\"Create a matcher for exact proposition matches.\"\"\"\n",
    "    def match_fn(prop: str) -> Optional[Binding]:\n",
    "        if prop == target_prop:\n",
    "            return {\"matched\": True}\n",
    "        return None\n",
    "    return match_fn\n",
    "\n",
    "\n",
    "def make_value_matcher(\n",
    "    prefix: str,\n",
    "    key: str,\n",
    "    expected_value: str,\n",
    "    case_insensitive: bool = True\n",
    ") -> Callable[[str], Optional[Binding]]:\n",
    "    \"\"\"Create a matcher for specific key=value propositions.\"\"\"\n",
    "    def match_fn(prop: str) -> Optional[Binding]:\n",
    "        target = f\"{prefix}:{key}={expected_value}\"\n",
    "        if case_insensitive:\n",
    "            if prop.lower() == target.lower():\n",
    "                return {\"matched\": True, \"value\": expected_value}\n",
    "        else:\n",
    "            if prop == target:\n",
    "                return {\"matched\": True, \"value\": expected_value}\n",
    "        return None\n",
    "    return match_fn\n",
    "\n",
    "\n",
    "# ---------- Apply Forward Reasons ----------\n",
    "\n",
    "def apply_forwards_reasons(context_or_state) -> Dict[str, Any]:\n",
    "    \"\"\"Apply all registered forward reasons to derive new beliefs.\n",
    "    \n",
    "    Scans current beliefs, matches premises against them, and adds\n",
    "    new beliefs for any conclusions that can be derived.\n",
    "    \n",
    "    Args:\n",
    "        context_or_state: Either a ToolContext or a state dict\n",
    "        \n",
    "    Returns:\n",
    "        Summary of new beliefs added\n",
    "    \"\"\"\n",
    "    # Handle both ToolContext and raw state dict\n",
    "    if hasattr(context_or_state, \"state\"):\n",
    "        state = context_or_state.state\n",
    "    else:\n",
    "        state = context_or_state\n",
    "    \n",
    "    beliefs = state.get(\"beliefs\", [])\n",
    "    new_beliefs = []\n",
    "    \n",
    "    # Get all propositions\n",
    "    props = []\n",
    "    for b in beliefs:\n",
    "        if isinstance(b, dict):\n",
    "            props.append(b.get(\"proposition\", \"\"))\n",
    "        elif hasattr(b, \"proposition\"):\n",
    "            props.append(b.proposition)\n",
    "    \n",
    "    # Apply each forward reason\n",
    "    for reason in FORWARDS_REASONS.values():\n",
    "        if not reason.premises:\n",
    "            continue\n",
    "            \n",
    "        # For simplicity, support single-premise and multi-premise reasons\n",
    "        if len(reason.premises) == 1:\n",
    "            # Single premise case\n",
    "            premise = reason.premises[0]\n",
    "            for prop in props:\n",
    "                binding = premise.match_fn(prop)\n",
    "                if binding is None:\n",
    "                    continue\n",
    "                if premise.condition_fn and not premise.condition_fn(prop, binding):\n",
    "                    continue\n",
    "                \n",
    "                # Generate conclusions\n",
    "                conclusions = reason.conclusion_fn(binding)\n",
    "                for c_prop, is_defeasible in conclusions:\n",
    "                    # Check if conclusion already exists\n",
    "                    if any(_get_prop(b) == c_prop for b in beliefs + new_beliefs):\n",
    "                        continue\n",
    "                    \n",
    "                    new_belief = {\n",
    "                        \"id\": f\"fr-{reason.name}-{len(beliefs) + len(new_beliefs) + 1}\",\n",
    "                        \"proposition\": c_prop,\n",
    "                        \"support\": f\"Forward reason '{reason.name}' from: {prop}\",\n",
    "                        \"strength\": reason.strength * reason.discount_factor,\n",
    "                        \"defeasible\": is_defeasible,\n",
    "                    }\n",
    "                    new_beliefs.append(new_belief)\n",
    "        \n",
    "        else:\n",
    "            # Multi-premise case: all premises must be satisfied\n",
    "            all_bindings = {}\n",
    "            all_matched = True\n",
    "            matched_props = []\n",
    "            \n",
    "            for premise in reason.premises:\n",
    "                matched = False\n",
    "                for prop in props:\n",
    "                    binding = premise.match_fn(prop)\n",
    "                    if binding is not None:\n",
    "                        if premise.condition_fn and not premise.condition_fn(prop, binding):\n",
    "                            continue\n",
    "                        all_bindings.update(binding)\n",
    "                        matched_props.append(prop)\n",
    "                        matched = True\n",
    "                        break\n",
    "                if not matched:\n",
    "                    all_matched = False\n",
    "                    break\n",
    "            \n",
    "            if all_matched:\n",
    "                conclusions = reason.conclusion_fn(all_bindings)\n",
    "                for c_prop, is_defeasible in conclusions:\n",
    "                    if any(_get_prop(b) == c_prop for b in beliefs + new_beliefs):\n",
    "                        continue\n",
    "                    \n",
    "                    new_belief = {\n",
    "                        \"id\": f\"fr-{reason.name}-{len(beliefs) + len(new_beliefs) + 1}\",\n",
    "                        \"proposition\": c_prop,\n",
    "                        \"support\": f\"Forward reason '{reason.name}' from: {', '.join(matched_props)}\",\n",
    "                        \"strength\": reason.strength * reason.discount_factor,\n",
    "                        \"defeasible\": is_defeasible,\n",
    "                    }\n",
    "                    new_beliefs.append(new_belief)\n",
    "    \n",
    "    # Add new beliefs to state\n",
    "    beliefs.extend(new_beliefs)\n",
    "    state[\"beliefs\"] = beliefs\n",
    "    \n",
    "    return {\n",
    "        \"status\": \"applied\",\n",
    "        \"num_new_beliefs\": len(new_beliefs),\n",
    "        \"new_beliefs\": new_beliefs,\n",
    "        \"reasons_checked\": list(FORWARDS_REASONS.keys()),\n",
    "    }\n",
    "\n",
    "\n",
    "def _get_prop(belief) -> str:\n",
    "    \"\"\"Helper to get proposition from belief (dict or object).\"\"\"\n",
    "    if isinstance(belief, dict):\n",
    "        return belief.get(\"proposition\", \"\")\n",
    "    return getattr(belief, \"proposition\", \"\")\n",
    "\n",
    "\n",
    "# ---------- Workers' Comp Forward Reasons ----------\n",
    "\n",
    "# Reason 1: Prima facie compensability from employment status + on-duty\n",
    "@def_forwards_reason(\n",
    "    name=\"prima_facie_compensable\",\n",
    "    premises=[\n",
    "        ForwardsPremise(\n",
    "            name=\"full_time_employee\",\n",
    "            match_fn=make_value_matcher(\"employment\", \"status\", \"full_time\"),\n",
    "        ),\n",
    "        ForwardsPremise(\n",
    "            name=\"on_duty\",\n",
    "            match_fn=make_value_matcher(\"employment\", \"on_duty_at_time\", \"true\"),\n",
    "        ),\n",
    "    ],\n",
    "    strength=0.85,\n",
    "    description=\"Full-time employee on duty establishes prima facie compensability\"\n",
    ")\n",
    "def infer_prima_facie_compensable(binding: Binding) -> List[Conclusion]:\n",
    "    return [(\"claim:prima_facie_compensable=true\", True)]\n",
    "\n",
    "\n",
    "# Reason 2: Fraud risk level from fraud score\n",
    "@def_forwards_reason(\n",
    "    name=\"fraud_risk_level\",\n",
    "    premises=[\n",
    "        ForwardsPremise(\n",
    "            name=\"fraud_score\",\n",
    "            match_fn=make_simple_pattern_matcher(\"risk\", \"score\", float),\n",
    "            condition_fn=lambda prop, binding: \"fraud_score\" in prop,\n",
    "        ),\n",
    "    ],\n",
    "    strength=0.9,\n",
    "    description=\"Classify fraud risk level from score\"\n",
    ")\n",
    "def infer_fraud_risk_level(binding: Binding) -> List[Conclusion]:\n",
    "    score = binding.get(\"score\", 0.0)\n",
    "    if score >= 0.7:\n",
    "        return [(\"risk:fraud_level=high\", True)]\n",
    "    elif score >= 0.5:\n",
    "        return [(\"risk:fraud_level=medium\", True)]\n",
    "    else:\n",
    "        return [(\"risk:fraud_level=low\", True)]\n",
    "\n",
    "\n",
    "# Reason 3: Coverage exclusion from contractor status\n",
    "@def_forwards_reason(\n",
    "    name=\"coverage_exclusion_contractor\",\n",
    "    premises=[\n",
    "        ForwardsPremise(\n",
    "            name=\"contractor_status\",\n",
    "            match_fn=lambda prop: {\"matched\": True} if \"employment:status=contractor\" in prop.lower() \n",
    "                                   or \"employment:status=independent_contractor\" in prop.lower() else None,\n",
    "        ),\n",
    "    ],\n",
    "    strength=0.95,\n",
    "    description=\"Contractor status implies coverage exclusion\"\n",
    ")\n",
    "def infer_coverage_exclusion(binding: Binding) -> List[Conclusion]:\n",
    "    return [\n",
    "        (\"policy:coverage_exclusion=contractor_status\", True),\n",
    "        (\"claim:prima_facie_compensable=false\", True),\n",
    "    ]\n",
    "\n",
    "\n",
    "# Reason 4: Timely notice from dates\n",
    "@def_forwards_reason(\n",
    "    name=\"timely_notice_check\",\n",
    "    premises=[\n",
    "        ForwardsPremise(\n",
    "            name=\"notice_status\",\n",
    "            match_fn=lambda prop: {\"days\": int(prop.split(\"=\")[1])} \n",
    "                                  if prop.startswith(\"claim:days_to_notice=\") else None,\n",
    "        ),\n",
    "    ],\n",
    "    strength=0.9,\n",
    "    description=\"Check if notice was timely based on days to notice\"\n",
    ")\n",
    "def infer_timely_notice(binding: Binding) -> List[Conclusion]:\n",
    "    days = binding.get(\"days\", 0)\n",
    "    if days <= 30:  # Default threshold\n",
    "        return [(\"compliance:timely_notice=true\", True)]\n",
    "    else:\n",
    "        return [(\"compliance:timely_notice=false\", True)]\n",
    "\n",
    "\n",
    "print(f\"Forward reasoning layer loaded with {len(FORWARDS_REASONS)} registered reasons:\")\n",
    "for name, reason in FORWARDS_REASONS.items():\n",
    "    print(f\"  - {name}: {reason.description or 'No description'}\")"
   ]

    "# ---------- Extended Domain Models for Workers' Compensation ----------\n",
    "# Extends the base PlanStep with workers' comp-specific fields\n",
    "\n",
    "from __future__ import annotations\n",
    "from typing import Any, List, Dict, Optional\n",
    "from pydantic import BaseModel, Field\n",
    "from datetime import datetime, date\n",
    "\n",
    "\n",
    "class WorkersCompPlanStep(BaseModel):\n",
    "    \"\"\"Extended PlanStep for workers' compensation claim processing.\n",
    "    \n",
    "    Adds required_documents and regulatory_dependencies to support\n",
    "    compliance tracking and audit requirements.\n",
    "    \"\"\"\n",
    "    id: str\n",
    "    description: str\n",
    "    preconditions: List[str] = Field(default_factory=list)\n",
    "    effects: List[str] = Field(default_factory=list)\n",
    "    risk_factors: List[str] = Field(default_factory=list)\n",
    "    defeaters: List[str] = Field(default_factory=list)\n",
    "    required_documents: List[str] = Field(default_factory=list)\n",
    "    regulatory_dependencies: List[str] = Field(default_factory=list)\n",
    "\n",
    "\n",

    "        if added_count == 0:\n",
    "            if verbose:\n",
    "                print(\"\\n[STOP] No new beliefs added. Cannot make further progress.\")\n",
    "            break\n",
    "        \n",
    "        if verbose:\n",
    "            print(\"\\n  Re-running analysis with new beliefs...\")\n",
    "    \n",
    "    # Build final summary\n",
    "    final_issues = get_unresolved_plan_issues(context.state)\n",
    "    backward_summary = get_backward_analysis_summary(context.state)\n",
    "    \n",
    "    final_summary = {\n",
    "        \"iterations_completed\": len(iteration_history),\n",
    "        \"max_iterations\": max_iterations,\n",
    "        \"final_step_statuses\": context.state.get(\"active_plan\", []),\n",
    "        \"final_beliefs_count\": len(context.state.get(\"beliefs\", [])),\n",
    "        \"final_questioned_count\": len(final_issues[\"questioned_steps\"]),\n",
    "        \"final_defeated_count\": len(final_issues[\"defeated_steps\"]),\n",
    "        \"iteration_history\": iteration_history,\n",
    "        \"backward_analysis\": context.state.get(\"backward_analysis\", []),\n",
    "        \"backward_summary\": backward_summary,\n",
    "        \"state\": context.state,\n",
    "    }\n",
    "    \n",
    "    if verbose:\n",

   "source": [
    "# ==========================================================================\n",
    "# LLM-Based Defeater Suggestion Tool\n",
    "# ==========================================================================\n",
    "# Uses Gemini to suggest additional evidential defeaters based on claim\n",
    "# patterns, without overriding deterministic statutory rules.\n",
    "# Severity is clamped to [0.3, 0.7] to prevent LLM defeaters from alone\n",
    "# causing a step to become DEFEATED (which requires severity >= 0.8).\n",
    "# ==========================================================================\n",
    "\n",
    "import asyncio\n",
    "import json\n",
    "import re\n",
    "from typing import List, Dict, Any\n",
    "from google.genai import types\n",
    "from google import genai\n",
    "\n",
    "# Enable nested event loops for Jupyter compatibility\n",
    "import nest_asyncio\n",
    "nest_asyncio.apply()\n",
    "\n",
    "# Initialize Gemini client for defeater suggestions\n",
    "_defeater_client = genai.Client()\n",
    "\n",
    "# Define JSON schema for LLM defeater output\n",
    "LLM_DEFEATER_SCHEMA = {\n",
    "    \"type\": \"object\",\n",
    "    \"properties\": {\n",
    "        \"suggested_defeaters\": {\n",
    "            \"type\": \"array\",\n",
    "            \"items\": {\n",
    "                \"type\": \"object\",\n",
    "                \"properties\": {\n",
    "                    \"target_step_id\": {\n",
    "                        \"type\": \"string\",\n",
    "                        \"description\": \"The step ID to attach this defeater to (e.g., 'wc-step-4')\"\n",
    "                    },\n",
    "                    \"message\": {\n",
    "                        \"type\": \"string\",\n",
    "                        \"description\": \"Clear explanation of the potential issue or concern\"\n",
    "                    },\n",
    "                    \"severity\": {\n",
    "                        \"type\": \"number\",\n",
    "                        \"description\": \"Severity score between 0.3 and 0.7\"\n",
    "                    },\n",
    "                    \"reasoning\": {\n",
    "                        \"type\": \"string\",\n",
    "                        \"description\": \"Brief explanation of why this defeater was suggested\"\n",
    "                    }\n",
    "                },\n",
    "                \"required\": [\"target_step_id\", \"message\", \"severity\"]\n",
    "            }\n",
    "        }\n",
    "    },\n",
    "    \"required\": [\"suggested_defeaters\"]\n",
    "}\n",
    "\n",
    "LLM_DEFEATER_PROMPT_TEMPLATE = '''You are an expert workers' compensation claims analyst reviewing a claim for potential issues.\n",
    "\n",
    "CURRENT BELIEFS (Evidence Base):\n",
    "{beliefs_summary}\n",
    "\n",
    "CURRENT PLAN STEPS:\n",
    "{plan_summary}\n",
    "\n",
    "CLAIM CONTEXT:\n",
    "{claim_context}\n",
    "\n",
    "Your task: Identify any additional evidential concerns that may not be captured by deterministic rules.\n",
    "Focus on:\n",
    "- Pattern-based red flags (e.g., timing inconsistencies, unusual claim narratives)\n",
    "- Missing documentation that should be present\n",
    "- Inconsistencies between reported facts\n",
    "- Industry-specific risk factors\n",
    "- Medical treatment patterns that warrant review\n",
    "\n",
    "IMPORTANT CONSTRAINTS:\n",
    "1. Do NOT duplicate issues already captured in existing defeaters\n",
    "2. Severity scores MUST be between 0.3 and 0.7 (you cannot override statutory rules)\n",
    "3. Focus on evidential/investigative concerns, not regulatory violations\n",
    "4. Each defeater should be actionable for a claims adjuster\n",
    "\n",
    "Return your analysis as JSON with the schema: {{\"suggested_defeaters\": [...]}}\n",
    "Each defeater needs: target_step_id, message, severity, and reasoning.\n",
    "\n",
    "Valid step IDs: wc-step-1 through wc-step-8\n",
    "'''\n",
    "\n",
    "\n",
    "def _build_beliefs_summary(beliefs: List[Dict[str, Any]]) -> str:\n",
    "    \"\"\"Build a concise summary of current beliefs for LLM context.\"\"\"\n",
    "    lines = []\n",
    "    for b in beliefs[:30]:  # Limit to avoid token overflow\n",
    "        prop = b.get(\"proposition\", \"\") if isinstance(b, dict) else str(b)\n",
    "        strength = b.get(\"strength\", 1.0) if isinstance(b, dict) else 1.0\n",
    "        lines.append(f\"  - {prop} (confidence: {strength:.0%})\")\n",
    "    if len(beliefs) > 30:\n",
    "        lines.append(f\"  ... and {len(beliefs) - 30} more beliefs\")\n",
    "    return \"\\n\".join(lines) if lines else \"  (No beliefs recorded)\"\n",
    "\n",
    "\n",
    "def _build_plan_summary(plan: List[Dict[str, Any]]) -> str:\n",
    "    \"\"\"Build a concise summary of current plan steps for LLM context.\"\"\"\n",
    "    lines = []\n",
    "    for step in plan:\n",
    "        step_id = step.get(\"id\", \"?\")\n",
    "        description = step.get(\"description\", \"No description\")[:100]\n",
    "        status = step.get(\"status\", \"SUPPORTED\")\n",
    "        defeaters = step.get(\"defeaters\", [])\n",
    "        defeater_count = len(defeaters)\n",
    "        \n",
    "        lines.append(f\"  [{step_id}] {status} - {description}\")\n",
    "        if defeaters:\n",
    "            lines.append(f\"      (has {defeater_count} existing defeater(s))\")\n",
    "            # Show first defeater as example\n",
    "            if isinstance(defeaters[0], dict):\n",
    "                first_msg = defeaters[0].get(\"message\", \"\")[:60]\n",
    "                lines.append(f\"      e.g., {first_msg}...\")\n",
    "    return \"\\n\".join(lines) if lines else \"  (No plan steps)\"\n",
    "\n",
    "\n",
    "def _build_claim_context_summary(claim_context: Dict[str, Any]) -> str:\n",
    "    \"\"\"Build summary of claim context.\"\"\"\n",
    "    if not claim_context:\n",
    "        return \"  (No claim context available)\"\n",
    "    lines = []\n",
    "    for key, value in claim_context.items():\n",
    "        if not key.startswith(\"_\"):  # Skip internal keys\n",
    "            lines.append(f\"  - {key}: {value}\")\n",
    "    return \"\\n\".join(lines) if lines else \"  (No claim context available)\"\n",
    "\n",
    "\n",
    "async def suggest_llm_defeaters_async(\n",
    "    tool_context,\n",
    "    max_defeaters: int = 5\n",
    ") -> List[Dict[str, Any]]:\n",
    "    \"\"\"Use Gemini to suggest additional evidential defeaters.\n",
    "    \n",
    "    This function queries the LLM to identify potential issues based on\n",
    "    pattern analysis of the claim data. All defeaters returned are:\n",
    "    - Forced to kind=\"evidential\"\n",
    "    - Forced to source_rule=\"llm_pattern_analysis\"\n",
    "    - Clamped to severity [0.3, 0.7] to prevent overriding statutory rules\n",
    "    \n",
    "    Args:\n",
    "        tool_context: ADK tool context with state containing beliefs, plan, claim_context\n",
    "        max_defeaters: Maximum number of defeaters to return (default 5)\n",
    "    \n",
    "    Returns:\n",
    "        List of Defeater-compatible dicts ready to add to plan steps\n",
    "    \"\"\"\n",
    "    state = tool_context.state\n",
    "    beliefs = state.get(\"beliefs\", [])\n",
    "    plan = state.get(\"active_plan\", [])\n",
    "    claim_context = state.get(\"claim_context\", {})\n",
    "    \n",
    "    # Build prompt context\n",
    "    beliefs_summary = _build_beliefs_summary(beliefs)\n",
    "    plan_summary = _build_plan_summary(plan)\n",
    "    claim_context_summary = _build_claim_context_summary(claim_context)\n",
    "    \n",
    "    prompt = LLM_DEFEATER_PROMPT_TEMPLATE.format(\n",
    "        beliefs_summary=beliefs_summary,\n",
    "        plan_summary=plan_summary,\n",
    "        claim_context=claim_context_summary\n",
    "    )\n",
    "    \n",
    "    try:\n",
    "        # Call Gemini with JSON mode\n",
    "        response = await _defeater_client.aio.models.generate_content(\n",
    "            model=\"gemini-2.5-flash\",\n",
    "            contents=[types.Content(role=\"user\", parts=[types.Part(text=prompt)])],\n",
    "            config=types.GenerateContentConfig(\n",
    "                response_mime_type=\"application/json\",\n",
    "                response_schema=LLM_DEFEATER_SCHEMA,\n",
    "                temperature=0.3,  # Lower temperature for more consistent outputs\n",
    "            )\n",
    "        )\n",
    "        \n",
    "        # Parse response with robust JSON extraction\n",
    "        if not response.text:\n",
    "            print(\"[LLM Defeaters] WARNING: Empty response from Gemini\")\n",
    "            return []\n",
    "        \n",
    "        raw_text = response.text.strip()\n",
    "        parsed = None\n",
    "        parse_method = \"direct\"\n",
    "        \n",
    "        # Try direct JSON parsing first\n",
    "        try:\n",
    "            parsed = json.loads(raw_text)\n",
    "        except json.JSONDecodeError:\n",
    "            parse_method = \"fallback\"\n",
    "            # Fallback: Extract JSON from wrapped responses (e.g., markdown code blocks)\n",
    "            json_match = re.search(r'```(?:json)?\\s*([\\s\\S]*?)\\s*```', raw_text)\n",
    "            if json_match:\n",
    "                try:\n",
    "                    parsed = json.loads(json_match.group(1).strip())\n",
    "                except json.JSONDecodeError:\n",
    "                    pass\n",
    "            \n",
    "            # Second fallback: Find JSON object anywhere in the response\n",
    "            if parsed is None:\n",
    "                json_obj_match = re.search(r'\\{[^{}]*\"suggested_defeaters\"\\s*:\\s*\\[.*?\\]\\s*\\}', raw_text, re.DOTALL)\n",
    "                if json_obj_match:\n",
    "                    try:\n",
    "                        parsed = json.loads(json_obj_match.group(0))\n",
    "                    except json.JSONDecodeError:\n",
    "                        pass\n",
    "        \n",
    "        if parsed is None:\n",
    "            print(f\"[LLM Defeaters] WARNING: Could not parse JSON from response\")\n",
    "            print(f\"[LLM Defeaters] Raw response (first 200 chars): {raw_text[:200]}...\")\n",
    "            return []\n",
    "        \n",
    "        raw_defeaters = parsed.get(\"suggested_defeaters\", [])\n",
    "        if parse_method == \"fallback\":\n",
    "            print(f\"[LLM Defeaters] INFO: Used fallback JSON extraction\")\n",
    "        \n",
    "    except json.JSONDecodeError as e:\n",
    "        print(f\"[LLM Defeaters] WARNING: JSON parsing failed: {e}\")\n",
    "        print(f\"[LLM Defeaters] Response text (first 200 chars): {response.text[:200] if response.text else 'None'}...\")\n",
    "        return []\n",
    "    except Exception as e:\n",
    "        print(f\"[LLM Defeaters] ERROR: Gemini API call failed: {type(e).__name__}: {e}\")\n",
    "        return []\n",
    "    \n",
    "    # Process and validate defeaters\n",
    "    validated_defeaters = []\n",
    "    valid_step_ids = {f\"wc-step-{i}\" for i in range(1, 9)}\n",
    "    \n",
    "    for raw in raw_defeaters[:max_defeaters]:\n",
    "        step_id = raw.get(\"target_step_id\", \"\")\n",
    "        message = raw.get(\"message\", \"\")\n",
    "        severity = raw.get(\"severity\", 0.5)\n",
    "        \n",
    "        # Validate step ID\n",
    "        if step_id not in valid_step_ids:\n",
    "            print(f\"[LLM Defeaters] Skipping invalid step_id: {step_id}\")\n",
    "            continue\n",
    "        \n",
    "        # Validate message\n",
    "        if not message or len(message) < 10:\n",
    "            print(f\"[LLM Defeaters] Skipping empty/short message for {step_id}\")\n",
    "            continue\n",
    "        \n",
    "        # CRITICAL: Clamp severity to safe range [0.3, 0.7]\n",
    "        clamped_severity = max(0.3, min(0.7, float(severity)))\n",
    "        \n",
    "        # Build validated defeater dict\n",
    "        validated_defeaters.append({\n",
    "            \"target_step_id\": step_id,\n",
    "            \"kind\": \"evidential\",  # Always evidential for LLM suggestions\n",
    "            \"source_rule\": \"llm_pattern_analysis\",  # Always this source\n",
    "            \"message\": message,\n",
    "            \"severity\": clamped_severity,\n",
    "            \"reasoning\": raw.get(\"reasoning\", \"Pattern-based analysis\"),\n",
    "        })\n",
    "    \n",
    "    print(f\"[LLM Defeaters] Suggested {len(validated_defeaters)} evidential defeater(s)\")\n",
    "    return validated_defeaters\n",
    "\n",
    "\n",
    "def suggest_llm_defeaters(\n",
    "    tool_context,\n",
    "    max_defeaters: int = 5\n",
    ") -> List[Dict[str, Any]]:\n",
    "    \"\"\"Synchronous wrapper for suggest_llm_defeaters_async.\n",
    "    \n",
    "    Args:\n",
    "        tool_context: ADK tool context with state\n",
    "        max_defeaters: Maximum number of defeaters to return\n",
    "    \n",
    "    Returns:\n",
    "        List of validated Defeater-compatible dicts\n",
    "    \"\"\"\n",
    "    loop = asyncio.get_event_loop()\n",
    "    return loop.run_until_complete(\n",
    "        suggest_llm_defeaters_async(tool_context, max_defeaters)\n",
    "    )\n",
    "\n",
    "\n",
    "def apply_llm_defeaters(\n",
    "    tool_context,\n",
    "    llm_defeaters: List[Dict[str, Any]]\n",
    ") -> int:\n",
    "    \"\"\"Apply LLM-suggested defeaters to the active plan.\n",
    "    \n",
    "    Args:\n",
    "        tool_context: ADK tool context with state\n",
    "        llm_defeaters: List of defeater dicts from suggest_llm_defeaters\n",
    "    \n",
    "    Returns:\n",
    "        Number of defeaters successfully added\n",
    "    \"\"\"\n",
    "    state = tool_context.state\n",
    "    plan = state.get(\"active_plan\", [])\n",
    "    added_count = 0\n",
    "    \n",
    "    for defeater in llm_defeaters:\n",
    "        step_id = defeater.get(\"target_step_id\")\n",
    "        for step in plan:\n",
    "            if step.get(\"id\") == step_id:\n",
    "                added = add_defeater_to_step(\n",
    "                    step=step,\n",
    "                    kind=defeater.get(\"kind\", \"evidential\"),\n",
    "                    source_rule=defeater.get(\"source_rule\", \"llm_pattern_analysis\"),\n",
    "                    message=defeater.get(\"message\", \"\"),\n",
    "                    severity=defeater.get(\"severity\", 0.5),\n",
    "                )\n",
    "                if added:\n",
    "                    added_count += 1\n",
    "                break\n",
    "    \n",
    "    state[\"active_plan\"] = plan\n",
    "    return added_count\n",
    "\n",
    "\n",
    "print(\"LLM-based defeater suggestion tool loaded.\")\n",
    "print(\"- suggest_llm_defeaters(tool_context): Query Gemini for evidential defeaters\")\n",
    "print(\"- apply_llm_defeaters(tool_context, defeaters): Add LLM defeaters to plan\")\n",
    "print(\"- Severity clamped to [0.3, 0.7] - LLM cannot alone cause DEFEATED status\")"
   ]
  },

    "# ==========================================================================\n",
    "# Forward & Backward Reasoning Integration Demo\n",
    "# ==========================================================================\n",
    "# This demo shows the complete pipeline with forward reasoning, rule \n",
    "# evaluation, and backward analysis working together.\n",
    "\n",
    "print(\"=\" * 70)\n",
    "print(\"FORWARD & BACKWARD REASONING INTEGRATION DEMO\")\n",
    "print(\"=\" * 70)\n",
    "\n",
    "# Demo claim: Standard compensable claim with some missing evidence\n",
    "demo_intake = {\n",
    "    \"jurisdiction\": \"IL\",\n",
    "    \"employee_age\": 35,\n",
    "    \"occupation\": \"Warehouse Worker\",\n",
    "    \"date_of_injury\": \"2025-01-15\",\n",
    "    \"injury_description\": \"Back strain from lifting heavy boxes\",\n",
    "    \"work_related\": \"yes\",\n",
    "    \"date_reported\": \"2025-01-18\",\n",
    "    \"prior_injuries\": \"None\",\n",
    "    \"current_work_status\": \"Off work - temporary disability\",\n",
    "    \"requested_benefits\": \"TTD and medical treatment\",\n",
    "}\n",
    "\n",
    "print(\"\\n[STEP 1] INITIAL CLAIM DATA\")\n",
    "print(\"-\" * 70)\n",
    "for key, value in demo_intake.items():\n",
    "    print(f\"  {key}: {value}\")\n",
    "\n",
    "# Create context and seed state\n",
    "demo_context = MockToolContext()\n",
    "plan_result = seed_state_from_intake(demo_context, demo_intake)\n",
    "\n",
    "print(f\"\\n[STEP 2] STATE SEEDED\")\n",
    "print(\"-\" * 70)\n",
    "initial_beliefs = demo_context.state.get(\"beliefs\", [])\n",
    "print(f\"  Initial beliefs: {len(initial_beliefs)}\")\n",
    "print(f\"  Plan steps: {len(demo_context.state.get('active_plan', []))}\")\n",
    "\n",
    "# Show key initial beliefs\n",
    "print(\"\\n  Key Initial Beliefs:\")\n",
    "for b in initial_beliefs[:8]:\n",
    "    prop = b.get(\"proposition\", \"\") if isinstance(b, dict) else str(b)\n",
    "    print(f\"    - {prop}\")\n",
    "if len(initial_beliefs) > 8:\n",
    "    print(f\"    ... and {len(initial_beliefs) - 8} more\")\n",
    "\n",
    "# Run the integrated analysis\n",
    "print(f\"\\n[STEP 3] RUNNING INTEGRATED ANALYSIS\")\n",
    "print(\"-\" * 70)\n",
    "print(\"  Pipeline: Forward Reasoning → Rule Evaluation → Backward Analysis\")\n",
    "\n",
    "rule_result = run_workers_comp_analyst_once(demo_context, verbose=True)\n",
    "\n",
    "print(f\"\\n[STEP 4] ANALYSIS RESULTS\")\n",
    "print(\"-\" * 70)\n",
    "print(f\"  Forward beliefs added: {rule_result.get('forward_beliefs_added', 0)}\")\n",
    "print(f\"  Violations found: {rule_result.get('violations_found', 0)}\")\n",
    "print(f\"  Step statuses: {rule_result.get('step_statuses', {})}\")\n",
    "\n",
    "# Show forward-derived beliefs\n",
    "print(\"\\n  Forward-Derived Beliefs:\")\n",
    "beliefs = demo_context.state.get(\"beliefs\", [])\n",
    "forward_beliefs = [b for b in beliefs if isinstance(b, dict) and \n",
    "                   b.get(\"support\", \"\").startswith(\"Forward reason\")]\n",
    "for b in forward_beliefs[:5]:\n",
    "    prop = b.get(\"proposition\", \"\")\n",
    "    support = b.get(\"support\", \"\")[:50]\n",
    "    print(f\"    ✓ {prop}\")\n",
    "    print(f\"      (from: {support})\")\n",
    "\n",
    "if not forward_beliefs:\n",
    "    print(\"    (No forward-derived beliefs in this run)\")\n",
    "\n",
    "# Show step statuses with defeaters\n",
    "print(\"\\n[STEP 5] PLAN STATUS & DEFEATERS\")\n",
    "print(\"-\" * 70)\n",
    "for step in demo_context.state.get(\"active_plan\", []):\n",
    "    status = step.get(\"status\", \"UNKNOWN\")\n",
    "    symbol = STATUS_SYMBOLS.get(status, \"?\")\n",
    "    step_id = step.get(\"id\", \"?\")\n",
    "    desc = step.get(\"description\", \"\")[:50]\n",
    "    print(f\"  {symbol} {step_id}: {status}\")\n",
    "    \n",
    "    defeaters = step.get(\"defeaters\", [])\n",
    "    if defeaters:\n",
    "        for d in defeaters[:2]:\n",
    "            kind = d.get(\"kind\", \"unknown\") if isinstance(d, dict) else \"unknown\"\n",
    "            msg = d.get(\"message\", str(d))[:50] if isinstance(d, dict) else str(d)[:50]\n",
    "            print(f\"      └─ [{kind}] {msg}...\")\n",
    "\n",
    "# Show backward analysis\n",
    "print(\"\\n[STEP 6] BACKWARD ANALYSIS\")\n",
    "display_backward_analysis(demo_context.state)\n",
    "\n",
    "# Show summary\n",
    "print(\"\\n[STEP 7] SUMMARY\")\n",
    "print(\"-\" * 70)\n",
    "final_beliefs = len(demo_context.state.get(\"beliefs\", []))\n",
    "backward_summary = get_backward_analysis_summary(demo_context.state)\n",
    "\n",
    "print(f\"  Total beliefs: {len(initial_beliefs)} → {final_beliefs}\")\n",
    "print(f\"  Forward inferences: +{final_beliefs - len(initial_beliefs)}\")\n",
    "print(f\"  Goals analyzed: {backward_summary.get('goals_analyzed', 0)}\")\n",
    "print(f\"  Goals supported: {backward_summary.get('goals_supported', 0)}\")\n",
    "print(f\"  Goals pending: {backward_summary.get('goals_pending', 0)}\")\n",
    "\n",
    "# Show what's missing for approval\n",
    "missing_for_approval = backward_summary.get(\"missing_subgoals_by_goal\", {}).get(\"decision:approve_claim\", [])\n",
    "if missing_for_approval:\n",
    "    print(f\"\\n  Missing for approval ({len(missing_for_approval)}):\")\n",
    "    for sg in missing_for_approval[:5]:\n",
    "        print(f\"    ○ {sg}\")\n",
    "\n",
    "print(\"\\n\" + \"=\" * 70)\n",
    "print(\"Demo complete. Forward reasoning ran before rules, backward analysis after.\")\n",
    "print(\"=\" * 70)"
   ]

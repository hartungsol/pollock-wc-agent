   "source": [
    "# ---------- Workers' Compensation Display Utilities ----------\n",
    "# Enhanced to display structured Defeater objects and step status\n",
    "\n",
    "# Status display symbols/colors (using Unicode for terminal compatibility)\n",
    "STATUS_SYMBOLS = {\n",
    "    \"SUPPORTED\": \"✓\",\n",
    "    \"QUESTIONED\": \"?\",\n",
    "    \"DEFEATED\": \"✗\",\n",
    "}\n",
    "\n",
    "STATUS_LABELS = {\n",
    "    \"SUPPORTED\": \"[SUPPORTED]\",\n",
    "    \"QUESTIONED\": \"[QUESTIONED]\",\n",
    "    \"DEFEATED\": \"[DEFEATED]\",\n",
    "}\n",
    "\n",
    "DEFEATER_KIND_LABELS = {\n",
    "    \"undercutting\": \"UNDERCUT\",\n",
    "    \"rebutting\": \"REBUT\",\n",
    "    \"regulatory\": \"REGULATORY\",\n",
    "    \"evidential\": \"EVIDENCE\",\n",
    "}\n",
    "\n",
    "def format_defeater(defeater: Dict[str, Any]) -> str:\n",
    "    \"\"\"Format a single structured defeater for display.\n",
    "    \n",
    "    Args:\n",
    "        defeater: A defeater dict with id, kind, source_rule, message, severity.\n",
    "    \n",
    "    Returns:\n",
    "        Formatted defeater string for display.\n",
    "    \"\"\"\n",
    "    kind = defeater.get(\"kind\", \"unknown\")\n",
    "    kind_label = DEFEATER_KIND_LABELS.get(kind, kind.upper())\n",
    "    severity = defeater.get(\"severity\", 1.0)\n",
    "    source = defeater.get(\"source_rule\", \"unknown\")\n",
    "    message = defeater.get(\"message\", \"No message\")\n",
    "    \n",
    "    # Truncate message if too long\n",
    "    max_msg_len = 100\n",
    "    if len(message) > max_msg_len:\n",
    "        message = message[:max_msg_len] + \"...\"\n",
    "    \n",
    "    severity_bar = \"█\" * int(severity * 5) + \"░\" * (5 - int(severity * 5))\n",
    "    \n",
    "    return f\"[{kind_label}] {severity_bar} ({severity:.0%}) {message}\\n        Rule: {source}\"\n",
    "\n",
    "\n",
    "def format_plan_for_display(state: Dict[str, Any]) -> str:\n",
    "    \"\"\"Format the current plan state for human-readable display.\n",
    "    \n",
    "    Enhanced version that shows:\n",
    "    - Step status (SUPPORTED/QUESTIONED/DEFEATED)\n",
    "    - Structured defeaters with kind, severity, and source rule\n",
    "    \n",
    "    Args:\n",
    "        state: The agent state containing beliefs, goals, and plan.\n",
    "    \n",
    "    Returns:\n",
    "        Formatted string suitable for adjuster/supervisor review.\n",
    "    \"\"\"\n",
    "    output_lines = []\n",
    "    \n",
    "    # Header\n",
    "    output_lines.append(\"=\" * 70)\n",
    "    output_lines.append(\"WORKERS' COMPENSATION CLAIM EVALUATION PLAN\")\n",
    "    output_lines.append(\"=\" * 70)\n",
    "    \n",
    "    # Claim context\n",
    "    claim_context = state.get(\"claim_context\", {})\n",
    "    if claim_context:\n",
    "        output_lines.append(\"\\nCLAIM CONTEXT:\")\n",
    "        output_lines.append(f\"  Jurisdiction: {claim_context.get('jurisdiction', 'N/A')}\")\n",
    "        output_lines.append(f\"  Severity: {claim_context.get('severity_level', 'N/A')}\")\n",
    "        output_lines.append(f\"  Description: {claim_context.get('description', 'N/A')[:100]}\")\n",
    "    \n",
    "    # Goals\n",
    "    goals = state.get(\"goals\", [])\n",
    "    if goals:\n",
    "        output_lines.append(\"\\nGOAL:\")\n",
    "        for g in goals:\n",
    "            output_lines.append(f\"  {g.get('description', 'N/A')}\")\n",
    "    \n",
    "    # Plan summary statistics\n",
    "    plan = state.get(\"active_plan\", [])\n",
    "    status_counts = {\"SUPPORTED\": 0, \"QUESTIONED\": 0, \"DEFEATED\": 0}\n",
    "    for step in plan:\n",
    "        status = step.get(\"status\", \"SUPPORTED\")\n",
    "        status_counts[status] = status_counts.get(status, 0) + 1\n",
    "    \n",
    "    output_lines.append(\"\\n\" + \"-\" * 70)\n",
    "    output_lines.append(f\"PLAN SUMMARY: {len(plan)} steps\")\n",
    "    output_lines.append(f\"  {STATUS_SYMBOLS['SUPPORTED']} Supported: {status_counts['SUPPORTED']}\")\n",
    "    output_lines.append(f\"  {STATUS_SYMBOLS['QUESTIONED']} Questioned: {status_counts['QUESTIONED']}\")\n",
    "    output_lines.append(f\"  {STATUS_SYMBOLS['DEFEATED']} Defeated: {status_counts['DEFEATED']}\")\n",
    "    output_lines.append(\"-\" * 70)\n",
    "    output_lines.append(\"PLAN STEPS:\")\n",
    "    output_lines.append(\"-\" * 70)\n",
    "    \n",
    "    for step in plan:\n",
    "        step_id = step.get(\"id\", \"?\")\n",
    "        description = step.get(\"description\", \"No description\")\n",
    "        defeaters = step.get(\"defeaters\", [])\n",
    "        status = step.get(\"status\", \"SUPPORTED\")\n",
    "        \n",
    "        # Status indicator with symbol\n",
    "        status_label = STATUS_LABELS.get(status, \"[UNKNOWN]\")\n",
    "        status_symbol = STATUS_SYMBOLS.get(status, \"?\")\n",
    "        \n",
    "        output_lines.append(f\"\\n{status_symbol} {step_id} {status_label}\")\n",
    "        output_lines.append(f\"  {description[:100]}\")\n",
    "        \n",
    "        # Preconditions\n",
    "        preconditions = step.get(\"preconditions\", [])\n",
    "        if preconditions:\n",
    "            output_lines.append(f\"  Requires: {', '.join(preconditions[:3])}\")\n",
    "            if len(preconditions) > 3:\n",
    "                output_lines.append(f\"            ... and {len(preconditions)-3} more\")\n",
    "        \n",
    "        # Required documents\n",
    "        docs = step.get(\"required_documents\", [])\n",
    "        if docs:\n",
    "            output_lines.append(f\"  Documents: {', '.join(docs)}\")\n",
    "        \n",
    "        # Defeaters (enhanced with structure)\n",
    "        if defeaters:\n",
    "            output_lines.append(f\"  DEFEATERS ({len(defeaters)}):\")\n",
    "            for d in defeaters:\n",
    "                # Handle both old string format and new structured format\n",
    "                if isinstance(d, str):\n",
    "                    # Legacy string format\n",
    "                    wrapped = d[:120] + \"...\" if len(d) > 120 else d\n",
    "                    output_lines.append(f\"    - {wrapped}\")\n",
    "                elif isinstance(d, dict):\n",
    "                    # New structured format\n",
    "                    formatted = format_defeater(d)\n",
    "                    for line in formatted.split(\"\\n\"):\n",
    "                        output_lines.append(f\"    - {line}\")\n",
    "                else:\n",
    "                    output_lines.append(f\"    - {str(d)}\")\n",
    "    \n",
    "    return \"\\n\".join(output_lines)\n",
    "\n",
    "\n",
    "def format_beliefs_for_display(beliefs: List[Dict[str, Any]]) -> str:\n",
    "    \"\"\"Format beliefs for human-readable display.\n",
    "    \n",
    "    Args:\n",
    "        beliefs: List of belief dictionaries.\n",
    "    \n",
    "    Returns:\n",
    "        Formatted string showing all current beliefs.\n",
    "    \"\"\"\n",
    "    output_lines = []\n",
    "    output_lines.append(\"-\" * 70)\n",
    "    output_lines.append(\"CURRENT BELIEFS (EVIDENCE BASE):\")\n",
    "    output_lines.append(\"-\" * 70)\n",
    "    \n",
    "    # Group beliefs by category\n",
    "    categories: Dict[str, List[Dict[str, Any]]] = {}\n",
    "    for b in beliefs:\n",
    "        prop = b.get(\"proposition\", \"\")\n",
    "        if \":\" in prop:\n",
    "            category = prop.split(\":\")[0]\n",
    "        else:\n",
    "            category = \"other\"\n",
    "        categories.setdefault(category, []).append(b)\n",
    "    \n",
    "    for category in sorted(categories.keys()):\n",
    "        output_lines.append(f\"\\n  [{category.upper()}]\")\n",
    "        for b in categories[category]:\n",
    "            prop = b.get(\"proposition\", \"?\")\n",
    "            strength = b.get(\"strength\", 0)\n",
    "            support = b.get(\"support\", \"\")[:50]\n",
    "            output_lines.append(f\"    - {prop}\")\n",
    "            if support:\n",
    "                output_lines.append(f\"      (Source: {support}, Confidence: {strength:.0%})\")\n",
    "    \n",
    "    return \"\\n\".join(output_lines)\n",
    "\n",
    "\n",
    "def format_resolution_recommendation(state: Dict[str, Any]) -> str:\n",
    "    \"\"\"Generate a resolution recommendation based on current state.\n",
    "    \n",
    "    Enhanced to consider structured defeaters and step statuses.\n",
    "    \n",
    "    Args:\n",
    "        state: The agent state.\n",
    "    \n",
    "    Returns:\n",
    "        Formatted recommendation string.\n",
    "    \"\"\"\n",
    "    plan = state.get(\"active_plan\", [])\n",
    "    beliefs = state.get(\"beliefs\", [])\n",
    "    \n",
    "    output_lines = []\n",
    "    output_lines.append(\"\\n\" + \"=\" * 70)\n",
    "    output_lines.append(\"RESOLUTION RECOMMENDATION\")\n",
    "    output_lines.append(\"=\" * 70)\n",
    "    \n",
    "    # Analyze step statuses\n",
    "    defeated_steps = [s for s in plan if s.get(\"status\") == \"DEFEATED\"]\n",
    "    questioned_steps = [s for s in plan if s.get(\"status\") == \"QUESTIONED\"]\n",
    "    supported_steps = [s for s in plan if s.get(\"status\") == \"SUPPORTED\"]\n",
    "    \n",
    "    # For legacy compatibility, also check for defeaters directly\n",
    "    blocked_steps = [s for s in plan if s.get(\"defeaters\")]\n",
    "    critical_blocked = [s for s in blocked_steps if s.get(\"id\") in [\"wc-step-4\", \"wc-step-8\"]]\n",
    "    \n",
    "    # Collect all defeaters for analysis\n",
    "    all_defeaters = []\n",
    "    for step in plan:\n",
    "        for d in step.get(\"defeaters\", []):\n",
    "            if isinstance(d, dict):\n",
    "                all_defeaters.append(d)\n",
    "            else:\n",
    "                # Legacy string format\n",
    "                all_defeaters.append({\"message\": d, \"kind\": \"unknown\", \"severity\": 1.0})\n",
    "    \n",
    "    # Analyze defeaters by kind\n",
    "    defeater_kinds = {}\n",
    "    max_severity = 0.0\n",
    "    for d in all_defeaters:\n",
    "        kind = d.get(\"kind\", \"unknown\")\n",
    "        severity = d.get(\"severity\", 1.0)\n",
    "        defeater_kinds[kind] = defeater_kinds.get(kind, 0) + 1\n",
    "        max_severity = max(max_severity, severity)\n",
    "    \n",
    "    # Analyze defeaters for specific flags\n",
    "    fraud_flags = any(\n",
    "        d.get(\"source_rule\", \"\").lower().find(\"fraud\") >= 0 or\n",
    "        d.get(\"message\", \"\").lower().find(\"fraud\") >= 0\n",
    "        for d in all_defeaters\n",
    "    )\n",
    "    eligibility_issues = any(\n",
    "        d.get(\"source_rule\", \"\").lower().find(\"eligib\") >= 0 or\n",
    "        d.get(\"message\", \"\").lower().find(\"contractor\") >= 0\n",
    "        for d in all_defeaters\n",
    "    )\n",
    "    causation_issues = any(\n",
    "        d.get(\"kind\") == \"undercutting\" or\n",
    "        d.get(\"message\", \"\").lower().find(\"course of employment\") >= 0\n",
    "        for d in all_defeaters\n",
    "    )\n",
    "    late_reporting = any(\n",
    "        d.get(\"source_rule\", \"\").lower().find(\"late\") >= 0 or\n",
    "        d.get(\"message\", \"\").lower().find(\"deadline\") >= 0\n",
    "        for d in all_defeaters\n",
    "    )\n",
    "    \n",
    "    # Determine recommendation based on enhanced analysis\n",
    "    if defeated_steps:\n",
    "        # Critical: We have DEFEATED steps\n",
    "        if fraud_flags:\n",
    "            recommendation = \"ESCALATE TO SIU\"\n",
    "            rationale = \"Fraud indicators require Special Investigations Unit review before any benefit determination.\"\n",
    "        elif eligibility_issues:\n",
    "            recommendation = \"DENY - COVERAGE EXCLUSION\"\n",
    "            rationale = \"Employment classification appears to exclude claimant from workers' compensation coverage.\"\n",
    "        elif causation_issues:\n",
    "            recommendation = \"ESCALATE FOR INVESTIGATION\"\n",
    "            rationale = \"Work-relatedness of injury requires additional investigation before compensability can be determined.\"\n",
    "        else:\n",
    "            recommendation = \"ESCALATE - CRITICAL DEFEATERS\"\n",
    "            rationale = f\"{len(defeated_steps)} step(s) have high-severity defeaters that block claim progression.\"\n",
    "    elif questioned_steps:\n",
    "        # Warning: We have QUESTIONED steps\n",
    "        if late_reporting:\n",
    "            recommendation = \"CONDITIONAL - LATE NOTICE REVIEW\"\n",
    "            rationale = f\"Claim has {len(questioned_steps)} questioned step(s). Late reporting may affect benefits. Review excuse for delay.\"\n",
    "        else:\n",
    "            recommendation = \"PENDING - QUESTIONS TO RESOLVE\"\n",
    "            rationale = f\"{len(questioned_steps)} step(s) have moderate-severity defeaters requiring adjuster review.\"\n",
    "    elif len(blocked_steps) > 0:\n",
    "        recommendation = \"PENDING - DEFEATERS REQUIRE RESOLUTION\"\n",
    "        rationale = f\"{len(blocked_steps)} step(s) have unresolved issues that must be addressed before proceeding.\"\n",
    "    else:\n",
    "        recommendation = \"PROCEED TO APPROVAL REVIEW\"\n",
    "        rationale = \"No blocking defeaters identified. Claim may proceed to benefit calculation and approval review.\"\n",
    "    \n",
    "    output_lines.append(f\"\\n  Recommendation: {recommendation}\")\n",
    "    output_lines.append(f\"\\n  Rationale: {rationale}\")\n",
    "    \n",
    "    # Status summary\n",
    "    output_lines.append(\"\\n  Step Status Summary:\")\n",
    "    output_lines.append(f\"    {STATUS_SYMBOLS['SUPPORTED']} Supported: {len(supported_steps)}\")\n",
    "    output_lines.append(f\"    {STATUS_SYMBOLS['QUESTIONED']} Questioned: {len(questioned_steps)}\")\n",
    "    output_lines.append(f\"    {STATUS_SYMBOLS['DEFEATED']} Defeated: {len(defeated_steps)}\")\n",
    "    \n",
    "    # Defeater kind breakdown\n",
    "    if defeater_kinds:\n",
    "        output_lines.append(\"\\n  Defeater Analysis:\")\n",
    "        for kind, count in sorted(defeater_kinds.items()):\n",
    "            label = DEFEATER_KIND_LABELS.get(kind, kind.upper())\n",
    "            output_lines.append(f\"    {label}: {count}\")\n",
    "        output_lines.append(f\"    Maximum Severity: {max_severity:.0%}\")\n",
    "    \n",
    "    if defeated_steps:\n",
    "        output_lines.append(f\"\\n  Defeated Steps:\")\n",
    "        for step in defeated_steps:\n",
    "            step_id = step.get(\"id\", \"?\")\n",
    "            num_defeaters = len(step.get(\"defeaters\", []))\n",
    "            output_lines.append(f\"    - {step_id}: {num_defeaters} defeater(s)\")\n",
    "    \n",
    "    output_lines.append(\"\\n\" + \"-\" * 70)\n",
    "    output_lines.append(\"This recommendation is for decision-support purposes only.\")\n",
    "    output_lines.append(\"Final determination requires adjuster review and supervisor approval.\")\n",
    "    output_lines.append(\"-\" * 70)\n",
    "    \n",
    "    return \"\\n\".join(output_lines)\n",
    "\n",
    "\n",
    "def format_defeaters_summary(plan: List[Dict[str, Any]]) -> str:\n",
    "    \"\"\"Generate a summary of all defeaters across the plan.\n",
    "    \n",
    "    Args:\n",
    "        plan: The active plan list.\n",
    "    \n",
    "    Returns:\n",
    "        Formatted summary of all defeaters.\n",
    "    \"\"\"\n",
    "    output_lines = []\n",
    "    output_lines.append(\"-\" * 70)\n",
    "    output_lines.append(\"DEFEATER SUMMARY\")\n",
    "    output_lines.append(\"-\" * 70)\n",
    "    \n",
    "    all_defeaters = []\n",
    "    step_defeater_map = {}\n",
    "    \n",
    "    for step in plan:\n",
    "        step_id = step.get(\"id\", \"?\")\n",
    "        defeaters = step.get(\"defeaters\", [])\n",
    "        if defeaters:\n",
    "            step_defeater_map[step_id] = defeaters\n",
    "            for d in defeaters:\n",
    "                if isinstance(d, dict):\n",
    "                    all_defeaters.append({**d, \"step_id\": step_id})\n",
    "    \n",
    "    if not all_defeaters:\n",
    "        output_lines.append(\"\\n  No defeaters found. All steps are supported.\")\n",
    "        return \"\\n\".join(output_lines)\n",
    "    \n",
    "    # Group by kind\n",
    "    by_kind = {}\n",
    "    for d in all_defeaters:\n",
    "        kind = d.get(\"kind\", \"unknown\")\n",
    "        by_kind.setdefault(kind, []).append(d)\n",
    "    \n",
    "    output_lines.append(f\"\\n  Total defeaters: {len(all_defeaters)}\")\n",
    "    output_lines.append(f\"  Affected steps: {len(step_defeater_map)}\")\n",
    "    \n",
    "    for kind in [\"rebutting\", \"undercutting\", \"regulatory\", \"evidential\", \"unknown\"]:\n",
    "        if kind in by_kind:\n",
    "            defeaters = by_kind[kind]\n",
    "            label = DEFEATER_KIND_LABELS.get(kind, kind.upper())\n",
    "            output_lines.append(f\"\\n  [{label}] ({len(defeaters)} total)\")\n",
    "            for d in defeaters[:3]:  # Show first 3 of each kind\n",
    "                step_id = d.get(\"step_id\", \"?\")\n",
    "                severity = d.get(\"severity\", 1.0)\n",
    "                source = d.get(\"source_rule\", \"unknown\")\n",
    "                message = d.get(\"message\", \"\")[:60]\n",
    "                output_lines.append(f\"    {step_id} ({severity:.0%}) {source}: {message}...\")\n",
    "            if len(defeaters) > 3:\n",
    "                output_lines.append(f\"    ... and {len(defeaters)-3} more\")\n",
    "    \n",
    "    return \"\\n\".join(output_lines)\n",
    "\n",
    "\n",
    "print(\"Enhanced display utilities loaded.\")\n",
    "print(\"- format_plan_for_display: Shows step status and structured defeaters\")\n",
    "print(\"- format_resolution_recommendation: Analyzes defeater kinds and severity\")\n",
    "print(\"- format_defeaters_summary: Groups defeaters by kind\")"
   ]

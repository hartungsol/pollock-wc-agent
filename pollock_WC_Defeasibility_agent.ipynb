    "# ==========================================================================\n",
    "# SECTION: Google Search Integration for Regulatory Lookup\n",
    "# ==========================================================================\n",
    "# This section provides:\n",
    "#   1. A dedicated regulation_search_agent using google_search\n",
    "#   2. AgentTool wrapper for use with orchestrators  \n",
    "#   3. Helper functions to fetch and parse regulation JSON\n",
    "#   4. Conversion of search results to Pollock Beliefs\n",
    "\n",
    "import asyncio\n",
    "import nest_asyncio\n",
    "nest_asyncio.apply()\n",
    "\n",
    "from google.adk.agents import Agent\n",
    "from google.adk.planners import BuiltInPlanner\n",
    "from google.adk.tools import google_search\n",
    "from google.adk.tools.agent_tool import AgentTool\n",
    "import json\n",
    "import re\n",
    "\n",
    "# ---------- Regulation Search Agent (Standalone with Google Search) ----------\n",
    "\n",
    "REGULATION_SEARCH_INSTRUCTION = \"\"\"\n",
    "You are a research assistant specializing in regulations and domain-specific rules.\n",
    "Given a jurisdiction and a short description of the case, use google_search to find:\n",
    "- relevant thresholds, deadlines, and mandatory requirements,\n",
    "- any key numeric parameters (days, percentages, limits).\n",
    "\n",
    "IMPORTANT: Always return a single JSON object in your final answer with these fields:\n",
    "{\n",
    "    \"jurisdiction\": \"<state code>\",\n",
    "    \"waiting_period_days\": <number>,\n",
    "    \"notice_deadline_days\": <number>,\n",
    "    \"ttd_rate_percent\": <number>,\n",
    "    \"fraud_threshold\": <number or null>,\n",
    "    \"compensability_standard\": \"<text description>\",\n",
    "    \"requires_mandatory_review_for_severity\": [\"<severity levels>\"],\n",
    "    \"mandatory_forms\": [\"<form names>\"],\n",
    "    \"statute_of_limitations_years\": <number>,\n",
    "    \"source_url\": \"<primary source URL>\"\n",
    "}\n",
    "\n",
    "If a value cannot be determined, use null.\n",
    "Prefer official .gov sources when available.\n",
    "Always cite the source of information.\n",
    "\"\"\"\n",
    "\n",
    "regulation_search_agent = Agent(\n",
    "    model=\"gemini-2.5-flash\",\n",
    "    name=\"regulation_search\",\n",
    "    description=\"Searches for jurisdiction-specific regulatory information using Google Search.\",\n",
    "    instruction=REGULATION_SEARCH_INSTRUCTION,\n",
    "    tools=[google_search],\n",
    ")\n",
    "\n",
    "# Create AgentTool wrapper for use with other agents\n",
    "regulation_search_tool = AgentTool(regulation_search_agent)\n",
    "\n",
    "# Create standalone runner for the research agent\n",
    "research_session_service = InMemorySessionService()\n",
    "research_runner = Runner(\n",
    "    agent=regulation_search_agent,\n",
    "    app_name=\"regulation_research\",\n",
    "    session_service=research_session_service,\n",
    ")\n",
    "\n",
    "# Initialize research session\n",
    "async def _init_research_session():\n",
    "    await research_session_service.create_session(\n",
    "        app_name=\"regulation_research\",\n",
    "        user_id=\"researcher\",\n",
    "        session_id=\"research_session\",\n",
    "        state={},\n",
    "    )\n",
    "\n",
    "asyncio.get_event_loop().run_until_complete(_init_research_session())\n",
    "\n",
    "\n",
    "# ---------- Helper Functions for Fetching and Parsing Regulation Info ----------\n",
    "\n",
    "async def fetch_regulation_info_async(jurisdiction: str, context_text: str) -> dict:\n",
    "    \"\"\"\n",
    "    Use the regulation_search_agent to retrieve structured regulation info\n",
    "    for a given jurisdiction and case context. Return a parsed JSON dict.\n",
    "    \n",
    "    Args:\n",
    "        jurisdiction: State code (e.g., \"IL\", \"CA\", \"TX\")\n",
    "        context_text: Brief description of the case context\n",
    "        \n",
    "    Returns:\n",
    "        Parsed dictionary of regulatory parameters, or dict with 'error' key on failure.\n",
    "    \"\"\"\n",
    "    # Build a concise prompt\n",
    "    prompt = f\"\"\"\n",
    "    Research workers' compensation regulations for {jurisdiction}.\n",
    "    \n",
    "    Case context: {context_text}\n",
    "    \n",
    "    Find and return the following information as a JSON object:\n",
    "    - waiting_period_days\n",
    "    - notice_deadline_days  \n",
    "    - ttd_rate_percent\n",
    "    - fraud_threshold\n",
    "    - compensability_standard\n",
    "    - requires_mandatory_review_for_severity\n",
    "    - mandatory_forms\n",
    "    - statute_of_limitations_years\n",
    "    - source_url\n",
    "    \n",
    "    Return ONLY the JSON object in your final response.\n",
    "    \"\"\"\n",
    "    \n",
    "    content = types.Content(\n",
    "        role=\"user\",\n",
    "        parts=[types.Part(text=prompt)],\n",
    "    )\n",
    "    \n",
    "    response_parts = []\n",
    "    async for event in research_runner.run_async(\n",
    "        user_id=\"researcher\",\n",
    "        session_id=\"research_session\",\n",
    "        new_message=content,\n",
    "    ):\n",
    "        if hasattr(event, 'content') and event.content:\n",
    "            for part in event.content.parts:\n",
    "                if hasattr(part, 'text') and part.text:\n",
    "                    response_parts.append(part.text)\n",
    "    \n",
    "    full_response = \"\\n\".join(response_parts)\n",
    "    \n",
    "    # Parse JSON from response\n",
    "    try:\n",
    "        # Try to find JSON object in the response\n",
    "        json_match = re.search(r'\\{[^{}]*(?:\\{[^{}]*\\}[^{}]*)*\\}', full_response, re.DOTALL)\n",
    "        if json_match:\n",
    "            json_str = json_match.group(0)\n",
    "            parsed = json.loads(json_str)\n",
    "            parsed[\"jurisdiction\"] = jurisdiction.upper()\n",
    "            parsed[\"_raw_response\"] = full_response\n",
    "            parsed[\"_source\"] = \"google_search\"\n",
    "            return parsed\n",
    "        else:\n",
    "            # Try parsing entire response as JSON\n",
    "            parsed = json.loads(full_response)\n",
    "            parsed[\"jurisdiction\"] = jurisdiction.upper()\n",
    "            parsed[\"_raw_response\"] = full_response\n",
    "            parsed[\"_source\"] = \"google_search\"\n",
    "            return parsed\n",
    "    except json.JSONDecodeError as e:\n",
    "        # Return partial info with error flag\n",
    "        return {\n",
    "            \"jurisdiction\": jurisdiction.upper(),\n",
    "            \"error\": f\"JSON parsing failed: {str(e)}\",\n",
    "            \"_raw_response\": full_response,\n",
    "            \"_source\": \"google_search\",\n",
    "        }\n",
    "\n",
    "\n",
    "def fetch_regulation_info(jurisdiction: str, context_text: str) -> dict:\n",
    "    \"\"\"\n",
    "    Synchronous wrapper for fetch_regulation_info_async.\n",
    "    \n",
    "    Args:\n",
    "        jurisdiction: State code (e.g., \"IL\", \"CA\", \"TX\")\n",
    "        context_text: Brief description of the case context\n",
    "        \n",
    "    Returns:\n",
    "        Parsed dictionary of regulatory parameters.\n",
    "    \"\"\"\n",
    "    loop = asyncio.get_event_loop()\n",
    "    return loop.run_until_complete(fetch_regulation_info_async(jurisdiction, context_text))\n",
    "\n",
    "\n",
    "# ---------- Convert Search Results to Beliefs ----------\n",
    "\n",
    "def add_regulation_beliefs_from_search(ctx, reg_info: dict, support: str = \"google_search\") -> List[Dict[str, Any]]:\n",
    "    \"\"\"\n",
    "    Convert regulation info from fetch_regulation_info into Belief objects\n",
    "    and add them to the context state.\n",
    "    \n",
    "    Args:\n",
    "        ctx: Context object with state dictionary (MockToolContext or similar)\n",
    "        reg_info: Dictionary returned by fetch_regulation_info\n",
    "        support: Source attribution for the beliefs\n",
    "        \n",
    "    Returns:\n",
    "        List of beliefs that were added.\n",
    "    \"\"\"\n",
    "    beliefs_to_add = []\n",
    "    jurisdiction = reg_info.get(\"jurisdiction\", \"UNKNOWN\")\n",
    "    \n",
    "    # Waiting period\n",
    "    if \"waiting_period_days\" in reg_info and reg_info[\"waiting_period_days\"] is not None:\n",
    "        beliefs_to_add.append(Belief(\n",
    "            id=f\"search-{jurisdiction}-wait\",\n",
    "            proposition=f\"jurisdiction:waiting_period_days={reg_info['waiting_period_days']}\",\n",
    "            support=f\"{support} for {jurisdiction}\",\n",
    "            strength=0.85,\n",
    "        ))\n",
    "    \n",
    "    # Notice deadline\n",
    "    if \"notice_deadline_days\" in reg_info and reg_info[\"notice_deadline_days\"] is not None:\n",
    "        beliefs_to_add.append(Belief(\n",
    "            id=f\"search-{jurisdiction}-notice\",\n",
    "            proposition=f\"jurisdiction:notice_deadline_days={reg_info['notice_deadline_days']}\",\n",
    "            support=f\"{support} for {jurisdiction}\",\n",
    "            strength=0.85,\n",
    "        ))\n",
    "    \n",
    "    # TTD rate\n",
    "    if \"ttd_rate_percent\" in reg_info and reg_info[\"ttd_rate_percent\"] is not None:\n",
    "        beliefs_to_add.append(Belief(\n",
    "            id=f\"search-{jurisdiction}-ttd\",\n",
    "            proposition=f\"jurisdiction:ttd_rate_percent={reg_info['ttd_rate_percent']}\",\n",
    "            support=f\"{support} for {jurisdiction}\",\n",
    "            strength=0.85,\n",
    "        ))\n",
    "    \n",
    "    # Fraud threshold\n",
    "    if \"fraud_threshold\" in reg_info and reg_info[\"fraud_threshold\"] is not None:\n",
    "        beliefs_to_add.append(Belief(\n",
    "            id=f\"search-{jurisdiction}-fraud\",\n",
    "            proposition=f\"jurisdiction:fraud_threshold={reg_info['fraud_threshold']}\",\n",
    "            support=f\"{support} for {jurisdiction}\",\n",
    "            strength=0.85,\n",
    "        ))\n",
    "    \n",
    "    # Compensability standard\n",
    "    if \"compensability_standard\" in reg_info and reg_info[\"compensability_standard\"]:\n",
    "        beliefs_to_add.append(Belief(\n",
    "            id=f\"search-{jurisdiction}-comp-std\",\n",
    "            proposition=f\"jurisdiction:compensability_standard={reg_info['compensability_standard']}\",\n",
    "            support=f\"{support} for {jurisdiction}\",\n",
    "            strength=0.85,\n",
    "        ))\n",
    "    \n",
    "    # Mandatory review severities\n",
    "    if \"requires_mandatory_review_for_severity\" in reg_info:\n",
    "        severities = reg_info[\"requires_mandatory_review_for_severity\"]\n",
    "        if isinstance(severities, list) and severities:\n",
    "            beliefs_to_add.append(Belief(\n",
    "                id=f\"search-{jurisdiction}-review\",\n",
    "                proposition=f\"jurisdiction:requires_mandatory_review_for={','.join(severities)}\",\n",
    "                support=f\"{support} for {jurisdiction}\",\n",
    "                strength=0.85,\n",
    "            ))\n",
    "    \n",
    "    # Mandatory forms\n",
    "    if \"mandatory_forms\" in reg_info:\n",
    "        forms = reg_info[\"mandatory_forms\"]\n",
    "        if isinstance(forms, list) and forms:\n",
    "            beliefs_to_add.append(Belief(\n",
    "                id=f\"search-{jurisdiction}-forms\",\n",
    "                proposition=f\"jurisdiction:mandatory_forms={','.join(forms)}\",\n",
    "                support=f\"{support} for {jurisdiction}\",\n",
    "                strength=0.85,\n",
    "            ))\n",
    "    \n",
    "    # Statute of limitations\n",
    "    if \"statute_of_limitations_years\" in reg_info and reg_info[\"statute_of_limitations_years\"] is not None:\n",
    "        beliefs_to_add.append(Belief(\n",
    "            id=f\"search-{jurisdiction}-sol\",\n",
    "            proposition=f\"jurisdiction:statute_of_limitations_years={reg_info['statute_of_limitations_years']}\",\n",
    "            support=f\"{support} for {jurisdiction}\",\n",
    "            strength=0.85,\n",
    "        ))\n",
    "    \n",
    "    # Add source URL as belief for auditability\n",
    "    if \"source_url\" in reg_info and reg_info[\"source_url\"]:\n",
    "        beliefs_to_add.append(Belief(\n",
    "            id=f\"search-{jurisdiction}-source\",\n",
    "            proposition=f\"jurisdiction:source_url={reg_info['source_url']}\",\n",
    "            support=f\"{support} for {jurisdiction}\",\n",
    "            strength=1.0,\n",
    "        ))\n",
    "    \n",
    "    # Add beliefs to context state\n",
    "    if \"beliefs\" not in ctx.state:\n",
    "        ctx.state[\"beliefs\"] = []\n",
    "    \n",
    "    added_beliefs = []\n",
    "    for b in beliefs_to_add:\n",
    "        belief_dict = b.model_dump()\n",
    "        ctx.state[\"beliefs\"].append(belief_dict)\n",
    "        added_beliefs.append(belief_dict)\n",
    "    \n",
    "    return added_beliefs\n",
    "\n",
    "\n",
    "# ---------- Legacy wrapper for backward compatibility ----------\n",
    "\n",
    "def search_wc_regulations(query: str) -> str:\n",
    "    \"\"\"\n",
    "    Search for workers' compensation regulations using Google Search.\n",
    "    Returns raw text response (use fetch_regulation_info for structured data).\n",

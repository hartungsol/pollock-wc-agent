    "# ==========================================================================\n",
    "# Regression Test Harness\n",
    "# ==========================================================================\n",
    "# Minimal tests to verify core defeasibility behavior across scenarios.\n",
    "# ==========================================================================\n",
    "\n",
    "from typing import Dict, List, Any, Tuple\n",
    "from datetime import date, timedelta\n",
    "\n",
    "# ---------- Test Scenario Definitions ----------\n",
    "\n",
    "TEST_SCENARIOS: List[Dict[str, Any]] = [\n",
    "    {\n",
    "        \"name\": \"Standard Compensable Claim\",\n",
    "        \"description\": \"Full-time employee, timely reporting, no red flags\",\n",
    "        \"intake\": {\n",
    "            \"jurisdiction\": \"CA\",\n",
    "            \"injury_description\": \"Back strain lifting boxes\",\n",
    "            \"date_of_injury\": \"2025-01-15\",\n",
    "            \"date_reported\": \"2025-01-17\",  # 2 days - within deadline\n",
    "            \"work_related\": \"yes\",\n",
    "            \"occupation\": \"Warehouse Worker\",\n",
    "            \"current_work_status\": \"off work\",\n",
    "        },\n",
    "        \"beliefs\": [\n",
    "            {\"proposition\": \"employment:status=full_time\", \"strength\": 1.0},\n",
    "            {\"proposition\": \"employment:on_duty_at_time=true\", \"strength\": 0.95},\n",
    "            {\"proposition\": \"risk:fraud_score=0.25\", \"strength\": 0.9},\n",
    "            {\"proposition\": \"jurisdiction:notice_deadline_days=30\", \"strength\": 1.0},\n",
    "            {\"proposition\": \"jurisdiction:fraud_threshold=0.70\", \"strength\": 1.0},\n",
    "        ],\n",
    "        \"expected\": {\n",
    "            \"has_defeated_steps\": False,\n",
    "            \"has_questioned_steps\": False,\n",
    "            \"step_status_contains\": {\"wc-step-1\": \"SUPPORTED\"},\n",
    "        },\n",
    "    },\n",
    "    {\n",
    "        \"name\": \"Late Reporting\",\n",
    "        \"description\": \"Notice filed 45 days after injury (past 30-day deadline)\",\n",
    "        \"intake\": {\n",
    "            \"jurisdiction\": \"CA\",\n",
    "            \"injury_description\": \"Shoulder strain\",\n",
    "            \"date_of_injury\": \"2025-01-01\",\n",
    "            \"date_reported\": \"2025-02-15\",  # 45 days late\n",
    "            \"work_related\": \"yes\",\n",
    "            \"occupation\": \"Office Worker\",\n",
    "            \"current_work_status\": \"light duty\",\n",
    "        },\n",
    "        \"beliefs\": [\n",
    "            {\"proposition\": \"employment:status=full_time\", \"strength\": 1.0},\n",
    "            {\"proposition\": \"employment:on_duty_at_time=true\", \"strength\": 0.95},\n",
    "            {\"proposition\": \"risk:fraud_score=0.30\", \"strength\": 0.9},\n",
    "            {\"proposition\": \"jurisdiction:notice_deadline_days=30\", \"strength\": 1.0},\n",
    "        ],\n",
    "        \"expected\": {\n",
    "            \"has_defeated_steps\": True,  # Late reporting should cause DEFEATED\n",
    "            \"has_questioned_steps\": False,  # Or at minimum QUESTIONED\n",
    "            \"min_defeaters\": 1,\n",
    "        },\n",
    "    },\n",
    "    {\n",
    "        \"name\": \"High Fraud Score\",\n",
    "        \"description\": \"Fraud score exceeds jurisdiction threshold\",\n",
    "        \"intake\": {\n",
    "            \"jurisdiction\": \"IL\",\n",
    "            \"injury_description\": \"Back injury\",\n",
    "            \"date_of_injury\": \"2025-01-10\",\n",
    "            \"date_reported\": \"2025-01-12\",\n",
    "            \"work_related\": \"yes\",\n",
    "            \"occupation\": \"Driver\",\n",
    "            \"current_work_status\": \"off work\",\n",
    "        },\n",
    "        \"beliefs\": [\n",
    "            {\"proposition\": \"employment:status=full_time\", \"strength\": 1.0},\n",
    "            {\"proposition\": \"employment:on_duty_at_time=true\", \"strength\": 0.95},\n",
    "            {\"proposition\": \"risk:fraud_score=0.85\", \"strength\": 0.9},  # High fraud score\n",
    "            {\"proposition\": \"jurisdiction:notice_deadline_days=45\", \"strength\": 1.0},\n",
    "            {\"proposition\": \"jurisdiction:fraud_threshold=0.70\", \"strength\": 1.0},\n",
    "        ],\n",
    "        \"expected\": {\n",
    "            \"has_defeated_steps\": True,  # Fraud should trigger DEFEATED\n",

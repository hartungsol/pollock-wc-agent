    "    {\n",
    "        \"name\": \"Off-Duty Injury (Course of Employment)\",\n",
    "        \"description\": \"Injury occurred while off duty - not compensable\",\n",
    "        \"intake\": {\n",
    "            \"jurisdiction\": \"NY\",\n",
    "            \"injury_description\": \"Knee injury playing basketball at lunch\",\n",
    "            \"date_of_injury\": \"2025-01-25\",\n",
    "            \"date_reported\": \"2025-01-26\",\n",
    "            \"work_related\": \"no\",\n",
    "            \"occupation\": \"Accountant\",\n",
    "            \"current_work_status\": \"working\",\n",
    "        },\n",
    "        \"beliefs\": [\n",
    "            {\"proposition\": \"employment:status=full_time\", \"strength\": 1.0},\n",
    "            {\"proposition\": \"employment:on_duty_at_time=false\", \"strength\": 0.95},  # Off duty\n",
    "            {\"proposition\": \"risk:fraud_score=0.15\", \"strength\": 0.9},\n",
    "        ],\n",
    "        \"expected\": {\n",
    "            \"has_defeated_steps\": True,  # Off-duty should defeat course of employment\n",
    "            \"min_defeaters\": 1,\n",

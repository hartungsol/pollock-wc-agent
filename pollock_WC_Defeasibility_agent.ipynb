    "# ==========================================================================\n",
    "# Step 3: Call Investigation Agent and Parse New Beliefs\n",
    "# ==========================================================================\n",
    "\n",
    "async def suggest_additional_beliefs_with_investigation_agent_async(\n",
    "    state: dict,\n",
    "    max_new_beliefs: int = 5,\n",
    ") -> list[dict]:\n",
    "    \"\"\"\n",
    "    Use the investigation_agent to suggest additional beliefs that might resolve\n",
    "    unresolved plan steps.\n",
    "\n",
    "    Args:\n",
    "        state: Current state dictionary with beliefs and active_plan\n",
    "        max_new_beliefs: Maximum number of beliefs to return\n",
    "        \n",
    "    Returns:\n",
    "        List of belief dicts, each with 'proposition', 'rationale', 'source_suggestion'\n",
    "    \"\"\"\n",
    "    issues = get_unresolved_plan_issues(state)\n",
    "    if not issues[\"questioned_steps\"] and not issues[\"defeated_steps\"]:\n",
    "        return []\n",
    "\n",
    "    # Build context for the investigation agent\n",
    "    context_str = format_unresolved_for_investigation(state)\n",
    "    \n",
    "    investigation_prompt = f\"\"\"\n",
    "You are investigating a workers' compensation claim that has unresolved issues.\n",
    "\n",
    "{context_str}\n",
    "\n",
    "Based on the QUESTIONED and DEFEATED steps above, suggest additional beliefs \n",
    "(facts that could be verified) that would help resolve these issues.\n",
    "\n",
    "Focus on:\n",
    "1. Information that would directly address the defeaters shown\n",
    "2. Evidence that is realistically obtainable (medical records, witness statements, etc.)\n",
    "3. Facts that would either support or refute the claim\n",
    "\n",
    "Return ONLY a JSON object with this structure:\n",
    "{{\n",
    "    \"new_beliefs\": [\n",
    "        {{\n",
    "            \"proposition\": \"category:key=value\",\n",
    "            \"rationale\": \"Why this helps resolve the issue\",\n",
    "            \"source_suggestion\": \"How to obtain this information\"\n",
    "        }}\n",
    "    ]\n",
    "}}\n",
    "\n",
    "Suggest 2-5 beliefs maximum. Be specific and actionable.\n",
    "\"\"\"\n",
    "\n",
    "    content = types.Content(\n",
    "        role=\"user\",\n",
    "        parts=[types.Part(text=investigation_prompt)],\n",
    "    )\n",
    "    \n",
    "    response_parts = []\n",
    "    async for event in investigation_runner.run_async(\n",
    "        user_id=INVESTIGATION_USER_ID,\n",
    "        session_id=INVESTIGATION_SESSION_ID,\n",
    "        new_message=content,\n",
    "    ):\n",
    "        if hasattr(event, 'content') and event.content:\n",
    "            for part in event.content.parts:\n",
    "                if hasattr(part, 'text') and part.text:\n",
    "                    response_parts.append(part.text)\n",
    "    \n",
    "    full_response = \"\\n\".join(response_parts)\n",
    "    \n",
    "    # Parse JSON from response\n",
    "    try:\n",
    "        # Try to find JSON object in the response\n",
    "        json_match = re.search(r'\\{[^{}]*\"new_beliefs\"[^{}]*\\[.*?\\][^{}]*\\}', full_response, re.DOTALL)\n",
    "        if json_match:\n",
    "            json_str = json_match.group(0)\n",
    "            parsed = json.loads(json_str)\n",
    "            new_beliefs = parsed.get(\"new_beliefs\", [])\n",
    "        else:\n",
    "            # Try parsing entire response as JSON\n",
    "            parsed = json.loads(full_response)\n",
    "            new_beliefs = parsed.get(\"new_beliefs\", [])\n",
    "    except json.JSONDecodeError as e:\n",
    "        print(f\"[Investigation Agent] JSON parsing failed: {e}\")\n",
    "        print(f\"[Investigation Agent] Raw response: {full_response[:200]}...\")\n",
    "        return []\n",
    "    except Exception as e:\n",
    "        print(f\"[Investigation Agent] Error: {e}\")\n",
    "        return []\n",
    "    \n",
    "    # Validate and truncate\n",
    "    valid_beliefs = []\n",
    "    for b in new_beliefs[:max_new_beliefs]:\n",
    "        if isinstance(b, dict) and \"proposition\" in b:\n",
    "            valid_beliefs.append({\n",
    "                \"proposition\": b.get(\"proposition\", \"\"),\n",
    "                \"rationale\": b.get(\"rationale\", \"\"),\n",
    "                \"source_suggestion\": b.get(\"source_suggestion\", \"\"),\n",
    "            })\n",
    "    \n",
    "    return valid_beliefs\n",
    "\n",
    "\n",
    "def suggest_additional_beliefs_with_investigation_agent(\n",
    "    state: dict,\n",
    "    max_new_beliefs: int = 5,\n",
    ") -> list[dict]:\n",
    "    \"\"\"\n",
    "    Synchronous wrapper for suggest_additional_beliefs_with_investigation_agent_async.\n",
    "    \n",
    "    Args:\n",

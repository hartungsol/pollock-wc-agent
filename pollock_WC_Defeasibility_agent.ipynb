    "# ==========================================================================\n",
    "# Final Recommendation Generator\n",
    "# ==========================================================================\n",
    "\n",
    "def generate_final_recommendation(result: dict) -> dict:\n",
    "    \"\"\"\n",
    "    Generate a structured recommendation based on investigation results.\n",
    "    \n",
    "    Args:\n",
    "        result: Dict returned by run_interactive_wc_investigation\n",
    "        \n",
    "    Returns:\n",
    "        Recommendation dict with action, reasoning, and next steps\n",
    "    \"\"\"\n",
    "    questioned = result.get(\"final_questioned_count\", 0)\n",
    "    defeated = result.get(\"final_defeated_count\", 0)\n",
    "    iterations = result.get(\"iterations_completed\", 0)\n",
    "    beliefs_count = result.get(\"final_beliefs_count\", 0)\n",
    "    \n",
    "    # Get state for detailed analysis\n",
    "    state = result.get(\"state\", {})\n",
    "    active_plan = state.get(\"active_plan\", [])\n",
    "    \n",
    "    # Collect defeater details\n",
    "    critical_defeaters = []\n",
    "    for step in active_plan:\n",
    "        if step.get(\"status\") == \"DEFEATED\":\n",
    "            for d in step.get(\"defeaters\", []):\n",
    "                if d.get(\"severity\", 0) >= 0.8:\n",
    "                    critical_defeaters.append({\n",
    "                        \"step\": step.get(\"id\"),\n",
    "                        \"claim\": d.get(\"claim\", \"Unknown\"),\n",
    "                        \"severity\": d.get(\"severity\", 0),\n",
    "                    })\n",
    "    \n",
    "    # Determine recommendation\n",
    "    if defeated > 0:\n",
    "        recommendation = {\n",
    "            \"action\": \"ESCALATE_FOR_REVIEW\",\n",
    "            \"priority\": \"HIGH\",\n",
    "            \"summary\": f\"Claim requires supervisor review - {defeated} step(s) have critical issues\",\n",
    "            \"reasoning\": [\n",
    "                f\"Investigation completed after {iterations} iteration(s)\",\n",
    "                f\"Found {defeated} DEFEATED step(s) with high-severity defeaters\",\n",
    "                f\"Total evidence collected: {beliefs_count} beliefs\",\n",
    "            ],\n",
    "            \"critical_issues\": critical_defeaters,\n",
    "            \"next_steps\": [\n",
    "                \"Review critical defeaters with supervisor\",\n",
    "                \"Consider denial if issues cannot be resolved\",\n",
    "                \"Document all investigation findings\",\n",
    "            ],\n",
    "        }\n",
    "    elif questioned > 0:\n",
    "        recommendation = {\n",
    "            \"action\": \"INVESTIGATE_FURTHER\",\n",
    "            \"priority\": \"MEDIUM\",\n",
    "            \"summary\": f\"Additional investigation needed - {questioned} step(s) have unresolved questions\",\n",
    "            \"reasoning\": [\n",
    "                f\"Investigation completed after {iterations} iteration(s)\",\n",
    "                f\"Found {questioned} QUESTIONED step(s) requiring clarification\",\n",
    "                f\"Total evidence collected: {beliefs_count} beliefs\",\n",
    "            ],\n",
    "            \"suggested_evidence\": [\n",
    "                \"Obtain additional medical documentation\",\n",
    "                \"Interview witnesses if not already done\",\n",
    "                \"Request employer incident report\",\n",
    "                \"Verify employment status at time of injury\",\n",
    "            ],\n",
    "            \"next_steps\": [\n",
    "                \"Schedule follow-up investigation\",\n",
    "                \"Request specific missing evidence\",\n",
    "                \"Set reminder for 7-day follow-up\",\n",
    "            ],\n",
    "        }\n",
    "    else:\n",
    "        recommendation = {\n",
    "            \"action\": \"PROCEED_TO_APPROVAL\",\n",
    "            \"priority\": \"STANDARD\",\n",
    "            \"summary\": \"Claim ready for approval review - all steps supported\",\n",
    "            \"reasoning\": [\n",
    "                f\"Investigation completed after {iterations} iteration(s)\",\n",
    "                \"All plan steps are SUPPORTED with no blocking defeaters\",\n",
    "                f\"Total evidence collected: {beliefs_count} beliefs\",\n",
    "            ],\n",
    "            \"verification_checklist\": [\n",
    "                \"Verify all required documentation is on file\",\n",
    "                \"Confirm benefit calculations\",\n",
    "                \"Check jurisdiction-specific requirements\",\n",
    "                \"Process approval through standard workflow\",\n",
    "            ],\n",
    "            \"next_steps\": [\n",
    "                \"Submit for approval\",\n",
    "                \"Notify claimant of decision timeline\",\n",
    "                \"Begin benefit processing preparation\",\n",
    "            ],\n",
    "        }\n",
    "    \n",
    "    return recommendation\n",
    "\n",
    "\n",
    "def print_final_recommendation(recommendation: dict) -> None:\n",
    "    \"\"\"Pretty-print the final recommendation.\"\"\"\n",
    "    print(\"\\n\" + \"=\" * 70)\n",
    "    print(\"FINAL RECOMMENDATION\")\n",
    "    print(\"=\" * 70)\n",
    "    \n",
    "    action = recommendation.get(\"action\", \"UNKNOWN\")\n",
    "    priority = recommendation.get(\"priority\", \"STANDARD\")\n",
    "    \n",
    "    # Action-specific formatting\n",
    "    if action == \"ESCALATE_FOR_REVIEW\":\n",
    "        print(f\"\\n⚠️  ACTION: {action}\")\n",
    "        print(f\"   Priority: {priority}\")\n",
    "    elif action == \"INVESTIGATE_FURTHER\":\n",
    "        print(f\"\\n🔍 ACTION: {action}\")\n",
    "        print(f\"   Priority: {priority}\")\n",
    "    else:\n",
    "        print(f\"\\n✓  ACTION: {action}\")\n",
    "        print(f\"   Priority: {priority}\")\n",
    "    \n",
    "    print(f\"\\n   {recommendation.get('summary', '')}\")\n",
    "    \n",
    "    # Reasoning\n",
    "    print(\"\\n   Reasoning:\")\n",
    "    for r in recommendation.get(\"reasoning\", []):\n",
    "        print(f\"     • {r}\")\n",
    "    \n",
    "    # Critical issues (if any)\n",
    "    if recommendation.get(\"critical_issues\"):\n",
    "        print(\"\\n   Critical Issues:\")\n",
    "        for issue in recommendation.get(\"critical_issues\", []):\n",
    "            print(f\"     ✗ [{issue.get('step')}] {issue.get('claim', '')[:50]}\")\n",
    "    \n",
    "    # Suggested evidence (if applicable)\n",

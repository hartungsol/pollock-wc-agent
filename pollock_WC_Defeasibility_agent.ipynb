    "# ==========================================================================\n",
    "# Backward Reasoning Schemas (OSCAR-style)\n",
    "# ==========================================================================\n",
    "# Maps conclusions to the subgoals that would establish them.\n",
    "# Used for goal-directed reasoning when we need to prove something.\n",
    "# ==========================================================================\n",
    "\n",
    "@dataclass\n",
    "class BackwardSchema:\n",
    "    \"\"\"\n",
    "    A backward reasoning schema in Pollock's sense.\n",
    "    \n",
    "    Given a desired conclusion, specifies what subgoals must be established\n",
    "    to derive that conclusion. Supports multiple alternative derivation paths.\n",
    "    \"\"\"\n",
    "    id: str\n",
    "    target_conclusion: str\n",
    "    \n",
    "    # Alternative sets of subgoals (any one set suffices)\n",
    "    # Each inner list is a conjunction of required subgoals\n",
    "    alternative_subgoals: List[List[str]] = field(default_factory=list)\n",
    "    \n",
    "    # Strength of derivation if subgoals are met\n",
    "    derivation_strength: float = 0.8\n",
    "    \n",
    "    # Conditions that must hold for schema to apply\n",
    "    applicability_conditions: List[str] = field(default_factory=list)\n",
    "    \n",
    "    def matches_conclusion(self, conclusion: str) -> bool:\n",
    "        \"\"\"Check if this schema can derive the given conclusion.\"\"\"\n",
    "        return self.target_conclusion.lower() in conclusion.lower()\n",
    "\n",
    "\n",
    "# ==========================================================================\n",
    "# Workers' Compensation Backward Schemas\n",
    "# ==========================================================================\n",
    "\n",
    "BACKWARD_SCHEMAS: Dict[str, BackwardSchema] = {}\n",
    "\n",
    "\n",
    "def register_backward_schema(schema: BackwardSchema) -> BackwardSchema:\n",
    "    \"\"\"Register a backward schema.\"\"\"\n",
    "    BACKWARD_SCHEMAS[schema.id] = schema\n",
    "    return schema\n",
    "\n",
    "\n",
    "# Schema: To establish \"claim is compensable\"\n",
    "register_backward_schema(BackwardSchema(\n",
    "    id=\"compensability_schema\",\n",
    "    target_conclusion=\"claim is compensable\",\n",
    "    alternative_subgoals=[\n",
    "        # Path 1: Standard work injury\n",
    "        [\n",
    "            \"injury occurred during employment\",\n",
    "            \"injury arose out of employment duties\",\n",
    "            \"injury was reported timely\",\n",
    "            \"medical evidence supports injury\",\n",
    "        ],\n",
    "        # Path 2: Occupational disease\n",
    "        [\n",
    "            \"disease is occupational in nature\",\n",
    "            \"exposure occurred at workplace\",\n",
    "            \"medical causation established\",\n",
    "            \"proper notice given\",\n",
    "        ],\n",
    "    ],\n",
    "    derivation_strength=0.85,\n",
    "    applicability_conditions=[\"claim is filed\", \"within statute of limitations\"],\n",
    "))\n",
    "\n",
    "\n",
    "# Schema: To establish \"medical treatment is reasonable\"\n",
    "register_backward_schema(BackwardSchema(\n",
    "    id=\"treatment_reasonableness_schema\",\n",
    "    target_conclusion=\"medical treatment is reasonable and necessary\",\n",
    "    alternative_subgoals=[\n",
    "        [\n",
    "            \"treatment is for accepted condition\",\n",
    "            \"treating physician recommends treatment\",\n",
    "            \"treatment is standard of care\",\n",
    "        ],\n",
    "        [\n",
    "            \"independent medical exam supports treatment\",\n",
    "            \"treatment improves functional capacity\",\n",
    "        ],\n",
    "    ],\n",
    "    derivation_strength=0.80,\n",
    "))\n",
    "\n",
    "\n",
    "# Schema: To establish \"temporary disability benefits due\"\n",
    "register_backward_schema(BackwardSchema(\n",
    "    id=\"ttd_benefits_schema\",\n",
    "    target_conclusion=\"temporary disability benefits are due\",\n",
    "    alternative_subgoals=[\n",
    "        [\n",
    "            \"injury is work-related\",\n",
    "            \"claimant is medically unable to work\",\n",
    "            \"claimant is within healing period\",\n",
    "            \"waiting period has passed\",\n",
    "        ],\n",
    "    ],\n",
    "    derivation_strength=0.90,\n",
    "))\n",
    "\n",
    "\n",
    "# Schema: To establish \"permanent disability exists\"\n",
    "register_backward_schema(BackwardSchema(\n",
    "    id=\"permanent_disability_schema\",\n",
    "    target_conclusion=\"permanent disability exists\",\n",
    "    alternative_subgoals=[\n",
    "        [\n",
    "            \"maximum medical improvement reached\",\n",
    "            \"permanent impairment rating assigned\",\n",
    "            \"impairment is causally related to injury\",\n",
    "        ],\n",
    "    ],\n",
    "    derivation_strength=0.85,\n",
    "))\n",
    "\n",
    "\n",
    "# Schema: To establish \"claim should be denied\"\n",
    "register_backward_schema(BackwardSchema(\n",
    "    id=\"denial_schema\",\n",
    "    target_conclusion=\"claim should be denied\",\n",
    "    alternative_subgoals=[\n",
    "        # Path 1: Not work-related\n",
    "        [\"injury did not arise out of employment\"],\n",
    "        # Path 2: Untimely\n",
    "        [\"claim filed after statute of limitations\"],\n",
    "        # Path 3: Fraud\n",
    "        [\"fraud established by clear evidence\"],\n",
    "        # Path 4: No injury\n",
    "        [\"no compensable injury occurred\"],\n",
    "    ],\n",
    "    derivation_strength=0.95,\n",
    "))\n",
    "\n",
    "\n",
    "def find_schemas_for_conclusion(conclusion: str) -> List[BackwardSchema]:\n",
    "    \"\"\"Find all backward schemas that could derive the given conclusion.\"\"\"\n",
    "    matching = []\n",
    "    for schema in BACKWARD_SCHEMAS.values():\n",
    "        if schema.matches_conclusion(conclusion):\n",
    "            matching.append(schema)\n",
    "    return matching\n",
    "\n",
    "\n",
    "def get_subgoals_for_conclusion(\n",
    "    conclusion: str,\n",
    "    satisfied_goals: Set[str] = None\n",
    ") -> List[Dict[str, Any]]:\n",
    "    \"\"\"\n",
    "    Get the subgoals needed to establish a conclusion.\n",
    "    \n",
    "    Returns list of alternative subgoal sets with their satisfaction status.\n",
    "    \"\"\"\n",
    "    satisfied_goals = satisfied_goals or set()\n",
    "    matching_schemas = find_schemas_for_conclusion(conclusion)\n",
    "    \n",
    "    results = []\n",
    "    for schema in matching_schemas:\n",
    "        for i, subgoal_set in enumerate(schema.alternative_subgoals):\n",
    "            satisfied = [g for g in subgoal_set if g in satisfied_goals]\n",
    "            unsatisfied = [g for g in subgoal_set if g not in satisfied_goals]\n",
    "            \n",
    "            results.append({\n",
    "                \"schema_id\": schema.id,\n",
    "                \"path_index\": i,\n",
    "                \"subgoals\": subgoal_set,\n",
    "                \"satisfied\": satisfied,\n",
    "                \"unsatisfied\": unsatisfied,\n",
    "                \"complete\": len(unsatisfied) == 0,\n",
    "                \"strength\": schema.derivation_strength if len(unsatisfied) == 0 else 0.0,\n",
    "            })\n",
    "    \n",
    "    return results\n",
    "\n",
    "\n",
    "print(\"✓ Backward reasoning schemas defined\")\n",
    "print(f\"  Registered {len(BACKWARD_SCHEMAS)} schemas:\")\n",
    "for sid, schema in BACKWARD_SCHEMAS.items():\n",
    "    print(f\"    - {sid}: {schema.target_conclusion}\")"
   ]

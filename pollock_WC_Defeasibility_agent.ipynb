    "# ==========================================================================\n",
    "# Global Defeat Computation Algorithm\n",
    "# ==========================================================================\n",
    "# Implements Pollock's approach: iteratively propagate defeat status through\n",
    "# the argument graph until a fixed point is reached.\n",
    "# ==========================================================================\n",
    "\n",
    "def build_arguments_from_plan(plan: List[Dict], defeaters: List[Defeater] = None) -> Dict[str, Argument]:\n",
    "    \"\"\"\n",
    "    Build argument graph from plan steps and their associated reasons.\n",
    "    \n",
    "    Each plan step becomes an argument supporting its conclusion.\n",
    "    Defeaters create attack relationships between arguments.\n",
    "    \"\"\"\n",
    "    arguments = {}\n",
    "    \n",
    "    for step in plan:\n",
    "        step_id = step.get(\"id\", f\"step_{len(arguments)}\")\n",
    "        \n",
    "        # Create a supporting reason for this step\n",
    "        reason = Reason(\n",
    "            id=f\"reason_{step_id}\",\n",
    "            name=f\"Reason for {step.get('description', step_id)[:40]}\",\n",
    "            kind=ReasonKind.PRIMA_FACIE,\n",
    "            premises=step.get(\"preconditions\", []),\n",
    "            conclusions=[step.get(\"description\", \"\")],\n",
    "            base_strength=0.7,\n",
    "            computed_strength=0.7,\n",
    "            target_step_id=step_id,\n",
    "        )\n",
    "        register_reason(reason)\n",
    "        \n",
    "        # Create argument from this reason\n",
    "        arg = Argument(\n",
    "            id=f\"arg_{step_id}\",\n",
    "            conclusion=step.get(\"description\", \"\"),\n",
    "            reasons=[reason],\n",
    "            supports_step_id=step_id,\n",
    "            status=ArgumentStatus.UNDECIDED,\n",
    "        )\n",
    "        arg.compute_aggregate_strength()\n",
    "        arguments[arg.id] = arg\n",
    "        register_argument(arg)\n",
    "    \n",
    "    return arguments\n",
    "\n",
    "\n",
    "def build_attack_arguments_from_defeaters(\n",
    "    defeaters: List[Defeater],\n",
    "    target_arguments: Dict[str, Argument]\n",
    ") -> Dict[str, Argument]:\n",
    "    \"\"\"\n",
    "    Create arguments from defeaters and link attacks to target arguments.\n",
    "    \n",
    "    Each defeater becomes a counter-argument that attacks the target.\n",
    "    \"\"\"\n",
    "    attack_arguments = {}\n",
    "    \n",
    "    for defeater in defeaters:\n",
    "        # Determine what this defeater attacks\n",
    "        target_step_id = defeater.target_step_id\n",
    "        target_arg_id = f\"arg_{target_step_id}\" if target_step_id else None\n",
    "        \n",
    "        # Create defeating reason\n",
    "        defeating_reason = Reason(\n",
    "            id=f\"reason_{defeater.id}\",\n",
    "            name=f\"Defeater: {defeater.source_rule}\",\n",
    "            kind=ReasonKind.CONCLUSIVE if defeater.kind == \"regulatory\" else ReasonKind.PRIMA_FACIE,\n",
    "            premises=[f\"From rule: {defeater.source_rule}\"],\n",
    "            conclusions=[defeater.message],\n",
    "            base_strength=defeater.severity,\n",
    "            computed_strength=defeater.severity,\n",
    "            target_step_id=target_step_id,\n",
    "            target_reason_ids=defeater.target_reason_ids,\n",
    "        )\n",
    "        register_reason(defeating_reason)\n",
    "        \n",
    "        # Create attack argument\n",
    "        attack_arg = Argument(\n",
    "            id=f\"attack_{defeater.id}\",\n",
    "            conclusion=defeater.message,\n",
    "            reasons=[defeating_reason],\n",
    "            status=ArgumentStatus.UNDECIDED,\n",
    "        )\n",
    "        attack_arg.compute_aggregate_strength()\n",
    "        \n",
    "        # Link the attack\n",
    "        if target_arg_id and target_arg_id in target_arguments:\n",
    "            attack_arg.attacks.append(target_arg_id)\n",
    "            target_arguments[target_arg_id].attacked_by.append(attack_arg.id)\n",
    "        \n",
    "        attack_arguments[attack_arg.id] = attack_arg\n",
    "        register_argument(attack_arg)\n",
    "    \n",
    "    return attack_arguments\n",
    "\n",
    "\n",
    "def compute_defeat_status(arguments: Dict[str, Argument], max_iterations: int = 10) -> Dict[str, Argument]:\n",
    "    \"\"\"\n",
    "    Compute global defeat status using iterative fixed-point algorithm.\n",
    "    \n",
    "    This implements Pollock's approach to computing the dialectical status of\n",
    "    arguments in a defeasibility graph. The algorithm iteratively updates\n",
    "    argument statuses until reaching a fixed point (convergence).\n",
    "    \n",
    "    Stopping Criteria:\n",
    "    1. CONVERGENCE: No status changes in an iteration (fixed point reached)\n",
    "    2. MAX ITERATIONS: Safety limit to prevent infinite loops in cyclic graphs\n",
    "    \n",
    "    Status Assignment Rules:\n",
    "    - UNDEFEATED: No attackers OR all attackers are themselves defeated\n",
    "    - DEFEATED: Has at least one undefeated attacker that is stronger\n",
    "    - UNDECIDED: In mutual attack with another argument of similar strength\n",
    "                (within 0.1 threshold), creating a dialectical standoff\n",
    "    \n",
    "    Special Cases:\n",
    "    - Conclusive reasons: Can only be defeated by other conclusive reasons\n",
    "    - Strength comparison: Attacker must be 0.1+ stronger to defeat\n",
    "    \n",
    "    Args:\n",
    "        arguments: Dict of argument_id -> Argument objects with attack relations\n",
    "        max_iterations: Safety limit for iterations (default 10)\n",
    "    \n",
    "    Returns:\n",
    "        The same arguments dict with updated status fields.\n",
    "        Logs convergence info to the global logger.\n",
    "    \"\"\"\n",
    "    # Initialize all arguments as UNDECIDED\n",
    "    for arg in arguments.values():\n",
    "        arg.status = ArgumentStatus.UNDECIDED\n",
    "    \n",
    "    # Iteratively compute status until fixed point\n",
    "    for iteration in range(max_iterations):\n",
    "        changed = False\n",
    "        \n",
    "        for arg_id, arg in arguments.items():\n",
    "            old_status = arg.status\n",
    "            \n",
    "            # Get all attacking arguments\n",
    "            attackers = [arguments.get(attacker_id) for attacker_id in arg.attacked_by]\n",
    "            attackers = [a for a in attackers if a is not None]\n",
    "            \n",
    "            if not attackers:\n",
    "                # No attackers -> UNDEFEATED\n",
    "                new_status = ArgumentStatus.UNDEFEATED\n",
    "            else:\n",
    "                # Check if any attacker is undefeated and stronger\n",
    "                undefeated_attackers = [\n",
    "                    a for a in attackers \n",
    "                    if a.status == ArgumentStatus.UNDEFEATED\n",
    "                ]\n",
    "                \n",
    "                if not undefeated_attackers:\n",
    "                    # All attackers are defeated -> UNDEFEATED\n",
    "                    new_status = ArgumentStatus.UNDEFEATED\n",
    "                else:\n",
    "                    # Check strength comparison\n",
    "                    max_attacker_strength = max(a.computed_strength for a in undefeated_attackers)\n",
    "                    my_strength = arg.computed_strength\n",
    "                    \n",
    "                    # Conclusive reasons cannot be defeated by prima facie ones\n",
    "                    if arg.has_conclusive_reason():\n",
    "                        conclusive_attackers = [\n",
    "                            a for a in undefeated_attackers \n",
    "                            if a.has_conclusive_reason()\n",
    "                        ]\n",
    "                        if not conclusive_attackers:\n",
    "                            new_status = ArgumentStatus.UNDEFEATED\n",
    "                        elif max(a.computed_strength for a in conclusive_attackers) > my_strength:\n",
    "                            new_status = ArgumentStatus.DEFEATED\n",
    "                        else:\n",
    "                            new_status = ArgumentStatus.UNDECIDED\n",
    "                    elif max_attacker_strength > my_strength + 0.1:  # Clear victory threshold\n",
    "                        new_status = ArgumentStatus.DEFEATED\n",
    "                    elif abs(max_attacker_strength - my_strength) <= 0.1:\n",
    "                        # Similar strength -> dialectical standoff\n",
    "                        new_status = ArgumentStatus.UNDECIDED\n",
    "                    else:\n",
    "                        new_status = ArgumentStatus.UNDEFEATED\n",
    "            \n",
    "            if new_status != old_status:\n",
    "                arg.status = new_status\n",
    "                changed = True\n",
    "        \n",
    "        if not changed:\n",
    "            logger.info(f\"Defeat computation converged after {iteration + 1} iterations\")\n",
    "            break\n",
    "    \n",
    "    return arguments\n",
    "\n",
    "\n",
    "def run_global_defeat_analysis(\n",
    "    plan: List[Dict],\n",
    "    defeaters: List[Defeater]\n",
    ") -> Tuple[Dict[str, Argument], Dict[str, Reason]]:\n",
    "    \"\"\"\n",
    "    Run complete OSCAR-style defeat analysis on a plan.\n",
    "    \n",
    "    1. Build arguments from plan steps\n",
    "    2. Build attack arguments from defeaters\n",
    "    3. Compute global defeat status\n",
    "    4. Return the complete argument and reason graphs\n",
    "    \"\"\"\n",
    "    # Clear previous state\n",
    "    clear_reasoning_state()\n",
    "    \n",
    "    # Build argument graph\n",
    "    plan_arguments = build_arguments_from_plan(plan)\n",
    "    \n",
    "    # Build attack arguments\n",
    "    all_arguments = {**plan_arguments}\n",
    "    attack_arguments = build_attack_arguments_from_defeaters(defeaters, plan_arguments)\n",
    "    all_arguments.update(attack_arguments)\n",
    "    \n",
    "    # Compute defeat status\n",
    "    compute_defeat_status(all_arguments)\n",
    "    \n",
    "    logger.info(f\"Defeat analysis complete: {len(all_arguments)} arguments, {len(REASONS)} reasons\")\n",
    "    \n",
    "    # Summary stats\n",
    "    undefeated = sum(1 for a in all_arguments.values() if a.status == ArgumentStatus.UNDEFEATED)\n",
    "    defeated = sum(1 for a in all_arguments.values() if a.status == ArgumentStatus.DEFEATED)\n",
    "    undecided = sum(1 for a in all_arguments.values() if a.status == ArgumentStatus.UNDECIDED)\n",
    "    \n",
    "    logger.info(f\"  UNDEFEATED: {undefeated}, DEFEATED: {defeated}, UNDECIDED: {undecided}\")\n",
    "    \n",
    "    return all_arguments, REASONS\n",
    "\n",
    "\n",
    "print(\"✓ Global defeat computation algorithm defined\")\n",
    "print(\"  - build_arguments_from_plan(plan)\")\n",
    "print(\"  - build_attack_arguments_from_defeaters(defeaters, target_args)\")\n",
    "print(\"  - compute_defeat_status(arguments, max_iterations)\")\n",
    "print(\"  - run_global_defeat_analysis(plan, defeaters)\")"
   ]

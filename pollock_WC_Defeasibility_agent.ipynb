    "            {\"proposition\": \"employment:status=independent_contractor\", \"strength\": 1.0},\n",
    "            {\"proposition\": \"employment:on_duty_at_time=true\", \"strength\": 0.95},\n",
    "            {\"proposition\": \"risk:fraud_score=0.20\", \"strength\": 0.9},\n",
    "        ],\n",
    "        \"expected\": {\n",
    "            \"has_defeated_steps\": True,  # Contractor should defeat eligibility\n",
    "            \"min_defeaters\": 1,\n",

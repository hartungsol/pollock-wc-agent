    "# ==========================================================================\n",
    "# Interactive Belief Review and Entry\n",
    "# ==========================================================================\n",
    "\n",
    "def display_suggested_beliefs(beliefs: list[dict]) -> None:\n",
    "    \"\"\"Display suggested beliefs in a numbered format for user review.\"\"\"\n",
    "    if not beliefs:\n",
    "        print(\"\\n  No beliefs suggested by Investigation Agent.\")\n",
    "        return\n",
    "    \n",
    "    print(\"\\n\" + \"-\" * 60)\n",
    "    print(\"SUGGESTED BELIEFS FROM INVESTIGATION AGENT\")\n",
    "    print(\"-\" * 60)\n",
    "    \n",
    "    for i, b in enumerate(beliefs, 1):\n",
    "        print(f\"\\n  [{i}] {b.get('proposition', 'Unknown')}\")\n",
    "        if b.get('rationale'):\n",
    "            print(f\"      Rationale: {b.get('rationale', '')[:80]}\")\n",
    "        if b.get('source_suggestion'):\n",
    "            print(f\"      Source: {b.get('source_suggestion', '')[:60]}\")\n",
    "\n",
    "\n",
    "def prompt_user_belief_selection(beliefs: list[dict]) -> list[dict]:\n",
    "    \"\"\"\n",
    "    Prompt user to select which suggested beliefs to accept.\n",
    "    \n",
    "    Args:\n",
    "        beliefs: List of belief dicts from investigation agent\n",
    "        \n",
    "    Returns:\n",
    "        List of accepted beliefs\n",
    "    \"\"\"\n",
    "    if not beliefs:\n",
    "        return []\n",
    "    \n",
    "    display_suggested_beliefs(beliefs)\n",
    "    \n",
    "    print(\"\\n\" + \"-\" * 60)\n",
    "    print(\"OPTIONS:\")\n",
    "    print(\"  - Enter numbers to accept (e.g., '1,3' or '1 3' or 'all')\")\n",
    "    print(\"  - Enter 'none' to reject all\")\n",
    "    print(\"  - Enter 'skip' to skip this round\")\n",
    "    print(\"-\" * 60)\n",
    "    \n",
    "    try:\n",
    "        user_input = input(\"\\nAccept beliefs: \").strip().lower()\n",
    "    except EOFError:\n",
    "        print(\"[No input - accepting all]\")\n",
    "        return beliefs\n",
    "    \n",
    "    if not user_input or user_input == 'skip':\n",
    "        print(\"[Skipping belief selection]\")\n",
    "        return []\n",
    "    \n",
    "    if user_input == 'all':\n",
    "        print(f\"[Accepting all {len(beliefs)} beliefs]\")\n",
    "        return beliefs\n",
    "    \n",
    "    if user_input == 'none':\n",
    "        print(\"[Rejecting all beliefs]\")\n",
    "        return []\n",
    "    \n",
    "    # Parse numbers\n",
    "    accepted = []\n",
    "    try:\n",
    "        # Handle comma or space separated numbers\n",
    "        parts = user_input.replace(',', ' ').split()\n",
    "        for p in parts:\n",
    "            idx = int(p) - 1  # Convert to 0-indexed\n",
    "            if 0 <= idx < len(beliefs):\n",
    "                accepted.append(beliefs[idx])\n",
    "            else:\n",
    "                print(f\"  [Warning] Index {p} out of range, skipping\")\n",
    "    except ValueError:\n",
    "        print(\"[Invalid input - accepting none]\")\n",
    "        return []\n",
    "    \n",
    "    print(f\"[Accepted {len(accepted)} belief(s)]\")\n",
    "    return accepted\n",
    "\n",
    "\n",
    "def prompt_user_for_custom_beliefs() -> list[dict]:\n",
    "    \"\"\"\n",
    "    Prompt user to enter custom beliefs manually.\n",
    "    \n",
    "    Accepts multiple formats:\n",
    "    - Full format: category:key=value (e.g., medical:confirmed=true)\n",
    "    - Simple format: category:statement (e.g., employment:supervisor_verified)\n",
    "    - Key=value format: key=value (auto-categorized as 'custom')\n",
    "    - Free text: Natural language statements (preserved for LLM analysis)\n",
    "    \n",
    "    Returns:\n",
    "        List of custom belief dicts\n",
    "    \"\"\"\n",
    "    print(\"\\n\" + \"-\" * 60)\n",
    "    print(\"ADD CUSTOM BELIEFS\")\n",
    "    print(\"-\" * 60)\n",
    "    print(\"Enter beliefs as:\")\n",
    "    print(\"  - Natural language: 'witness saw employee doing backflips'\")\n",
    "    print(\"  - Structured: category:key=value (e.g., investigation:fraud_suspected=true)\")\n",
    "    print(\"\\nCategories: claim, medical, employment, investigation, documentation, witness\")\n",
    "    print(\"\\nEnter beliefs one per line. Type 'done' when finished.\")\n",
    "    print(\"-\" * 60)\n",
    "    \n",
    "    custom_beliefs = []\n",
    "    \n",
    "    while True:\n",
    "        try:\n",
    "            user_input = input(\"Belief: \").strip()\n",
    "        except EOFError:\n",
    "            break\n",
    "        \n",
    "        if not user_input or user_input.lower() == 'done':\n",
    "            break\n",
    "        \n",
    "        # Normalize the input into proposition format\n",
    "        proposition = None\n",
    "        original_text = user_input  # Preserve original for rationale\n",
    "        \n",
    "        if ':' in user_input and '=' in user_input:\n",
    "            # Full format: category:key=value\n",
    "            proposition = user_input\n",
    "        elif ':' in user_input:\n",
    "            # Simple format: category:statement - add =true\n",
    "            proposition = f\"{user_input}=true\"\n",
    "        elif '=' in user_input:\n",
    "            # Key=value format - add 'custom:' prefix\n",
    "            proposition = f\"custom:{user_input}\"\n",
    "        else:\n",
    "            # Free text - preserve as natural language for LLM analysis\n",
    "            # Store as witness:statement=\"original text\" so LLM can interpret it\n",
    "            proposition = f\"witness:statement={user_input}\"\n",
    "        \n",
    "        # Create belief dict with original text preserved in rationale\n",
    "        custom_beliefs.append({\n",
    "            \"proposition\": proposition,\n",
    "            \"rationale\": f\"User observation: {original_text}\",\n",
    "            \"source_suggestion\": \"Manual entry by claims adjuster\",\n",
    "        })\n",
    "        print(f\"  [Added: {proposition}]\")\n",
    "    \n",
    "    print(f\"\\n[Added {len(custom_beliefs)} custom belief(s)]\")\n",
    "    return custom_beliefs\n",
    "\n",
    "\n",
    "def prompt_continue_investigation() -> str:\n",
    "    \"\"\"\n",
    "    Prompt user whether to continue investigation.\n",
    "    \n",
    "    Returns:\n",
    "        'continue', 'stop', or 'add' (to add more beliefs manually)\n",
    "    \"\"\"\n",
    "    print(\"\\n\" + \"-\" * 60)\n",
    "    print(\"INVESTIGATION OPTIONS\")\n",
    "    print(\"-\" * 60)\n",
    "    print(\"  [c] Continue - Run another analysis pass\")\n",
    "    print(\"  [a] Add - Add more beliefs manually\")\n",
    "    print(\"  [s] Stop - End investigation and show results\")\n",
    "    print(\"-\" * 60)\n",
    "    \n",
    "    try:\n",
    "        user_input = input(\"Choice [c/a/s]: \").strip().lower()\n",
    "    except EOFError:\n",
    "        return 'stop'\n",
    "    \n",
    "    if user_input in ['c', 'continue', '']:\n",
    "        return 'continue'\n",
    "    elif user_input in ['a', 'add']:\n",
    "        return 'add'\n",
    "    else:\n",
    "        return 'stop'\n",
    "\n",
    "\n",
    "print(\"Interactive belief helpers loaded:\")\n",
    "print(\"  - display_suggested_beliefs(beliefs)\")\n",
    "print(\"  - prompt_user_belief_selection(beliefs) -> list[dict]\")\n",
    "print(\"  - prompt_user_for_custom_beliefs() -> list[dict]\")\n",
    "print(\"  - prompt_continue_investigation() -> str\")"
   ]

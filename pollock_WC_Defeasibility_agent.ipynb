    "        print(f\"  Plan steps: {len(context.state.get('active_plan', []))}\")\n",
    "    \n",
    "    # Iterative analysis loop\n",
    "    for iteration in range(max_iterations):\n",
    "        if verbose:\n",
    "            print(f\"\\n{'='*70}\")\n",
    "            print(f\"ITERATION {iteration + 1}/{max_iterations}\")\n",
    "            print(\"=\" * 70)\n",
    "        \n",
    "        # 2. Run combined analysis: forward -> rules -> backward\n",
    "        if verbose:\n",
    "            print(\"\\n[ANALYZE] Running forward reasoning + rules + backward analysis...\")\n",
    "        \n",
    "        rule_result = run_workers_comp_analyst_once(context, verbose=verbose)\n",
    "        \n",
    "        # Record iteration state\n",
    "        issues = get_unresolved_plan_issues(context.state)\n",
    "        backward_summary = get_backward_analysis_summary(context.state)\n",
    "        \n",
    "        iter_record = {\n",
    "            \"iteration\": iteration + 1,\n",
    "            \"violations_found\": rule_result.get(\"violations_found\", 0),\n",
    "            \"step_statuses\": rule_result.get(\"step_statuses\", {}),\n",
    "            \"questioned_count\": len(issues[\"questioned_steps\"]),\n",
    "            \"defeated_count\": len(issues[\"defeated_steps\"]),\n",
    "            \"beliefs_count\": len(context.state.get(\"beliefs\", [])),\n",
    "            \"forward_beliefs_added\": rule_result.get(\"forward_beliefs_added\", 0),\n",
    "            \"backward_goals_pending\": backward_summary.get(\"goals_pending\", 0),\n",
    "        }\n",
    "        iteration_history.append(iter_record)\n",
    "        \n",
    "        if verbose:\n",
    "            print(f\"  Violations: {rule_result.get('violations_found', 0)}\")\n",
    "            print(f\"  Step statuses: {rule_result.get('step_statuses', {})}\")\n",
    "            print(f\"  QUESTIONED steps: {len(issues['questioned_steps'])}\")\n",
    "            print(f\"  Goals pending: {backward_summary.get('goals_pending', 0)}\")\n",
    "        \n",
    "        # 3. Check if we have unresolved issues\n",
    "        if not issues[\"questioned_steps\"]:\n",
    "            if verbose:\n",
    "                print(\"\\n[COMPLETE] No QUESTIONED steps remain. Analysis complete.\")\n",
    "            break\n",
    "        \n",
    "        # 4. Call Investigation Agent for new beliefs\n",
    "        if verbose:\n",
    "            print(\"\\n[INVESTIGATE] Calling Investigation Agent...\")\n",
    "            for step in issues[\"questioned_steps\"]:\n",
    "                print(f\"  - {step['id']}: {step['description'][:50]}...\")\n",
    "        \n",
    "        new_beliefs = suggest_additional_beliefs_with_investigation_agent(\n",

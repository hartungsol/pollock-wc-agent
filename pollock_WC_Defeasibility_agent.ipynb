    "]\n",
    "\n",
    "\n",
    "def setup_test_context(scenario: Dict[str, Any]) -> Any:\n",
    "    \"\"\"\n",
    "    Create and populate a test context from a scenario definition.\n",
    "    \n",
    "    Args:\n",
    "        scenario: Test scenario dict with intake and beliefs\n",
    "        \n",
    "    Returns:\n",
    "        MockToolContext ready for rule evaluation\n",
    "    \"\"\"\n",
    "    ctx = MockToolContext()\n",
    "    intake = scenario[\"intake\"]\n",
    "    \n",
    "    # Initialize the plan\n",
    "    init_workers_comp_claim_plan(\n",
    "        claim_description=intake.get(\"injury_description\", \"Test injury\"),\n",
    "        jurisdiction=intake.get(\"jurisdiction\", \"CA\"),\n",
    "        severity_level=\"moderate\",\n",
    "        tool_context=ctx,\n",
    "    )\n",
    "    \n",
    "    # Add scenario-specific beliefs\n",
    "    for belief in scenario.get(\"beliefs\", []):\n",
    "        b = Belief(\n",
    "            id=f\"test-{len(ctx.state['beliefs'])+1}\",\n",
    "            proposition=belief[\"proposition\"],\n",
    "            support=\"test_scenario\",\n",
    "            strength=belief.get(\"strength\", 0.9),\n",
    "        )\n",
    "        ctx.state[\"beliefs\"].append(b.model_dump())\n",
    "    \n",
    "    # Add intake-derived beliefs\n",
    "    if \"date_of_injury\" in intake and \"date_reported\" in intake:\n",
    "        ctx.state[\"claim_context\"][\"date_of_injury\"] = intake[\"date_of_injury\"]\n",
    "        ctx.state[\"claim_context\"][\"first_notice_date\"] = intake[\"date_reported\"]\n",
    "    \n",
    "    return ctx\n",
    "\n",
    "\n",
    "def run_test_scenario(scenario: Dict[str, Any]) -> Tuple[bool, str, Dict[str, Any]]:\n",
    "    \"\"\"\n",
    "    Run a single test scenario and check expectations.\n",
    "    \n",
    "    Args:\n",
    "        scenario: Test scenario dict\n",
    "        \n",
    "    Returns:\n",
    "        Tuple of (passed: bool, message: str, details: dict)\n",
    "    \"\"\"\n",
    "    name = scenario[\"name\"]\n",
    "    expected = scenario[\"expected\"]\n",
    "    \n",
    "    try:\n",
    "        # Setup context\n",
    "        ctx = setup_test_context(scenario)\n",
    "        \n",
    "        # Run rule evaluation\n",
    "        result = evaluate_workers_comp_rules(ctx)\n",
    "        \n",
    "        # Analyze results\n",
    "        plan = ctx.state.get(\"active_plan\", [])\n",
    "        defeated_steps = [s for s in plan if s.get(\"status\") == \"DEFEATED\"]\n",
    "        questioned_steps = [s for s in plan if s.get(\"status\") == \"QUESTIONED\"]\n",
    "        total_defeaters = sum(len(s.get(\"defeaters\", [])) for s in plan)\n",
    "        \n",
    "        details = {\n",
    "            \"defeated_count\": len(defeated_steps),\n",
    "            \"questioned_count\": len(questioned_steps),\n",
    "            \"total_defeaters\": total_defeaters,\n",
    "            \"violations_found\": result.get(\"violations_found\", 0),\n",
    "        }\n",
    "        \n",
    "        # Check expectations\n",
    "        failures = []\n",
    "        \n",
    "        if \"has_defeated_steps\" in expected:\n",
    "            has_defeated = len(defeated_steps) > 0\n",
    "            if has_defeated != expected[\"has_defeated_steps\"]:\n",
    "                failures.append(\n",
    "                    f\"has_defeated_steps: expected {expected['has_defeated_steps']}, got {has_defeated}\"\n",
    "                )\n",
    "        \n",
    "        if \"has_questioned_steps\" in expected:\n",
    "            has_questioned = len(questioned_steps) > 0\n",
    "            if has_questioned != expected[\"has_questioned_steps\"]:\n",
    "                failures.append(\n",
    "                    f\"has_questioned_steps: expected {expected['has_questioned_steps']}, got {has_questioned}\"\n",
    "                )\n",
    "        \n",
    "        if \"min_defeaters\" in expected:\n",
    "            if total_defeaters < expected[\"min_defeaters\"]:\n",
    "                failures.append(\n",
    "                    f\"min_defeaters: expected >={expected['min_defeaters']}, got {total_defeaters}\"\n",
    "                )\n",
    "        \n",
    "        if \"step_status_contains\" in expected:\n",
    "            for step_id, expected_status in expected[\"step_status_contains\"].items():\n",
    "                step = next((s for s in plan if s.get(\"id\") == step_id), None)\n",
    "                if step:\n",
    "                    actual_status = step.get(\"status\", \"UNKNOWN\")\n",
    "                    if actual_status != expected_status:\n",
    "                        failures.append(\n",
    "                            f\"{step_id}: expected {expected_status}, got {actual_status}\"\n",
    "                        )\n",
    "                else:\n",
    "                    failures.append(f\"{step_id}: step not found in plan\")\n",
    "        \n",
    "        if failures:\n",
    "            return False, \"; \".join(failures), details\n",
    "        else:\n",
    "            return True, \"All expectations met\", details\n",
    "            \n",
    "    except Exception as e:\n",
    "        return False, f\"Exception: {type(e).__name__}: {e}\", {}\n",
    "\n",
    "\n",
    "def test_regression() -> Dict[str, Any]:\n",
    "    \"\"\"\n",
    "    Run all regression test scenarios and report results.\n",
    "    \n",
    "    Returns:\n",
    "        Summary dict with pass/fail counts and details\n",
    "    \"\"\"\n",
    "    print(\"\\n\" + \"=\" * 70)\n",
    "    print(\"REGRESSION TEST HARNESS\")\n",
    "    print(\"=\" * 70)\n",
    "    print(f\"Running {len(TEST_SCENARIOS)} test scenarios...\\n\")\n",
    "    \n",
    "    results = []\n",
    "    passed = 0\n",
    "    failed = 0\n",
    "    \n",
    "    for i, scenario in enumerate(TEST_SCENARIOS, 1):\n",
    "        name = scenario[\"name\"]\n",
    "        print(f\"[{i}/{len(TEST_SCENARIOS)}] {name}...\")\n",
    "        \n",
    "        success, message, details = run_test_scenario(scenario)\n",
    "        \n",
    "        if success:\n",
    "            print(f\"  ✓ PASS: {message}\")\n",
    "            passed += 1\n",
    "        else:\n",
    "            print(f\"  ✗ FAIL: {message}\")\n",
    "            failed += 1\n",
    "        \n",
    "        results.append({\n",
    "            \"name\": name,\n",
    "            \"passed\": success,\n",
    "            \"message\": message,\n",
    "            \"details\": details,\n",
    "        })\n",
    "    \n",
    "    # Summary\n",
    "    print(\"\\n\" + \"-\" * 70)\n",
    "    print(\"SUMMARY\")\n",
    "    print(\"-\" * 70)\n",
    "    print(f\"  Passed: {passed}/{len(TEST_SCENARIOS)}\")\n",
    "    print(f\"  Failed: {failed}/{len(TEST_SCENARIOS)}\")\n",
    "    \n",
    "    if failed == 0:\n",
    "        print(\"\\n  ✓ All tests passed!\")\n",
    "    else:\n",
    "        print(\"\\n  ✗ Some tests failed. Review output above.\")\n",
    "    \n",
    "    print(\"=\" * 70)\n",
    "    \n",
    "    return {\n",
    "        \"total\": len(TEST_SCENARIOS),\n",
    "        \"passed\": passed,\n",
    "        \"failed\": failed,\n",
    "        \"results\": results,\n",
    "    }\n",
    "\n",
    "\n",
    "print(\"Regression test harness loaded.\")\n",
    "print(\"Run test_regression() to execute all scenarios.\")"
   ]

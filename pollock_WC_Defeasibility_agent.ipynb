    "        print(\"\\n   Suggested Evidence to Obtain:\")\n",
    "        for e in recommendation.get(\"suggested_evidence\", []):\n",
    "            print(f\"     → {e}\")\n",
    "    \n",
    "    # Verification checklist (if applicable)\n",
    "    if recommendation.get(\"verification_checklist\"):\n",
    "        print(\"\\n   Verification Checklist:\")\n",
    "        for v in recommendation.get(\"verification_checklist\", []):\n",
    "            print(f\"     □ {v}\")\n",
    "    \n",
    "    # Next steps\n",
    "    print(\"\\n   Next Steps:\")\n",
    "    for i, ns in enumerate(recommendation.get(\"next_steps\", []), 1):\n",
    "        print(f\"     {i}. {ns}\")\n",
    "    \n",
    "    print(\"\\n\" + \"=\" * 70)\n",
    "\n",
    "\n",
    "print(\"Recommendation helpers loaded:\")\n",
    "print(\"  - generate_final_recommendation(result) -> dict\")\n",
    "print(\"  - print_final_recommendation(recommendation)\")"
   ]

    "# ---------- Regulatory Lookup Function ----------\n",
    "# Retrieves jurisdiction-specific workers' compensation parameters\n",
    "\n",
    "def lookup_workers_comp_regulations(jurisdiction: str) -> Dict[str, Any]:\n",
    "    \"\"\"Retrieve workers' compensation regulatory parameters for a jurisdiction.\n",
    "    \n",
    "    This function simulates looking up jurisdiction-specific rules. In production,\n",
    "    this could connect to a regulatory database, API, or document store.\n",
    "    \n",
    "    Args:\n",
    "        jurisdiction: State abbreviation (e.g., \"IL\", \"CA\", \"TX\")\n",
    "    \n",
    "    Returns:\n",
    "        Dictionary of regulatory parameters and their values.\n",
    "    \"\"\"\n",
    "    # Simulated regulatory database\n",
    "    # In production, this would query an external source\n",
    "    JURISDICTION_RULES = {\n",
    "        \"IL\": {\n",
    "            \"state_name\": \"Illinois\",\n",
    "            \"waiting_period_days\": 3,\n",
    "            \"notice_deadline_days\": 45,\n",
    "            \"statute_of_limitations_years\": 3,\n",
    "            \"requires_mandatory_review_severity\": [\"severe\", \"catastrophic\"],\n",
    "            \"compensability_standard\": \"arising_out_of_and_in_course_of_employment\",\n",
    "            \"ttd_rate_percent\": 66.67,\n",
    "            \"max_ttd_weeks\": 500,\n",
    "            \"mandatory_forms\": [\"IC45\", \"IC54\"],\n",
    "            \"fraud_threshold_for_siu\": 0.70,\n",
    "        },\n",
    "        \"CA\": {\n",
    "            \"state_name\": \"California\",\n",
    "            \"waiting_period_days\": 3,\n",
    "            \"notice_deadline_days\": 30,\n",
    "            \"statute_of_limitations_years\": 1,\n",
    "            \"requires_mandatory_review_severity\": [\"moderate\", \"severe\", \"catastrophic\"],\n",
    "            \"compensability_standard\": \"arising_out_of_employment\",\n",
    "            \"ttd_rate_percent\": 66.67,\n",
    "            \"max_ttd_weeks\": 104,\n",
    "            \"mandatory_forms\": [\"DWC-1\", \"5020\"],\n",
    "            \"fraud_threshold_for_siu\": 0.65,\n",
    "        },\n",
    "        \"TX\": {\n",
    "            \"state_name\": \"Texas\",\n",
    "            \"waiting_period_days\": 7,\n",
    "            \"notice_deadline_days\": 30,\n",
    "            \"statute_of_limitations_years\": 1,\n",
    "            \"requires_mandatory_review_severity\": [\"severe\", \"catastrophic\"],\n",
    "            \"compensability_standard\": \"in_course_and_scope_of_employment\",\n",
    "            \"ttd_rate_percent\": 70.0,\n",
    "            \"max_ttd_weeks\": 104,\n",
    "            \"mandatory_forms\": [\"DWC-041\", \"DWC-001\"],\n",
    "            \"fraud_threshold_for_siu\": 0.70,\n",
    "        },\n",
    "        \"NY\": {\n",
    "            \"state_name\": \"New York\",\n",
    "            \"waiting_period_days\": 7,\n",
    "            \"notice_deadline_days\": 30,\n",
    "            \"statute_of_limitations_years\": 2,\n",
    "            \"requires_mandatory_review_severity\": [\"severe\", \"catastrophic\"],\n",
    "            \"compensability_standard\": \"arising_out_of_and_in_course_of_employment\",\n",
    "            \"ttd_rate_percent\": 66.67,\n",
    "            \"max_ttd_weeks\": 525,\n",
    "            \"mandatory_forms\": [\"C-2\", \"C-3\"],\n",
    "            \"fraud_threshold_for_siu\": 0.65,\n",
    "        },\n",
    "    }\n",
    "    \n",
    "    # Default values for unknown jurisdictions\n",
    "    DEFAULT_RULES = {\n",
    "        \"state_name\": f\"Unknown ({jurisdiction})\",\n",
    "        \"waiting_period_days\": 7,\n",
    "        \"notice_deadline_days\": 30,\n",
    "        \"statute_of_limitations_years\": 2,\n",
    "        \"requires_mandatory_review_severity\": [\"severe\", \"catastrophic\"],\n",
    "        \"compensability_standard\": \"arising_out_of_and_in_course_of_employment\",\n",
    "        \"ttd_rate_percent\": 66.67,\n",
    "        \"max_ttd_weeks\": 104,\n",
    "        \"mandatory_forms\": [],\n",
    "        \"fraud_threshold_for_siu\": 0.70,\n",
    "    }\n",
    "    \n",
    "    rules = JURISDICTION_RULES.get(jurisdiction.upper(), DEFAULT_RULES)\n",
    "    \n",
    "    return {\n",
    "        \"jurisdiction\": jurisdiction.upper(),\n",
    "        **rules,\n",
    "        \"lookup_source\": \"simulated_regulatory_database\",\n",
    "        \"lookup_timestamp\": datetime.now().isoformat(),\n",
    "    }\n",
    "\n",
    "\n",
    "def convert_regulations_to_beliefs(regulations: Dict[str, Any]) -> List[Dict[str, Any]]:\n",
    "    \"\"\"Convert regulatory lookup results to belief format.\n",
    "    \n",
    "    Args:\n",
    "        regulations: Dictionary from lookup_workers_comp_regulations\n",
    "    \n",
    "    Returns:\n",
    "        List of belief dictionaries ready to add to state.\n",
    "    \"\"\"\n",
    "    beliefs = []\n",
    "    jurisdiction = regulations.get(\"jurisdiction\", \"UNKNOWN\")\n",
    "    \n",
    "    # Convert key regulatory parameters to beliefs\n",
    "    belief_mappings = [\n",
    "        (\"waiting_period_days\", f\"jurisdiction:waiting_period_days={regulations.get('waiting_period_days')}\"),\n",
    "        (\"notice_deadline_days\", f\"jurisdiction:notice_deadline_days={regulations.get('notice_deadline_days')}\"),\n",
    "        (\"fraud_threshold_for_siu\", f\"jurisdiction:fraud_threshold={regulations.get('fraud_threshold_for_siu')}\"),\n",
    "        (\"compensability_standard\", f\"jurisdiction:compensability_standard={regulations.get('compensability_standard')}\"),\n",
    "        (\"ttd_rate_percent\", f\"jurisdiction:ttd_rate_percent={regulations.get('ttd_rate_percent')}\"),\n",
    "    ]\n",
    "    \n",
    "    for key, proposition in belief_mappings:\n",
    "        if key in regulations:\n",
    "            beliefs.append({\n",
    "                \"id\": f\"reg-{len(beliefs)+1}\",\n",
    "                \"proposition\": proposition,\n",
    "                \"support\": f\"Regulatory lookup for {jurisdiction}\",\n",
    "                \"strength\": 1.0,  # Regulatory facts are high confidence\n",
    "            })\n",
    "    \n",
    "    # Add mandatory review requirement\n",
    "    review_severities = regulations.get(\"requires_mandatory_review_severity\", [])\n",
    "    if review_severities:\n",
    "        beliefs.append({\n",
    "            \"id\": f\"reg-{len(beliefs)+1}\",\n",
    "            \"proposition\": f\"jurisdiction:requires_mandatory_review_for={','.join(review_severities)}\",\n",
    "            \"support\": f\"Regulatory lookup for {jurisdiction}\",\n",
    "            \"strength\": 1.0,\n",
    "        })\n",
    "    \n",
    "    # Add mandatory forms\n",
    "    forms = regulations.get(\"mandatory_forms\", [])\n",
    "    if forms:\n",
    "        beliefs.append({\n",
    "            \"id\": f\"reg-{len(beliefs)+1}\",\n",
    "            \"proposition\": f\"jurisdiction:mandatory_forms={','.join(forms)}\",\n",
    "            \"support\": f\"Regulatory lookup for {jurisdiction}\",\n",
    "            \"strength\": 1.0,\n",
    "        })\n",
    "    \n",
    "    return beliefs\n",
    "\n",
    "\n",
    "print(\"Regulatory lookup functions defined.\")\n",
    "print(\"Supported jurisdictions: IL, CA, TX, NY (with defaults for others)\")"
   ]

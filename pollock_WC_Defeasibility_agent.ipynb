    "# ==========================================================================\n",
    "# Workers' Compensation Rule Library\n",
    "# ==========================================================================\n",
    "# Modular rule functions for workers' compensation claim evaluation.\n",
    "# Each rule is a separate function that can be tested independently.\n",
    "# ==========================================================================\n",
    "\n",
    "class RuleContext:\n",
    "    \"\"\"Context object passed to each rule function for belief lookup and defeater creation.\n",
    "    \n",
    "    This provides a clean interface for rules to:\n",
    "    - Look up belief values by prefix\n",
    "    - Add defeaters to plan steps\n",
    "    - Track violations\n",
    "    \"\"\"\n",
    "    \n",
    "    def __init__(self, beliefs: list, plan: list, claim_context: dict):\n",
    "        self.plan = plan\n",
    "        self.claim_context = claim_context\n",
    "        self.violations: List[Dict[str, Any]] = []\n",
    "        \n",
    "        # Build belief index for efficient lookup\n",
    "        self._belief_index: Dict[str, Dict[str, Any]] = {}\n",
    "        for b in beliefs:\n",
    "            prop = b.get(\"proposition\", \"\") if isinstance(b, dict) else str(b)\n",
    "            self._belief_index[prop] = b\n",
    "            if \"=\" in prop:\n",
    "                key, value = prop.split(\"=\", 1)\n",
    "                self._belief_index[key] = {\"proposition\": prop, \"value\": value}\n",
    "    \n",
    "    def find_belief_value(self, prefix: str) -> Optional[str]:\n",
    "        \"\"\"Find the value portion of a belief matching the given prefix.\"\"\"\n",
    "        for prop in self._belief_index:\n",
    "            if prop.startswith(prefix) and \"=\" in prop:\n",
    "                return prop.split(\"=\", 1)[1]\n",
    "        return None\n",
    "    \n",
    "    def add_defeater(\n",
    "        self,\n",
    "        step_id: str,\n",
    "        message: str,\n",
    "        rule_name: str,\n",
    "        kind: str = \"regulatory\",\n",
    "        severity: float = 1.0\n",
    "    ) -> bool:\n",
    "        \"\"\"Add a structured defeater to a plan step.\"\"\"\n",
    "        for step in self.plan:\n",
    "            if step.get(\"id\") == step_id:\n",
    "                added = add_defeater_to_step(\n",
    "                    step=step,\n",
    "                    kind=kind,\n",
    "                    source_rule=rule_name,\n",
    "                    message=message,\n",
    "                    severity=severity,\n",
    "                )\n",
    "                if added:\n",
    "                    self.violations.append({\n",
    "                        \"step_id\": step_id,\n",
    "                        \"rule\": rule_name,\n",
    "                        \"kind\": kind,\n",
    "                        \"severity\": severity,\n",
    "                        \"defeater\": message,\n",
    "                    })\n",
    "                return added\n",
    "        return False\n",
    "\n",
    "\n",
    "# ---------- Individual Rule Functions ----------\n",
    "\n",
    "def apply_employment_eligibility_rule(ctx: RuleContext) -> None:\n",
    "    \"\"\"Check if employment status is eligible for workers' comp coverage.\n",
    "    \n",
    "    Type: Rebutting (directly contradicts coverage)\n",
    "    Severity: 0.95 (high - coverage exclusion is nearly conclusive)\n",
    "    \"\"\"\n",
    "    employment_status = ctx.find_belief_value(\"employment:status\")\n",
    "    ineligible_statuses = [\"contractor\", \"independent_contractor\", \"volunteer\", \"unpaid_intern\"]\n",
    "    \n",
    "    if employment_status and employment_status.lower() in ineligible_statuses:\n",
    "        message = (\n",
    "            f\"Employment status '{employment_status}' is not eligible for workers' compensation \"\n",
    "            f\"coverage under standard policy provisions. Independent contractors and similar \"\n",
    "            f\"classifications are typically excluded from coverage.\"\n",
    "        )\n",
    "        ctx.add_defeater(\"wc-step-2\", message, \"employment_eligibility_rule\", \n",
    "                         kind=\"rebutting\", severity=0.95)\n",
    "        ctx.add_defeater(\"wc-step-4\", message, \"employment_eligibility_rule\", \n",
    "                         kind=\"rebutting\", severity=0.95)\n",
    "        ctx.add_defeater(\"wc-step-8\", message, \"employment_eligibility_rule\", \n",
    "                         kind=\"rebutting\", severity=0.95)\n",
    "\n",
    "\n",
    "def apply_course_of_employment_rule(ctx: RuleContext) -> None:\n",
    "    \"\"\"Check if injury arose out of and in the course of employment.\n",
    "    \n",
    "    Type: Undercutting (attacks the inference from injury to compensability)\n",
    "    Severity: 0.6-0.9 depending on evidence\n",
    "    \"\"\"\n",
    "    on_duty = ctx.find_belief_value(\"employment:on_duty_at_time\")\n",
    "    work_location = ctx.find_belief_value(\"claim:location\")\n",
    "    \n",
    "    # Not on duty is a strong defeater\n",
    "    if on_duty and on_duty.lower() == \"false\":\n",
    "        message = (\n",
    "            \"The injury does not appear to have occurred while the employee was on duty. \"\n",
    "            \"Under most jurisdictional standards, an injury must arise out of and in the \"\n",
    "            \"course of employment to be compensable. Recommend escalation for further investigation.\"\n",
    "        )\n",
    "        ctx.add_defeater(\"wc-step-4\", message, \"course_of_employment_rule\", \n",
    "                         kind=\"undercutting\", severity=0.9)\n",
    "        ctx.add_defeater(\"wc-step-7\", message, \"course_of_employment_rule\", \n",
    "                         kind=\"undercutting\", severity=0.85)\n",
    "        ctx.add_defeater(\"wc-step-8\", message, \"course_of_employment_rule\", \n",
    "                         kind=\"undercutting\", severity=0.9)\n",
    "    \n",
    "    # Off-premises location raises questions but is not conclusive\n",
    "    if work_location:\n",
    "        off_premises_indicators = [\"home\", \"personal\", \"commuting\", \"parking_lot\", \"lunch\"]\n",
    "        if any(ind in work_location.lower() for ind in off_premises_indicators):\n",
    "            message = (\n",
    "                f\"Injury location '{work_location}' raises questions about whether the injury \"\n",
    "                f\"arose in the course of employment. Off-premises injuries require additional \"\n",
    "                f\"scrutiny to establish work-relatedness.\"\n",
    "            )\n",
    "            ctx.add_defeater(\"wc-step-4\", message, \"course_of_employment_rule\", \n",
    "                             kind=\"evidential\", severity=0.6)\n",
    "\n",
    "\n",
    "def apply_notice_deadline_rule(ctx: RuleContext) -> None:\n",
    "    \"\"\"Check if claim was reported within the jurisdictional notice deadline.\n",
    "    \n",
    "    Type: Regulatory (based on statutory deadlines)\n",
    "    Severity: 0.4-0.85 depending on how late\n",
    "    \"\"\"\n",
    "    injury_date_str = ctx.find_belief_value(\"claim:date_of_injury\")\n",
    "    notice_date_str = ctx.find_belief_value(\"claim:first_notice_date\")\n",
    "    notice_deadline_str = ctx.find_belief_value(\"jurisdiction:notice_deadline_days\")\n",
    "    \n",
    "    if not (injury_date_str and notice_date_str):\n",
    "        return\n",
    "    \n",
    "    try:\n",
    "        injury_date = datetime.fromisoformat(injury_date_str.replace(\"/\", \"-\")).date()\n",
    "        notice_date = datetime.fromisoformat(notice_date_str.replace(\"/\", \"-\")).date()\n",
    "        days_delayed = (notice_date - injury_date).days\n",
    "        \n",
    "        notice_deadline = 30  # Default\n",
    "        if notice_deadline_str:\n",
    "            try:\n",
    "                notice_deadline = int(notice_deadline_str)\n",
    "            except (ValueError, TypeError):\n",
    "                pass\n",
    "        \n",
    "        if days_delayed > notice_deadline:\n",
    "            message = (\n",
    "                f\"Claim was reported {days_delayed} days after the injury date, exceeding \"\n",
    "                f\"the jurisdictional notice deadline of {notice_deadline} days. Late reporting \"\n",
    "                f\"may provide grounds for claim denial or benefit reduction under statutory provisions.\"\n",
    "            )\n",
    "            ctx.add_defeater(\"wc-step-5\", message, \"late_reporting_rule\", \n",
    "                             kind=\"regulatory\", severity=0.85)\n",
    "            ctx.add_defeater(\"wc-step-8\", message, \"late_reporting_rule\", \n",
    "                             kind=\"regulatory\", severity=0.8)\n",
    "        elif days_delayed > (notice_deadline * 0.75):\n",
    "            message = (\n",
    "                f\"Claim was reported {days_delayed} days after injury, approaching the \"\n",
    "                f\"{notice_deadline}-day statutory deadline. Document reasons for delay.\"\n",
    "            )\n",
    "            ctx.add_defeater(\"wc-step-5\", message, \"late_reporting_rule\", \n",
    "                             kind=\"regulatory\", severity=0.4)\n",
    "    except (ValueError, TypeError):\n",
    "        pass  # Date parsing failed - skip this rule\n",
    "\n",
    "\n",
    "def apply_fraud_indicator_rule(ctx: RuleContext) -> None:\n",
    "    \"\"\"Check if fraud risk score exceeds the jurisdictional threshold.\n",
    "    \n",
    "    Type: Evidential (based on risk indicators)\n",
    "    Severity: 0.5-0.9 depending on score\n",
    "    \"\"\"\n",
    "    fraud_score_str = ctx.find_belief_value(\"risk:fraud_score\")\n",
    "    fraud_threshold_str = ctx.find_belief_value(\"jurisdiction:fraud_threshold\")\n",
    "    \n",
    "    fraud_threshold = 0.70  # Default\n",
    "    if fraud_threshold_str:\n",
    "        try:\n",
    "            fraud_threshold = float(fraud_threshold_str)\n",
    "        except (ValueError, TypeError):\n",
    "            pass\n",
    "    \n",
    "    if not fraud_score_str:\n",
    "        return\n",
    "    \n",
    "    try:\n",
    "        fraud_score = float(fraud_score_str)\n",
    "        \n",
    "        if fraud_score >= fraud_threshold:\n",
    "            message = (\n",
    "                f\"Fraud risk score of {fraud_score:.2f} exceeds the jurisdictional threshold \"\n",
    "                f\"of {fraud_threshold:.2f}. Automatic approval is not permitted. Claim must be \"\n",
    "                f\"referred to Special Investigations Unit (SIU) for review before proceeding.\"\n",
    "            )\n",
    "            ctx.add_defeater(\"wc-step-7\", message, \"fraud_red_flag_rule\", \n",
    "                             kind=\"evidential\", severity=0.9)\n",
    "            ctx.add_defeater(\"wc-step-8\", message, \"fraud_red_flag_rule\", \n",
    "                             kind=\"evidential\", severity=0.85)\n",
    "        elif fraud_score >= (fraud_threshold * 0.85):\n",
    "            message = (\n",
    "                f\"Fraud risk score of {fraud_score:.2f} is elevated. Enhanced documentation \"\n",
    "                f\"and supervisor review recommended before benefit approval.\"\n",
    "            )\n",
    "            ctx.add_defeater(\"wc-step-6\", message, \"fraud_red_flag_rule\", \n",
    "                             kind=\"evidential\", severity=0.5)\n",
    "    except (ValueError, TypeError):\n",
    "        pass\n",
    "\n",
    "\n",
    "def apply_mandatory_review_rule(ctx: RuleContext) -> None:\n",
    "    \"\"\"Check if claim severity requires mandatory independent review.\n",
    "    \n",
    "    Type: Regulatory\n",
    "    Severity: 0.7 (moderate - requires review but not blocking)\n",
    "    \"\"\"\n",
    "    severity = ctx.claim_context.get(\"severity_level\") or ctx.find_belief_value(\"claim:severity_level\")\n",
    "    mandatory_review_str = ctx.find_belief_value(\"jurisdiction:requires_mandatory_review_for\")\n",
    "    \n",
    "    if not (severity and mandatory_review_str):\n",
    "        return\n",
    "    \n",
    "    mandatory_severities = [s.strip().lower() for s in mandatory_review_str.split(\",\")]\n",
    "    if severity.lower() in mandatory_severities:\n",
    "        message = (\n",
    "            f\"Claim severity level '{severity}' requires mandatory independent review \"\n",
    "            f\"under jurisdictional regulations before final resolution can be reached.\"\n",
    "        )\n",
    "        ctx.add_defeater(\"wc-step-8\", message, \"mandatory_review_rule\", \n",
    "                         kind=\"regulatory\", severity=0.7)\n",
    "\n",
    "\n",
    "# ---------- Rule Registry ----------\n",
    "# All rules are registered here for easy iteration and extension\n",
    "\n",
    "WC_RULES = [\n",
    "    apply_employment_eligibility_rule,\n",
    "    apply_course_of_employment_rule,\n",
    "    apply_notice_deadline_rule,\n",
    "    apply_fraud_indicator_rule,\n",
    "    apply_mandatory_review_rule,\n",
    "]\n",
    "\n",
    "\n",
    "# ==========================================================================\n",
    "# Main Rule Evaluation Function\n",
    "# ==========================================================================\n",
    "\n",
    "def evaluate_workers_comp_rules(\n",
    "    tool_context: ToolContext,\n",
    ") -> Dict[str, Any]:\n",
    "    \"\"\"Apply workers' compensation-specific defeasible rules to the active plan.\n",
    "    \n",
    "    This function orchestrates the rule library, applying each rule in sequence\n",
    "    and then updating step statuses based on the resulting defeaters.\n",
    "    \n",
    "    Defeaters are classified by:\n",
    "    - kind: undercutting, rebutting, regulatory, evidential\n",
    "    - severity: 0.0 to 1.0\n",
    "    - source_rule: identifying which rule generated the defeater\n",
    "    \n",
    "    After applying all rules, updates each PlanStep's status using compute_step_status():\n",
    "    - SUPPORTED: No defeaters\n",
    "    - QUESTIONED: Has defeaters with max severity < 0.8\n",
    "    - DEFEATED: Has defeaters with max severity >= 0.8\n",
    "    \n",
    "    Args:\n",
    "        tool_context: ADK tool context for state access.\n",
    "    \n",
    "    Returns:\n",
    "        Summary of rule evaluations, defeaters applied, and status changes.\n",
    "    \"\"\"\n",
    "    state = tool_context.state\n",
    "    plan = state.get(\"active_plan\", [])\n",
    "    beliefs = state.get(\"beliefs\", [])\n",
    "    claim_context = state.get(\"claim_context\", {})\n",
    "    \n",
    "    # Create rule context\n",
    "    ctx = RuleContext(beliefs, plan, claim_context)\n",
    "    \n",
    "    # Apply each rule from the registry\n",
    "    for rule_fn in WC_RULES:\n",
    "        try:\n",
    "            rule_fn(ctx)\n",
    "        except Exception as e:\n",
    "            logger.warning(f\"Rule {rule_fn.__name__} failed: {e}\")\n",
    "    \n",
    "    # ========== OPTIONAL: LLM-Based Evidential Defeaters ==========\n",
    "    # After deterministic rules, optionally query Gemini for additional\n",
    "    # pattern-based defeaters. These are always evidential with clamped\n",
    "    # severity [0.3, 0.7] so they cannot alone cause DEFEATED status.\n",
    "    llm_defeaters_added = 0\n",
    "    llm_defeaters_list = []\n",
    "    \n",
    "    try:\n",
    "        if 'suggest_llm_defeaters' in globals() and 'apply_llm_defeaters' in globals():\n",
    "            llm_defeaters_list = suggest_llm_defeaters(tool_context, max_defeaters=5)\n",
    "            if llm_defeaters_list:\n",
    "                llm_defeaters_added = apply_llm_defeaters(tool_context, llm_defeaters_list)\n",
    "                for d in llm_defeaters_list:\n",
    "                    if d.get(\"target_step_id\"):\n",
    "                        ctx.violations.append({\n",
    "                            \"step_id\": d[\"target_step_id\"],\n",
    "                            \"rule\": \"llm_pattern_analysis\",\n",
    "                            \"kind\": \"evidential\",\n",
    "                            \"severity\": d.get(\"severity\", 0.5),\n",
    "                            \"defeater\": d.get(\"message\", \"\"),\n",
    "                            \"llm_generated\": True,\n",
    "                        })\n",
    "    except Exception as e:\n",
    "        logger.warning(f\"LLM defeater suggestion failed: {e}\")\n",
    "    \n",
    "    # ========== Update Step Statuses using compute_step_status ==========\n",
    "    status_counts = update_all_step_statuses(plan)\n",
    "    \n",
    "    state[\"active_plan\"] = plan\n",
    "    \n",
    "    return {\n",
    "        \"status\": \"evaluated\",\n",
    "        \"domain\": \"workers_compensation\",\n",
    "        \"rules_applied\": [fn.__name__ for fn in WC_RULES] + [\"llm_pattern_analysis\"],\n",
    "        \"violations_found\": len(ctx.violations),\n",
    "        \"violations\": ctx.violations,\n",
    "        \"step_statuses\": status_counts,\n",
    "        \"defeater_kinds_used\": list(set(v[\"kind\"] for v in ctx.violations)),\n",
    "        \"llm_defeaters_added\": llm_defeaters_added,\n",
    "        \"llm_defeaters\": llm_defeaters_list,\n",
    "    }\n",
    "\n",
    "\n",
    "print(\"Enhanced defeasibility engine loaded.\")\n",
    "print(\"Defeater classifications: undercutting, rebutting, regulatory, evidential\")\n",
    "print(\"Severity-based status: SUPPORTED (no defeaters), QUESTIONED (<0.8), DEFEATED (>=0.8)\")"
   ]

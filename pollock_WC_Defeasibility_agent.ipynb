    "# ==========================================================================\n",
    "# Step 4: Merge New Beliefs into State (with Deduplication)\n",
    "# ==========================================================================\n",
    "\n",
    "def merge_new_beliefs_into_state(state: dict, new_beliefs: list[dict]) -> int:\n",
    "    \"\"\"\n",
    "    Add new belief dicts into state['beliefs'] if they are not duplicates.\n",
    "    \n",
    "    New beliefs from the investigation agent are converted to the standard\n",
    "    Belief format used throughout the notebook.\n",
    "    \n",
    "    Args:\n",
    "        state: State dictionary with 'beliefs' list\n",
    "        new_beliefs: List of dicts with 'proposition', 'rationale', 'source_suggestion'\n",
    "        \n",
    "    Returns:\n",
    "        Number of beliefs actually added (after deduplication)\n",
    "    \"\"\"\n",
    "    if not new_beliefs:\n",
    "        return 0\n",
    "\n",
    "    existing = state.get(\"beliefs\", [])\n",
    "    \n",
    "    # Build set of existing propositions for deduplication\n",
    "    existing_props = set()\n",
    "    for b in existing:\n",
    "        if isinstance(b, dict):\n",
    "            prop = b.get(\"proposition\", \"\")\n",
    "        else:\n",
    "            prop = str(b)\n",
    "        existing_props.add(prop.lower().strip())\n",
    "\n",
    "    added = 0\n",
    "    for b in new_beliefs:\n",
    "        prop = b.get(\"proposition\", \"\")\n",
    "        if not prop:\n",
    "            continue\n",
    "            \n",
    "        # Check for duplicates (case-insensitive)\n",
    "        if prop.lower().strip() in existing_props:\n",
    "            continue\n",
    "        \n",
    "        # Convert to standard Belief format\n",
    "        new_belief = Belief(\n",
    "            id=f\"investigation-{uuid.uuid4().hex[:8]}\",\n",
    "            proposition=prop,\n",
    "            support=f\"Investigation suggestion: {b.get('source_suggestion', 'Investigation Agent')}\",\n",
    "            strength=0.7,  # Moderate confidence for investigation-suggested beliefs\n",
    "        )\n",
    "        \n",
    "        existing.append(new_belief.model_dump())\n",
    "        existing_props.add(prop.lower().strip())\n",
    "        added += 1\n",
    "\n",
    "    state[\"beliefs\"] = existing\n",
    "    return added\n",
    "\n",
    "\n",
    "def convert_investigation_beliefs_to_standard(new_beliefs: list[dict]) -> list[dict]:\n",
    "    \"\"\"\n",
    "    Convert investigation agent belief suggestions to standard Belief format.\n",
    "    \n",
    "    Args:\n",
    "        new_beliefs: List of dicts from investigation agent\n",
    "        \n",
    "    Returns:\n",
    "        List of Belief-compatible dicts\n",
    "    \"\"\"\n",
    "    result = []\n",
    "    for b in new_beliefs:\n",
    "        prop = b.get(\"proposition\", \"\")\n",
    "        if not prop:\n",
    "            continue\n",
    "        \n",
    "        belief = Belief(\n",
    "            id=f\"investigation-{uuid.uuid4().hex[:8]}\",\n",
    "            proposition=prop,\n",
    "            support=f\"Investigation: {b.get('source_suggestion', 'Unknown')}\",\n",
    "            strength=0.7,\n",
    "        )\n",
    "        result.append(belief.model_dump())\n",
    "    \n",
    "    return result\n",
    "\n",
    "\n",
    "print(\"Belief merging helpers loaded:\")\n",
    "print(\"  - merge_new_beliefs_into_state(state, new_beliefs) -> int\")\n",
    "print(\"  - convert_investigation_beliefs_to_standard(new_beliefs) -> list[dict]\")"
   ]

    "            \n",
    "            # Ask again after adding\n",
    "            next_action = prompt_continue_investigation()\n",
    "            if next_action == 'stop':\n",
    "                print(\"\\n[User requested to stop investigation]\")\n",
    "                break\n",
    "                \n",
    "        elif next_action == 'stop':\n",
    "            print(\"\\n[User requested to stop investigation]\")\n",
    "            break\n",
    "        \n",
    "        # Continue to next iteration\n",
    "        print(\"\\n[Continuing to next iteration...]\")\n",

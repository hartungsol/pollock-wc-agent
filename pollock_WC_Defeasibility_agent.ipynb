    "# ==========================================================================\n",
    "# Evidence-Based Strength Functions\n",
    "# ==========================================================================\n",
    "# Compute reason strengths from domain-specific evidence signals.\n",
    "# ==========================================================================\n",
    "\n",
    "def compute_fraud_reason_strength(\n",
    "    num_red_flags: int,\n",
    "    inconsistency_count: int = 0,\n",
    "    prior_claims_history: bool = False\n",
    ") -> float:\n",
    "    \"\"\"\n",
    "    Compute strength of a fraud-suspicion reason based on evidence.\n",
    "    \n",
    "    Args:\n",
    "        num_red_flags: Number of fraud indicators detected\n",
    "        inconsistency_count: Number of statement inconsistencies\n",
    "        prior_claims_history: Whether claimant has suspicious claims history\n",
    "    \n",
    "    Returns:\n",
    "        Strength value in [0, 1] where higher = stronger fraud suspicion\n",
    "    \"\"\"\n",
    "    base_strength = 0.3\n",
    "    \n",
    "    # Each red flag adds to strength (diminishing returns)\n",
    "    red_flag_boost = min(num_red_flags * 0.15, 0.45)  # Max 0.45 from flags\n",
    "    \n",
    "    # Inconsistencies add significant weight\n",
    "    inconsistency_boost = min(inconsistency_count * 0.1, 0.2)\n",
    "    \n",
    "    # Prior history is a major factor\n",
    "    history_boost = 0.15 if prior_claims_history else 0.0\n",
    "    \n",
    "    total = base_strength + red_flag_boost + inconsistency_boost + history_boost\n",
    "    return min(total, 1.0)\n",
    "\n",
    "\n",
    "def compute_lateness_reason_strength(days_late: int) -> float:\n",
    "    \"\"\"\n",
    "    Compute strength of a late-filing defeater based on days past deadline.\n",
    "    \n",
    "    Args:\n",
    "        days_late: Number of days past the filing deadline\n",
    "    \n",
    "    Returns:\n",
    "        Strength value in [0, 1] where higher = stronger lateness defeater\n",
    "    \"\"\"\n",
    "    if days_late <= 0:\n",
    "        return 0.0\n",
    "    elif days_late <= 3:\n",
    "        return 0.3  # Minor lateness\n",
    "    elif days_late <= 7:\n",
    "        return 0.5  # Moderate lateness\n",
    "    elif days_late <= 14:\n",
    "        return 0.7  # Significant lateness\n",
    "    elif days_late <= 30:\n",
    "        return 0.85  # Serious lateness\n",
    "    else:\n",
    "        return 0.95  # Severe lateness (likely regulatory bar)\n",
    "\n",
    "\n",
    "def compute_medical_evidence_strength(\n",
    "    has_treating_physician: bool,\n",
    "    has_independent_exam: bool,\n",
    "    has_imaging: bool,\n",
    "    records_consistent: bool\n",
    ") -> float:\n",
    "    \"\"\"\n",
    "    Compute strength of medical evidence supporting a claim.\n",
    "    \n",
    "    Args:\n",
    "        has_treating_physician: Has statement from treating physician\n",
    "        has_independent_exam: Has independent medical examination\n",
    "        has_imaging: Has diagnostic imaging (X-ray, MRI, etc.)\n",
    "        records_consistent: Medical records are internally consistent\n",
    "    \n",
    "    Returns:\n",
    "        Strength value in [0, 1] for medical support\n",
    "    \"\"\"\n",
    "    strength = 0.2  # Base\n",
    "    \n",
    "    if has_treating_physician:\n",
    "        strength += 0.25\n",
    "    if has_independent_exam:\n",
    "        strength += 0.20\n",
    "    if has_imaging:\n",
    "        strength += 0.15\n",
    "    if records_consistent:\n",
    "        strength += 0.20\n",
    "    \n",
    "    return min(strength, 1.0)\n",
    "\n",
    "\n",
    "def compute_employer_dispute_strength(\n",
    "    disputes_injury_occurred: bool,\n",
    "    disputes_work_relatedness: bool,\n",
    "    has_witness_contradiction: bool,\n",
    "    has_surveillance_evidence: bool\n",
    ") -> float:\n",
    "    \"\"\"\n",
    "    Compute strength of employer's dispute of the claim.\n",
    "    \n",
    "    Returns:\n",
    "        Strength value in [0, 1] for employer dispute\n",
    "    \"\"\"\n",
    "    strength = 0.2  # Base (employer always has some standing)\n",
    "    \n",
    "    if disputes_injury_occurred:\n",
    "        strength += 0.25\n",
    "    if disputes_work_relatedness:\n",
    "        strength += 0.20\n",
    "    if has_witness_contradiction:\n",
    "        strength += 0.20\n",
    "    if has_surveillance_evidence:\n",
    "        strength += 0.25\n",
    "    \n",
    "    return min(strength, 1.0)\n",
    "\n",
    "\n",
    "def compute_regulatory_compliance_strength(\n",
    "    jurisdiction: str,\n",
    "    meets_all_requirements: bool,\n",
    "    has_proper_notice: bool,\n",
    "    within_statute_of_limitations: bool\n",
    ") -> float:\n",
    "    \"\"\"\n",
    "    Compute strength of regulatory compliance (for or against claim).\n",
    "    \n",
    "    Regulatory compliance is typically conclusive (not defeasible).\n",
    "    \"\"\"\n",
    "    if not within_statute_of_limitations:\n",
    "        return 0.0  # Absolute bar\n",
    "    \n",
    "    strength = 0.5\n",
    "    if meets_all_requirements:\n",
    "        strength += 0.35\n",
    "    if has_proper_notice:\n",
    "        strength += 0.15\n",
    "    \n",
    "    return min(strength, 1.0)\n",
    "\n",
    "\n",
    "def update_reason_strength_from_evidence(\n",
    "    reason: Reason,\n",
    "    evidence_type: str,\n",
    "    evidence_data: Dict[str, Any]\n",
    ") -> Reason:\n",
    "    \"\"\"\n",
    "    Update a reason's computed strength based on evidence.\n",
    "    \n",
    "    Args:\n",
    "        reason: The Reason object to update\n",
    "        evidence_type: Type of evidence (\"fraud\", \"lateness\", \"medical\", \"employer\", \"regulatory\")\n",
    "        evidence_data: Evidence parameters for the strength function\n",
    "    \n",
    "    Returns:\n",
    "        The updated Reason\n",
    "    \"\"\"\n",
    "    if evidence_type == \"fraud\":\n",
    "        reason.computed_strength = compute_fraud_reason_strength(\n",
    "            num_red_flags=evidence_data.get(\"num_red_flags\", 0),\n",
    "            inconsistency_count=evidence_data.get(\"inconsistency_count\", 0),\n",
    "            prior_claims_history=evidence_data.get(\"prior_claims_history\", False),\n",
    "        )\n",
    "    elif evidence_type == \"lateness\":\n",
    "        reason.computed_strength = compute_lateness_reason_strength(\n",
    "            days_late=evidence_data.get(\"days_late\", 0)\n",
    "        )\n",
    "    elif evidence_type == \"medical\":\n",
    "        reason.computed_strength = compute_medical_evidence_strength(\n",
    "            has_treating_physician=evidence_data.get(\"has_treating_physician\", False),\n",
    "            has_independent_exam=evidence_data.get(\"has_independent_exam\", False),\n",
    "            has_imaging=evidence_data.get(\"has_imaging\", False),\n",
    "            records_consistent=evidence_data.get(\"records_consistent\", True),\n",
    "        )\n",
    "    elif evidence_type == \"employer\":\n",
    "        reason.computed_strength = compute_employer_dispute_strength(\n",
    "            disputes_injury_occurred=evidence_data.get(\"disputes_injury_occurred\", False),\n",
    "            disputes_work_relatedness=evidence_data.get(\"disputes_work_relatedness\", False),\n",
    "            has_witness_contradiction=evidence_data.get(\"has_witness_contradiction\", False),\n",
    "            has_surveillance_evidence=evidence_data.get(\"has_surveillance_evidence\", False),\n",
    "        )\n",
    "    elif evidence_type == \"regulatory\":\n",
    "        reason.computed_strength = compute_regulatory_compliance_strength(\n",
    "            jurisdiction=evidence_data.get(\"jurisdiction\", \"\"),\n",
    "            meets_all_requirements=evidence_data.get(\"meets_all_requirements\", False),\n",
    "            has_proper_notice=evidence_data.get(\"has_proper_notice\", False),\n",
    "            within_statute_of_limitations=evidence_data.get(\"within_statute_of_limitations\", True),\n",
    "        )\n",
    "    \n",
    "    reason.evidence_sources.append(f\"{evidence_type}: {evidence_data}\")\n",
    "    return reason\n",
    "\n",
    "\n",
    "print(\"✓ Evidence-based strength functions defined\")\n",
    "print(\"  - compute_fraud_reason_strength(red_flags, inconsistencies, history)\")\n",
    "print(\"  - compute_lateness_reason_strength(days_late)\")\n",
    "print(\"  - compute_medical_evidence_strength(...)\")\n",
    "print(\"  - compute_employer_dispute_strength(...)\")\n",
    "print(\"  - compute_regulatory_compliance_strength(...)\")\n",
    "print(\"  - update_reason_strength_from_evidence(reason, type, data)\")"
   ]

    "# ==========================================================================\n",
    "# OSCAR-Style Reason Objects\n",
    "# ==========================================================================\n",
    "# First-class Reason and Argument objects for Pollock-style defeasible reasoning\n",
    "# ==========================================================================\n",
    "\n",
    "from dataclasses import dataclass, field\n",
    "from typing import List, Optional, Set, Callable, Dict, Any\n",
    "from enum import Enum\n",
    "\n",
    "class ReasonKind(str, Enum):\n",
    "    \"\"\"Kind of reason - determines defeasibility.\"\"\"\n",
    "    PRIMA_FACIE = \"prima_facie\"    # Defeasible - can be overridden\n",
    "    CONCLUSIVE = \"conclusive\"       # Indefeasible - cannot be overridden\n",
    "\n",
    "class DefeaterType(str, Enum):\n",
    "    \"\"\"Type of defeater attack.\"\"\"\n",
    "    REBUTTING = \"rebutting\"         # Attacks the conclusion directly\n",
    "    UNDERCUTTING = \"undercutting\"   # Attacks the inference connection\n",
    "\n",
    "class ArgumentStatus(str, Enum):\n",
    "    \"\"\"Status after global defeat computation.\"\"\"\n",
    "    UNDEFEATED = \"undefeated\"       # No active defeating attackers\n",
    "    DEFEATED = \"defeated\"           # Has active defeating attackers  \n",
    "    UNDECIDED = \"undecided\"         # In dialectical conflict (mutual attack)\n",
    "\n",
    "@dataclass\n",
    "class Reason:\n",
    "    \"\"\"\n",
    "    A first-class reason in Pollock's sense - a justification for a conclusion.\n",
    "    \n",
    "    Reasons can be:\n",
    "    - Prima facie (defeasible): Can be overridden by stronger counter-reasons\n",
    "    - Conclusive (indefeasible): Cannot be overridden (e.g., statutory requirements)\n",
    "    \n",
    "    Strengths are computed from evidence and can be compared for defeat resolution.\n",
    "    \"\"\"\n",
    "    id: str\n",
    "    name: str\n",
    "    kind: ReasonKind = ReasonKind.PRIMA_FACIE\n",
    "    \n",
    "    # Logical content\n",
    "    premises: List[str] = field(default_factory=list)\n",
    "    conclusions: List[str] = field(default_factory=list)\n",
    "    \n",
    "    # Strength and defeasibility\n",
    "    base_strength: float = 0.5          # Base strength [0, 1]\n",
    "    computed_strength: float = 0.5      # After evidence adjustments\n",
    "    defeasible: bool = True             # Can this reason be defeated?\n",
    "    \n",
    "    # Evidence tracking\n",
    "    evidence_sources: List[str] = field(default_factory=list)\n",
    "    evidence_weight: float = 1.0        # Multiplier for evidence quality\n",
    "    \n",
    "    # Temporal aspects (for Pollock's time-dependent reasoning)\n",
    "    temporal_valid: bool = True         # Is reason currently valid?\n",
    "    discount_factor: float = 1.0        # Time-based strength decay\n",
    "    \n",
    "    # Defeat relationships\n",
    "    undercutters: List[str] = field(default_factory=list)   # Reason IDs that undercut this\n",
    "    defeated_by: List[str] = field(default_factory=list)    # Reason IDs that rebut this\n",
    "    \n",
    "    # Link to plan/step\n",
    "    target_step_id: Optional[str] = None\n",
    "    target_reason_ids: List[str] = field(default_factory=list)\n",
    "    \n",
    "    def final_strength(self) -> float:\n",
    "        \"\"\"Compute final strength with temporal discount.\"\"\"\n",
    "        if not self.temporal_valid:\n",
    "            return 0.0\n",
    "        return self.computed_strength * self.discount_factor * self.evidence_weight\n",
    "    \n",
    "    def is_conclusive(self) -> bool:\n",
    "        \"\"\"Conclusive reasons cannot be defeated.\"\"\"\n",
    "        return self.kind == ReasonKind.CONCLUSIVE\n",
    "    \n",
    "    def __hash__(self):\n",
    "        return hash(self.id)\n",
    "\n",
    "\n",
    "@dataclass \n",
    "class Argument:\n",
    "    \"\"\"\n",
    "    An argument in the OSCAR sense - a structured justification built from reasons.\n",
    "    \n",
    "    Arguments form a graph where:\n",
    "    - Each argument supports a conclusion using one or more reasons\n",
    "    - Arguments can attack other arguments (rebutting or undercutting)\n",
    "    - Global defeat computation determines which arguments survive\n",
    "    \"\"\"\n",
    "    id: str\n",
    "    conclusion: str\n",
    "    reasons: List[Reason] = field(default_factory=list)\n",
    "    \n",
    "    # Attack relationships\n",
    "    attacks: List[str] = field(default_factory=list)        # Argument IDs this attacks\n",
    "    attacked_by: List[str] = field(default_factory=list)    # Argument IDs attacking this\n",
    "    \n",
    "    # Defeat status after global computation\n",
    "    status: ArgumentStatus = ArgumentStatus.UNDECIDED\n",
    "    \n",
    "    # Aggregate strength from constituent reasons\n",
    "    computed_strength: float = 0.5\n",
    "    \n",
    "    # Link to plan structure\n",
    "    supports_step_id: Optional[str] = None\n",
    "    \n",
    "    def compute_aggregate_strength(self) -> float:\n",
    "        \"\"\"Aggregate strength from all reasons (max of final strengths).\"\"\"\n",
    "        if not self.reasons:\n",
    "            return 0.0\n",
    "        self.computed_strength = max(r.final_strength() for r in self.reasons)\n",
    "        return self.computed_strength\n",
    "    \n",
    "    def is_defeatable(self) -> bool:\n",
    "        \"\"\"An argument is defeatable if any of its reasons are prima facie.\"\"\"\n",
    "        return any(r.kind == ReasonKind.PRIMA_FACIE for r in self.reasons)\n",
    "    \n",
    "    def has_conclusive_reason(self) -> bool:\n",
    "        \"\"\"Check if argument contains any conclusive reason.\"\"\"\n",
    "        return any(r.is_conclusive() for r in self.reasons)\n",
    "    \n",
    "    def __hash__(self):\n",
    "        return hash(self.id)\n",
    "\n",
    "\n",
    "# ==========================================================================\n",
    "# Global Reason and Argument Registries\n",
    "# ==========================================================================\n",
    "\n",
    "REASONS: Dict[str, Reason] = {}\n",
    "ARGUMENTS: Dict[str, Argument] = {}\n",
    "\n",
    "\n",
    "def register_reason(reason: Reason) -> Reason:\n",
    "    \"\"\"Register a reason in the global registry.\"\"\"\n",
    "    REASONS[reason.id] = reason\n",
    "    logger.debug(f\"Registered reason: {reason.id} ({reason.kind.value})\")\n",
    "    return reason\n",
    "\n",
    "\n",
    "def register_argument(argument: Argument) -> Argument:\n",
    "    \"\"\"Register an argument in the global registry.\"\"\"\n",
    "    ARGUMENTS[argument.id] = argument\n",
    "    logger.debug(f\"Registered argument: {argument.id} -> {argument.conclusion}\")\n",
    "    return argument\n",
    "\n",
    "\n",
    "def clear_reasoning_state():\n",
    "    \"\"\"Clear all reasons and arguments for fresh analysis.\"\"\"\n",
    "    global REASONS, ARGUMENTS\n",
    "    REASONS.clear()\n",
    "    ARGUMENTS.clear()\n",
    "    logger.info(\"Cleared reasoning state (reasons and arguments)\")\n",
    "\n",
    "\n",
    "def get_reason(reason_id: str) -> Optional[Reason]:\n",
    "    \"\"\"Get a reason by ID.\"\"\"\n",
    "    return REASONS.get(reason_id)\n",
    "\n",
    "\n",
    "def get_argument(arg_id: str) -> Optional[Argument]:\n",
    "    \"\"\"Get an argument by ID.\"\"\"\n",
    "    return ARGUMENTS.get(arg_id)\n",
    "\n",
    "\n",
    "print(\"✓ OSCAR-style Reason and Argument classes defined\")\n",
    "print(f\"  - ReasonKind: {[k.value for k in ReasonKind]}\")\n",
    "print(f\"  - DefeaterType: {[t.value for t in DefeaterType]}\")\n",
    "print(f\"  - ArgumentStatus: {[s.value for s in ArgumentStatus]}\")"

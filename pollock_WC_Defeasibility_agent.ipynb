    "# ==========================================================================\n",
    "# Step 5: Iterative Workers' Comp Analysis Loop\n",
    "# ==========================================================================\n",
    "\n",
    "def seed_state_from_intake(context, intake_data: dict, jurisdiction: str = None) -> dict:\n",
    "    \"\"\"\n",
    "    Seed the context state from intake data.\n",
    "    \n",
    "    Converts intake data to beliefs and initializes the plan.\n",
    "    This reuses existing helper functions.\n",
    "    \n",
    "    Args:\n",
    "        context: MockToolContext or similar with state dict\n",
    "        intake_data: Dictionary with claim intake information\n",
    "        jurisdiction: Optional override for jurisdiction\n",
    "        \n",
    "    Returns:\n",
    "        Plan initialization result\n",
    "    \"\"\"\n",
    "    # Extract jurisdiction\n",
    "    jur = jurisdiction or intake_data.get(\"jurisdiction\", \"CA\")\n",
    "    \n",
    "    # Convert intake data to beliefs\n",
    "    intake_beliefs = [\n",
    "        Belief(id=\"intake-jurisdiction\", proposition=f\"claim:jurisdiction={jur}\",\n",
    "               support=\"Claim intake\", strength=1.0),\n",
    "        Belief(id=\"intake-injury\", \n",
    "               proposition=f\"claim:injury_description={intake_data.get('injury_description', 'Unknown')[:50]}\",\n",
    "               support=\"Claimant statement\", strength=0.9),\n",
    "        Belief(id=\"intake-work-related\", \n",
    "               proposition=f\"claim:work_related={intake_data.get('work_related', 'Unknown')}\",\n",
    "               support=\"Claimant statement\", strength=0.8),\n",
    "        Belief(id=\"intake-date-injury\", \n",
    "               proposition=f\"claim:date_of_injury={intake_data.get('date_of_injury', 'Unknown')}\",\n",
    "               support=\"Claim intake\", strength=1.0),\n",
    "        Belief(id=\"intake-date-reported\", \n",
    "               proposition=f\"claim:first_notice_date={intake_data.get('date_reported', 'Unknown')}\",\n",
    "               support=\"Claim intake\", strength=1.0),\n",
    "        Belief(id=\"intake-work-status\", \n",
    "               proposition=f\"claim:current_work_status={intake_data.get('current_work_status', 'Unknown')}\",\n",
    "               support=\"Claim intake\", strength=0.9),\n",
    "        Belief(id=\"intake-prior-injuries\", \n",
    "               proposition=f\"claim:prior_injuries={intake_data.get('prior_injuries', 'None')}\",\n",
    "               support=\"Claimant statement\", strength=0.8),\n",
    "        Belief(id=\"intake-occupation\", \n",
    "               proposition=f\"employment:job_title={intake_data.get('occupation', 'Unknown')}\",\n",
    "               support=\"Claim intake\", strength=1.0),\n",
    "    ]\n",
    "    \n",
    "    # Add employee age if provided\n",
    "    if intake_data.get(\"employee_age\"):\n",
    "        intake_beliefs.append(\n",
    "            Belief(id=\"intake-age\", \n",
    "                   proposition=f\"claimant:age={intake_data.get('employee_age')}\",\n",
    "                   support=\"Claim intake\", strength=1.0)\n",
    "        )\n",
    "    \n",
    "    # Add employment status (assume full_time if not specified)\n",
    "    intake_beliefs.append(\n",
    "        Belief(id=\"intake-employment-status\", \n",
    "               proposition=\"employment:status=full_time\",\n",
    "               support=\"Claim intake (assumed)\", strength=0.9)\n",
    "    )\n",
    "    \n",
    "    # Add on-duty status (assume true if work-related)\n",
    "    if intake_data.get(\"work_related\", \"\").lower() in [\"yes\", \"true\", \"y\"]:\n",
    "        intake_beliefs.append(\n",
    "            Belief(id=\"intake-on-duty\", \n",
    "                   proposition=\"employment:on_duty_at_time=true\",\n",
    "                   support=\"Work-related injury implies on-duty\", strength=0.85)\n",
    "        )\n",
    "    \n",
    "    # Add beliefs to context\n",
    "    for b in intake_beliefs:\n",
    "        context.state[\"beliefs\"].append(b.model_dump())\n",
    "    \n",
    "    # Get regulatory information for jurisdiction\n",
    "    reg_info = lookup_workers_comp_regulations(jur)\n",
    "    reg_beliefs = convert_regulations_to_beliefs(reg_info)\n",
    "    for rb in reg_beliefs:\n",
    "        context.state[\"beliefs\"].append(rb)\n",
    "    \n",
    "    # Determine severity from work status\n",
    "    work_status = (intake_data.get(\"current_work_status\") or \"\").lower()\n",
    "    if \"off work\" in work_status or \"disability\" in work_status:\n",
    "        severity = \"moderate\"\n",
    "    elif \"light duty\" in work_status:\n",
    "        severity = \"minor\"\n",
    "    else:\n",
    "        severity = \"minor\"\n",
    "    \n",
    "    # Initialize plan\n",
    "    plan_result = init_workers_comp_claim_plan(\n",
    "        claim_description=intake_data.get(\"injury_description\", \"Workers compensation claim\")[:100],\n",
    "        jurisdiction=jur,\n",
    "        severity_level=severity,\n",
    "        tool_context=context,\n",
    "    )\n",
    "    \n",
    "    return plan_result\n",
    "\n",
    "\n",
    "def run_workers_comp_analyst_once(context, verbose: bool = False) -> dict:\n",
    "    \"\"\"\n",
    "    Run a single analysis pass: forward reasoning -> rule evaluation -> backward analysis.\n",
    "    \n",
    "    This is the core analysis function that:\n",
    "    1. Applies forward reasoning to derive new beliefs from existing ones\n",
    "    2. Applies deterministic WC rules and LLM defeater suggestions\n",
    "    3. Runs backward analysis to identify what's needed for key goals\n",
    "    \n",
    "    Args:\n",
    "        context: MockToolContext with state containing beliefs and active_plan\n",
    "        verbose: If True, print progress information\n",
    "        \n",
    "    Returns:\n",
    "        Rule evaluation result dict, augmented with forward/backward info\n",
    "    \"\"\"\n",
    "    state = context.state\n",
    "    \n",
    "    # 1. FORWARD REASONING: Derive new beliefs before rule evaluation\n",
    "    forward_count = run_forward_reasoning_step(state)\n",
    "    if verbose:\n",
    "        print(f\"  [FORWARD] Derived {forward_count} new belief(s)\")\n",
    "    \n",
    "    # 2. RULE EVALUATION: Apply WC rules and LLM defeaters\n",
    "    rule_result = evaluate_workers_comp_rules(context)\n",
    "    \n",
    "    # 3. BACKWARD ANALYSIS: Analyze key goals after rule evaluation\n",
    "    backward_results = run_backward_analysis_for_key_goals(state)\n",
    "    if verbose:\n",
    "        goals_pending = sum(1 for r in backward_results \n",
    "                          if r.get(\"analysis\", {}).get(\"status\") != \"supported\")\n",
    "        print(f\"  [BACKWARD] Analyzed {len(backward_results)} goals, {goals_pending} pending\")\n",
    "    \n",
    "    # Augment result with forward/backward info\n",
    "    rule_result[\"forward_beliefs_added\"] = forward_count\n",
    "    rule_result[\"backward_analysis_count\"] = len(backward_results)\n",
    "    \n",
    "    return rule_result\n",
    "\n",
    "\n",
    "def run_iterative_wc_analysis(\n",
    "    intake_data: dict,\n",
    "    max_iterations: int = 3,\n",
    "    verbose: bool = True,\n",
    ") -> dict:\n",
    "    \"\"\"\n",
    "    Perform workers' comp claim analysis with an iterative investigation loop.\n",
    "\n",
    "    Steps:\n",
    "      1. Seed beliefs and plan from intake (using existing functions).\n",
    "      2. Run forward reasoning -> WC rule evaluation -> backward analysis.\n",
    "      3. If QUESTIONED steps remain:\n",
    "         - Call Investigation Agent to propose new beliefs.\n",
    "         - Merge new beliefs into state.\n",
    "         - Re-run the evaluation.\n",
    "      4. Stop when:\n",
    "         - no QUESTIONED steps remain, or\n",
    "         - no new beliefs are added, or\n",
    "         - max_iterations is reached.\n",
    "\n",
    "    Args:\n",
    "        intake_data: Dictionary with claim intake information\n",
    "        max_iterations: Maximum analysis passes (default 3)\n",
    "        verbose: Print progress information\n",
    "        \n",
    "    Returns:\n",
    "        Final state summary including plan, beliefs, backward_analysis, and iteration history\n",
    "    \"\"\"\n",
    "    # Create fresh context for this analysis\n",
    "    context = MockToolContext()\n",
    "    \n",
    "    # Track iteration history\n",
    "    iteration_history = []\n",
    "    \n",
    "    if verbose:\n",
    "        print(\"\\n\" + \"=\" * 70)\n",
    "        print(\"ITERATIVE WORKERS' COMP ANALYSIS (with Forward/Backward Reasoning)\")\n",
    "        print(\"=\" * 70)\n",
    "        print(f\"Max iterations: {max_iterations}\")\n",
    "        print(f\"Jurisdiction: {intake_data.get('jurisdiction', 'Unknown')}\")\n",
    "        print(f\"Injury: {intake_data.get('injury_description', 'Unknown')[:50]}...\")\n",
    "    \n",
    "    # 1. Initialize state from intake\n",
    "    if verbose:\n",
    "        print(\"\\n[INIT] Seeding state from intake data...\")\n",
    "    \n",

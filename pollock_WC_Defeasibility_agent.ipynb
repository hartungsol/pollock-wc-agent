    "# ==========================================================================\n",
    "# Core Domain Models for Pollock-style Reasoning\n",
    "# ==========================================================================\n",
    "\n",
    "from dataclasses import dataclass, field\n",
    "from typing import List, Optional, Callable, Dict, Any, Tuple\n",
    "\n",
    "# ---------- Defeater Model (Enhanced) ----------\n",
    "\n",
    "@dataclass\n",
    "class Defeater:\n",
    "    \"\"\"Structured representation of a defeater in Pollock-style reasoning.\n",
    "    \n",
    "    Attributes:\n",
    "        id: Unique identifier for this defeater\n",
    "        kind: Classification of the defeater:\n",
    "              - \"undercutting\": attacks the inference, not the conclusion\n",
    "              - \"rebutting\": directly contradicts the conclusion\n",
    "              - \"regulatory\": based on statutory/regulatory requirements\n",
    "              - \"evidential\": based on evidentiary gaps or conflicts\n",
    "        source_rule: Name/ID of the rule or reason that generated this defeater\n",
    "        message: Human-readable explanation\n",
    "        severity: Strength of the defeater (0.0 to 1.0, where 1.0 = strong)\n",
    "        target_step_id: Optional step ID this defeater attacks\n",
    "        target_reason_ids: List of Reason IDs this defeater attacks (OSCAR-style)\n",
    "    \"\"\"\n",
    "    id: str\n",
    "    kind: str  # \"undercutting\", \"rebutting\", \"regulatory\", \"evidential\"\n",
    "    source_rule: str\n",
    "    message: str\n",
    "    severity: float = 1.0\n",
    "    target_step_id: Optional[str] = None\n",
    "    target_reason_ids: List[str] = field(default_factory=list)\n",
    "    \n",
    "    def to_dict(self) -> Dict[str, Any]:\n",
    "        return {\n",
    "            \"id\": self.id,\n",
    "            \"kind\": self.kind,\n",
    "            \"source_rule\": self.source_rule,\n",
    "            \"message\": self.message,\n",
    "            \"severity\": self.severity,\n",
    "            \"target_step_id\": self.target_step_id,\n",
    "            \"target_reason_ids\": self.target_reason_ids,\n",
    "        }\n",
    "\n",
    "\n",
    "# ---------- Belief Model ----------\n",
    "\n",
    "class Belief(BaseModel):\n",
    "    \"\"\"A belief/proposition with support and confidence strength.\"\"\"\n",
    "    id: str\n",
    "    proposition: str\n",
    "    support: str = \"\"\n",
    "    strength: float = Field(ge=0.0, le=1.0, default=0.7)\n",
    "\n",
    "\n",
    "# ---------- Goal Model ----------\n",
    "\n",
    "class Goal(BaseModel):\n",
    "    \"\"\"A goal with priority and optional deadline.\"\"\"\n",
    "    id: str\n",
    "    description: str\n",
    "    priority: int = 5\n",
    "    deadline: datetime | None = None\n",
    "\n",
    "\n",
    "# ---------- PlanStep Model (Enhanced) ----------\n",
    "\n",
    "class PlanStep(BaseModel):\n",
    "    \"\"\"A step in a plan with enhanced defeasibility support.\n",
    "    \n",
    "    The status field tracks the current epistemic state:\n",
    "    - SUPPORTED: No active defeaters, step is justified\n",
    "    - QUESTIONED: Has defeaters but not fully defeated (severity < 0.8)\n",
    "    - DEFEATED: Has strong defeaters (severity >= 0.8)\n",
    "    \"\"\"\n",
    "    id: str\n",
    "    description: str\n",
    "    preconditions: list[str] = Field(default_factory=list)\n",
    "    effects: list[str] = Field(default_factory=list)\n",
    "    risk_factors: list[str] = Field(default_factory=list)\n",
    "    defeaters: list[dict] = Field(default_factory=list)  # List of Defeater.to_dict()\n",
    "    status: str = Field(default=\"SUPPORTED\")  # SUPPORTED, QUESTIONED, DEFEATED\n",
    "    \n",
    "    class Config:\n",
    "        # Allow arbitrary types for flexibility\n",
    "        arbitrary_types_allowed = True\n",
    "\n",
    "\n",
    "# ==========================================================================\n",
    "# Step Status Computation (Central Logic)\n",
    "# ==========================================================================\n",
    "# This is the SINGLE authoritative function for determining step status.\n",
    "# All status updates throughout the notebook MUST use this function.\n",
    "# ==========================================================================\n",
    "\n",
    "# Severity thresholds for status determination\n",
    "SEVERITY_THRESHOLD_DEFEATED = 0.8  # >= this means DEFEATED\n",
    "SEVERITY_THRESHOLD_QUESTIONED = 0.0  # any defeater means at least QUESTIONED\n",
    "\n",
    "\n",
    "def compute_step_status(defeaters: list) -> str:\n",
    "    \"\"\"\n",
    "    Compute the status of a plan step based on its defeaters.\n",
    "    \n",
    "    This is the CENTRAL function for step status computation. All code that\n",
    "    needs to determine step status MUST call this function.\n",
    "    \n",
    "    Semantics:\n",
    "      - SUPPORTED: No active defeaters; step conclusion is justified.\n",
    "      - QUESTIONED: Has defeaters but max severity < 0.8; needs investigation.\n",
    "      - DEFEATED: Has at least one defeater with severity >= 0.8; step is blocked.\n",
    "    \n",
    "    Special handling for regulatory defeaters:\n",
    "      - Regulatory defeaters with high severity (>= 0.8) always cause DEFEATED\n",
    "        status, as they represent statutory requirements that cannot be waived.\n",
    "    \n",
    "    Args:\n",
    "        defeaters: List of defeater dicts, each with at least 'severity' key.\n",
    "                   May also have 'kind' for regulatory defeater detection.\n",
    "    \n",
    "    Returns:\n",
    "        One of: 'SUPPORTED', 'QUESTIONED', 'DEFEATED'\n",
    "    \"\"\"\n",
    "    if not defeaters:\n",
    "        return \"SUPPORTED\"\n",
    "    \n",
    "    # Extract severities and check for regulatory defeaters\n",
    "    max_severity = 0.0\n",
    "    has_high_regulatory = False\n",
    "    \n",
    "    for d in defeaters:\n",
    "        severity = d.get(\"severity\", 1.0) if isinstance(d, dict) else 1.0\n",
    "        max_severity = max(max_severity, severity)\n",
    "        \n",
    "        # Check for regulatory defeaters (conclusive)\n",
    "        kind = d.get(\"kind\", \"\") if isinstance(d, dict) else \"\"\n",
    "        if kind == \"regulatory\" and severity >= SEVERITY_THRESHOLD_DEFEATED:\n",
    "            has_high_regulatory = True\n",
    "    \n",
    "    # Regulatory defeaters with high severity are conclusive\n",
    "    if has_high_regulatory:\n",
    "        return \"DEFEATED\"\n",
    "    \n",
    "    # Standard severity-based determination\n",
    "    if max_severity >= SEVERITY_THRESHOLD_DEFEATED:\n",
    "        return \"DEFEATED\"\n",
    "    \n",
    "    # Has defeaters but below defeat threshold\n",
    "    return \"QUESTIONED\"\n",
    "\n",
    "\n",
    "# ---------- Helper Functions for Defeaters ----------\n",
    "\n",
    "def create_defeater(\n",
    "    source_rule: str,\n",
    "    message: str,\n",
    "    kind: str = \"regulatory\",\n",
    "    severity: float = 1.0,\n",
    "    defeater_count: int = 0,\n",
    ") -> Defeater:\n",
    "    \"\"\"Create a new Defeater instance.\"\"\"\n",
    "    return Defeater(\n",
    "        id=f\"{source_rule}-{defeater_count + 1}\",\n",
    "        kind=kind,\n",
    "        source_rule=source_rule,\n",
    "        message=message,\n",
    "        severity=severity,\n",
    "    )\n",
    "\n",
    "\n",
    "def add_defeater_to_step(\n",
    "    step: dict,\n",
    "    kind: str,\n",
    "    source_rule: str,\n",
    "    message: str,\n",
    "    severity: float = 1.0,\n",
    ") -> bool:\n",
    "    \"\"\"Add a structured defeater to a plan step (dict form).\n",
    "    \n",
    "    Returns True if defeater was added, False if duplicate.\n",
    "    \"\"\"\n",
    "    defeaters = step.get(\"defeaters\", [])\n",
    "    \n",
    "    # Check for duplicates by message + source_rule\n",
    "    for d in defeaters:\n",
    "        if d.get(\"message\") == message and d.get(\"source_rule\") == source_rule:\n",
    "            return False\n",
    "    \n",
    "    defeater = Defeater(\n",
    "        id=f\"{source_rule}-{len(defeaters) + 1}\",\n",
    "        kind=kind,\n",
    "        source_rule=source_rule,\n",
    "        message=message,\n",
    "        severity=severity,\n",
    "    )\n",
    "    defeaters.append(defeater.to_dict())\n",
    "    step[\"defeaters\"] = defeaters\n",
    "    return True\n",
    "\n",
    "\n",
    "def update_step_status(step: dict) -> str:\n",
    "    \"\"\"Update and return the status of a plan step based on its defeaters.\n",
    "    \n",
    "    This function uses compute_step_status() internally and updates the\n",
    "    step dict in place.\n",
    "    \n",
    "    Args:\n",
    "        step: Plan step dict with 'defeaters' list.\n",
    "        \n",
    "    Returns:\n",
    "        The computed status string.\n",
    "    \"\"\"\n",
    "    defeaters = step.get(\"defeaters\", [])\n",
    "    status = compute_step_status(defeaters)\n",
    "    step[\"status\"] = status\n",
    "    return status\n",
    "\n",
    "\n",
    "def update_all_step_statuses(plan: list) -> Dict[str, int]:\n",
    "    \"\"\"Update status for all steps in a plan and return summary counts.\n",
    "    \n",
    "    Uses compute_step_status() for each step.\n",
    "    \n",
    "    Args:\n",
    "        plan: List of plan step dicts.\n",
    "        \n",
    "    Returns:\n",
    "        Dict with counts for each status type.\n",
    "    \"\"\"\n",
    "    counts = {\"SUPPORTED\": 0, \"QUESTIONED\": 0, \"DEFEATED\": 0}\n",
    "    for step in plan:\n",
    "        status = update_step_status(step)\n",
    "        counts[status] = counts.get(status, 0) + 1\n",
    "    return counts\n",
    "\n",
    "\n",
    "print(\"Core models loaded: Belief, Goal, PlanStep, Defeater\")\n",
    "print(\"Defeater kinds: undercutting, rebutting, regulatory, evidential\")\n",
    "print(\"PlanStep statuses: SUPPORTED, QUESTIONED, DEFEATED\")"
   ]
  },
